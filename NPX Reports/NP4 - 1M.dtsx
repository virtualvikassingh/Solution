                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Declare @QuarterEndDate Date
Declare @QuarterStartDate Date
Declare @PreviousQuarterStartDate Date
Declare @PreviousQuarterEndDate Date

Set @QuarterStartDate =  ?--   '4/1/2017' 
Set @QuarterEndDate   =  ?--   '6/30/2017'

Set @PreviousQuarterStartDate	= DATEADD(qq, DATEDIFF(qq, 0, @QuarterStartDate) -1, 0)
Set @PreviousQuarterEndDate		= DATEADD (dd, -1, DATEADD(qq, DATEDIFF(qq, 0, @QuarterStartDate), 0))

;With CTE_UnEarnedPremium as 
(
		Select	PolicyNumber,
			TermNumber, RatingPlan, Year(EffectiveDate) PolicyYear, ExpirationDate, EffectiveDate,
            SUM(Cast(TotalPremium/(	DATEDIFF (dd, EffectiveDate, ExpirationDate) -1) as decimal(10,4))*(DATEDIFF (dd, @QuarterEndDate, ExpirationDate))) UnearnedPremium,
			0 as CollectionExpenses, 
			0 as SpecialAssessments
  
	From  udf_CurrentQuarterEffectivePolicies('MI', @QuarterEndDate) 
	group by PolicyNumber,TermNumber, RatingPlan, ExpirationDate, EffectiveDate
),
 
 
CTE_UnCollectedPremium as
(

    SELECT
		p.PolicyNumber,
		P.TermNumber,
		Sum(case when BT.TransactionTypeCode in ('ChargeWrittenOff') and BT.WriteOffReason = 'Uncollectable' then BT.TransactionAmount else 0 end) As UncollectedPremiums,
		Sum(case when BT.TransactionTypeCode = 'PolicyCmsnPayable' then BT.TransactionAmount else 0 end) As ProducerFeesOnPremiumWritten
    
	FROM FactBillingTransaction F 
		Inner Join DimBillingTransaction BT on F.BillingTransactionID = BT.BillingTransactionID  
		Inner join DimPolicy p on  F.PolicyID = P.PolicyID
		Inner Join DimInvoiceItem IT on F.InvoiceItemID = it.InvoiceItemID
    
	where P.BaseState			= 'MI'
		And P.PolicyStatus		= 'Bound' 
		and F.LegacySourceSystem = 'WC'  	
		And BT.TransactionTypeCode in ('PolicyCmsnPayable','ChargeWrittenOff')    
		And cast(BT.TransactionDate as date) Between @QuarterStartDate and @QuarterEndDate
		Group by P.PolicyNumber,P.TermNumber
) 

	Select	'MI' State,
	'NP4'	TransactionCodeC1,	
	'1M'	RecordIdentifierC2,	 
	 Case CP.RatingPlan
		When 'Rating Plan A' Then '0300'
		When 'Rating Plan B' Then '0400'
		When 'Rating Plan C' Then '0500'
		End ReportCodeC3,
	'15'	PoolCodeC4,		
	'19968' CarrierCodeC5,			
	Left( Replace(convert(varchar, @QuarterEndDate,110),'-',''),4) + Right(Year(@QuarterEndDate),2) QTREndingDateC6,	
	 cp.PolicyYear PolicyYearC7, 
	'210' StateCodeC8,	 
	Cast(Sum(Round(ISNULL(PremiumCededToThePool,0),2))*100 as Int)	PremiumsWrittenC9,
	Cast(Sum(Round(ISNULL(UnEarnedPremium,0),2))*100 as Int) - Cast(Sum(Round(ISNULL(EndingDeferredPremium,0),2))*100 as Int)  UnearnedPremiumReserveC10, 
	Cast(0.228*(Sum(Round(ISNULL(PremiumCededToThePool,0),2))-Sum(Round(ISNULL(UncollectedPremiums,0),2)))*100 as INT) ServicingCarrierAllowanceC11,
	Cast(Sum(Round(ISNULL(UncollectedPremiums,0),2))*100 as Int)	UncollectedPremiumsC12,
	Cast(Sum(Round(ISNULL(CollectionExpenses,0),2))*100 as Int)		CollectionExpensesC13,
	Cast(Sum(Round(ISNULL(ProducerFeesOnPremiumWritten,0),2))*100 as Int) ProducerFeesC14,
	Cast(Sum(Round(ISNULL(SpecialAssessments,0),2))*100 as Int)		SpecialAssessmentsC15,
	Replicate(' ',93) as FillerC16
	
From udf_CededPremium('MI', @QuarterStartDate ,@QuarterEndDate, @PreviousQuarterStartDate, @PreviousQuarterEndDate)  CP  
		Left Join CTE_UnEarnedPremium UE on UE.PolicyNumber = CP.PolicyNumber and UE.TermNumber = CP.TermNumber
		Left join CTE_UnCollectedPremium UC  on cp.PolicyNumber = UC.PolicyNumber and cp.TermNumber = UC.TermNumber 
 
Group By cp.PolicyYear, cp.RatingPlan</property>
