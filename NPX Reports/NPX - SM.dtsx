                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Declare @QuarterEndDate as  date
Declare @QuarterStartDate as date

Set @QuarterStartDate = '1/1/2017'
Set @QuarterEndDate =    '3/31/2019'

Select Distinct BaseState State,
	'NPX' as TransactionCodeC1,
	'SM'  as RecordIdentifierC2,  
	Case When BaseState = 'MI' Then
			Case RatingPlan
				When 'Rating Plan A' Then '0365'
				When 'Rating Plan B' Then '0465'
				When 'Rating Plan C' Then '0565'
		  END
		Else  IIF(TypeofLoss in ('Cumulative Injury (other than disease)','Occupational Disease') , '0265' ,'0165') 
	End	as ReportCodeC3,
	IIF(BaseState = 'MI', '15', '12')   as PoolCodeC4,
	'19968' as CarrierCodeC5,
	Left( Replace(convert(varchar, @QuarterEndDate,110),'-',''),4) + Right(Year(@QuarterEndDate),2) QTREndingDateC6,
	Year(c.LossDate) as AccidentYearC7,
	StateCode2Digits + '0' as StateCodeC8,
	Cast(Sum(Round(ISNULL(SubrogationRecovery,0),2))*100 as INT) SubrogationRecoveredC9,
	Replicate(' ',177) FillerC10
 	 
From FactClaimTransaction f
		inner Join DimClaim c on f.ClaimID = c.ClaimID
		inner Join DimPolicy p on f.PolicyID = p.PolicyID
		inner Join DimDate d on f.TransactionDateID = d.DateID
		inner Join DimExposure e on f.ExposureID = e.ExposureID
		inner join DimState ds on p.BaseState = ds.StateAbbr
Where c.ClaimStatus != 'Draft'
		And p.BaseState in ('MI', 'IL','CT','VA','SC')
		And d.Date Between @QuarterStartDate and @QuarterEndDate
		And e.ExposureName = 'Medical Details'
		And SubrogationRecovery != 0

 Group by 
	 LossDate,
	 BaseState, 
	 StateCode2Digits,
	 Case When BaseState = 'MI' Then
			Case RatingPlan
				When 'Rating Plan A' Then '0365'
				When 'Rating Plan B' Then '0465'
				When 'Rating Plan C' Then '0565'
		  END
		Else  IIF(TypeofLoss in ('Cumulative Injury (other than disease)','Occupational Disease') , '0265' ,'0165') 
	End</property>
