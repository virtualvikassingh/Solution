                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Declare @QuarterEndDate Date
Declare @QuarterStartDate Date
Declare @PreviousQuarterStartDate Date
Declare @PreviousQuarterEndDate Date

Set @QuarterStartDate = ?--'4/1/2017'
Set @QuarterEndDate   = ?--'6/30/2017'

Set @PreviousQuarterStartDate	= DATEADD(qq, DATEDIFF(qq, 0, @QuarterStartDate) -1, 0)
Set @PreviousQuarterEndDate		= DATEADD (dd, -1, DATEADD(qq, DATEDIFF(qq, 0, @QuarterStartDate), 0))

;With CTE_EffectivePolicies as 
(

	Select	PolicyNumber, TermNumber, EffectiveDate, ExpirationDate,
			SUM(Cast(TotalPremium/(	DATEDIFF (dd, EffectiveDate, ExpirationDate) -1) as decimal(10,4))*(DATEDIFF (dd, @QuarterEndDate, ExpirationDate))) UnearnedPremium 
	From  udf_CurrentQuarterEffectivePolicies('IN', @QuarterEndDate) 
	group by PolicyNumber,TermNumber, RatingPlan, ExpirationDate, EffectiveDate
),

CTE_WrittenPremium as 
(
	Select Policynumber, ReportCode,  TermNumber,
		Sum(WrittenPremium) WrittenPremium,
		Max(NewPolicyAllowance) + (Sum(WrittenPremium)*0.153) ServicingCarrierAllowanceOnWrittenPremium,
		Sum(UncollectiblePremium) UncollectiblePremium,
		Sum(ProducerFeesOnPremiumWritten) ProducerFeesOnPremiumWritten,
		0 ProducerFeesOnUncollectiblePremium,
		0 ServicingCarrierAllowanceOnUncollectiblePremium
		From 
		(	 
			SELECT PolicyNumber, TermNumber, 
				Case when IT.ChargeCode = 'OccupationalDisease' then '0200' else '0100' end As ReportCode,		
				Case when cast(BT.TransactionDate as date) Between @QuarterStartDate And @QuarterEndDate and BT.TransactionTypeCode ='ChargeBilled' and BT.LedgersideName = 'CREDIT'   then BT.TransactionAmount else 0 end WrittenPremium,
				Case when cast(P.EffectiveDate as date) Between @QuarterStartDate And  @QuarterEndDate 
				and not exists(select 1 from DimPolicy cp where cp.PolicyNumber = P.PolicyNumber and cp.TermNumber = p.TermNumber and cp.TransactionType='Cancellation' and cp.PolicyStatus='Bound' and cp.PolicyTransactionEffectiveDate &lt;= @QuarterEndDate)
				then  300 Else 0 End as NewPolicyAllowance,
				Case when cast(BT.TransactionDate as date) Between @QuarterStartDate And @QuarterEndDate and BT.TransactionTypeCode ='ChargeWrittenOff'  and BT.WriteOffReason = 'Uncollectable' then BT.TransactionAmount else 0 end  As UncollectiblePremium,
				Case when cast(BT.TransactionDate as date) Between @QuarterStartDate And @QuarterEndDate and BT.TransactionTypeCode = 'PolicyCmsnPayable' then  BT.TransactionAmount Else 0 end As ProducerFeesOnPremiumWritten,
				0 As ProducerFeesOnUncollectiblePremium,
				0 As ServicingCarrierAllowanceOnUncollectiblePremium
	
			FROM	FactBillingTransaction As F, DimBillingTransaction As BT, DimPolicy As P, DimInvoiceItem As IT
			where	F.BillingTransactionID = BT.BillingTransactionID and F.PolicyID = P.PolicyID  and F.InvoiceItemID = it.InvoiceItemID
		 			and cast(P.EffectiveDate as Date) &lt;= @QuarterEndDate 
					and cast(P.JobCloseDate as Date) &lt;= @QuarterEndDate
					and P.BaseState = 'IN'	
					and F.LegacySourceSystem = 'WC'  		
					and IT.InvoiceCategory &lt;&gt; 'Indiana Second Injury Fund Surcharge' 
					And Not Exists (Select 1 from DimPolicy cp Where p.PolicyNumber = cp.PolicyNumber and p.TermNumber = cp.TermNumber and Cast(cp.EffectiveDate as Date) = Cast(cp.CancellationDate as Date) 
										and cp.ModelNumber = (select max(ModelNumber) From DimPolicy As cp1 where  p.PolicyNumber = cp1.PolicyNumber and p.TermNumber = cp1.TermNumber and Convert(varchar, cp1.JobCloseDate, 101) &lt;= @QuarterEndDate))  -- To include Policies got cancelled and reinstated 

	)T
	Group By Policynumber, ReportCode,  TermNumber
		
),
	 

CTE_TransactionType as 
(
	Select 
	 p.PolicyNumber
	,p.TermNumber
	,p.TransactionType
	,p.ModelNumber
	From DimPolicy p 
	Where PolicyStatus	= 'Bound'
		And BaseState	= 'IN'
		And Convert(varchar, p.ModelDate , 101) between @QuarterStartDate and @QuarterEndDate
		and p.ModelNumber = (select max(p2.ModelNumber) from DimPolicy p2 where p2.PolicyNumber = p.PolicyNumber and p2.TermNumber = p.TermNumber and p2.PolicyStatus	= 'Bound' and Convert(varchar, p2.ModelDate, 101) between @QuarterStartDate and @QuarterEndDate)
) 

Select	
	'IN' as State,   
	'IN2' as RecordTypeCodeC1		
	,'D1' as RecordIdentifierC2	
	,ReportCode	ReportCodeC3	
	,'13' as PoolCodeC4			
	,'19968' as CarrierCode5		
	,Left( Replace(convert(varchar, @QuarterEndDate,110),'-',''),4) + Right(Year(@QuarterEndDate),2) QtrEndingDateC6
	,Year(ep.EffectiveDate) PolicyYearC7	
	,'130' as StateCodeC8		
	,dbo.udf_MakeFixedWidthStringIN(ep.PolicyNumber,' ',30) PolicyNumberC9
	,dbo.udf_MakeFixedWidthStringIN(cast(month(ep.EffectiveDate) as varchar(2)),' ',2) PolicyEffectiveMonthC10
	,dbo.udf_MakeFixedWidthStringIN(cast(DATEDIFF(month, ep.EffectiveDate, ep.ExpirationDate) as varchar(5)),'0',3) TermMonthC11
	,dbo.udf_MakeFixedWidthStringIN(cast(year(ep.ExpirationDate)*100+month(ep.ExpirationDate) as varchar(6)),' ',6) ExpirationDateC12
	,dbo.udf_MakeFixedWidthStringIN(cast(round(ISNULL(wp.WrittenPremium,0),2) as decimal(18,2)),'0',13) PremiumsWrittenC13
	,dbo.udf_MakeFixedWidthStringIN(cast(round(ISNULL(wp.ServicingCarrierAllowanceOnWrittenPremium,0),2) as decimal(18,2)),'0',13) ServicingCarrierAllowanceC14
	,dbo.udf_MakeFixedWidthStringIN(cast(round(ISNULL(wp.ProducerFeesOnPremiumWritten,0),2) as decimal(18,2)),'0',10) ProducerFeesC15 
	,dbo.udf_MakeFixedWidthStringIN(cast(round(ISNULL(ep.UnearnedPremium,0),2) - round(ISNULL(cp.EndingDeferredPremium, 0),2) as decimal(18,2)),'0',13) UnearnedPremiumC16
	,dbo.udf_MakeFixedWidthStringIN(case tp.TransactionType 
									when 'Submission' then 'N' 
									when 'Policy Change' then 'N'
									when 'Renewal' then 'R'
									when 'Cancellation' then 'C'
									when 'Audit' then 'A'
									end ,'  ', 1) as PremiumTransactionTypeC17
From  udf_CededPremium('IN',@QuarterStartDate,@QuarterEndDate,@PreviousQuarterStartDate, @PreviousQuarterEndDate) Cp 
Left Join CTE_EffectivePolicies ep on Cp.PolicyNumber = ep.PolicyNumber and Cp.TermNumber = ep.TermNumber
Left Join CTE_WrittenPremium	wp on Cp.PolicyNumber = wp.PolicyNumber and Cp.TermNumber = wp.TermNumber
Left Join CTE_TransactionType	tp on Cp.PolicyNumber = tp.PolicyNumber and Cp.TermNumber = tp.TermNumber
Order by cp.PolicyNumber</property>
