      <DTS:Variables />
      <DTS:ObjectData>
        <pipeline
          version="1">
          <components>
            <component
              refId="Package\Create NP4-1M File\EDW"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="EDW"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Declare @QuarterEndDate Date
Declare @QuarterStartDate Date

Set @QuarterStartDate = ?-- '2017-01-01'
Set @QuarterEndDate   = ?--'2017-02-28' 

select	
    'NP4' TransactionCode 
    ,'1M' RecordIdentifier	
    ,Case B.RatingPlan
    When 'Rating Plan A' Then '0300'
    When 'Rating Plan B' Then '0400'
    When 'Rating Plan C' Then '0500'
    End ReportCode		
	,'15' PoolCode			
	,'19968' CarrierCode			
	,Replace(convert(varchar,  @QuarterEndDate,110),'-','') QtrEndingMMDDYY		
	,PolicyYear	
	,'210' StateCode		
	,Cast(sum(ISNULL(PremiumsWritten,0))*100 as INT) as PremiumsWritten
	,Cast(sum(ISNULL(uep.UnearnedPremium,0))*100 as INT) as UnearnedPremiumReserve
	,Cast(0.228*(sum(ISNULL(PremiumsWritten,0))-sum(ISNULL(UncollectedPremiums,0)))*100 as INT) as ServicingCarrierAllowance
	,Cast(sum(ISNULL(UncollectedPremiums,0))*100 as INT) as UncollectedPremiums
	,Cast(sum(ISNULL(CollectionExpenses,0))*100 as INT) as CollectionExpenses
	,Cast(sum(ISNULL(ProducerFeesonPremiumWritten,0))*100 as INT) as ProducerFees
	,Cast(sum(ISNULL(SpecialAssessments,0))*100 as INT) as SpecialAssessments
	,'' as FILLER
from (
    select	PolicyNumber,
    		TermNumber,  
			RatingPlan,
    		PolicyYear,
    		sum(A.WrittenPremium) PremiumsWritten,
    		0 as CollectionExpenses, 
    		0 as SpecialAssessments
    from (
    	Select P.PolicyNumber,
    	       P.TermNumber,
				P.RatingPlan,
    			Year(P.EffectiveDate) PolicyYear,
    			F.WrittenPremium
    	from FactWCPremiumTransaction F
		,DimPolicy As P
    	where P.PolicyID = F.PolicyID 
    		and P.PolicyStatus = 'Bound'
    		and P.BaseState  = 'MI'  
    		and P.JobCloseDate &gt;= @QuarterStartDate 
    		and P.JobCloseDate &lt;= @QuarterEndDate
    ) As A
    	group by 
    	    PolicyNumber,
    		TermNumber,  
    		RatingPlan,
    		PolicyYear
) As B

left join 
(
 select PolicyNumber
        ,TermNumber
        ,RatingPlan
        ,SUM(Cast(WrittenPremium/TotalDays as decimal(10,4))*RemainingDays) UnearnedPremium
 from (
	Select 
		p.PolicyNumber, 
		p.TermNumber,
		p.RatingPlan,
		COALESCE((SELECT p1.TotalPremium FROM DimPolicy p1 WHERE p1.TransactionType IN ('Policy Change', 'Cancellation') and p1.PolicyStatus = 'Bound' and p1.PolicyNumber = p.PolicyNumber and p1.EffectiveDate = p.EffectiveDate AND Convert(varchar, p1.JobCloseDate, 101) &lt;= @QuarterEndDate), p.TotalPremium) WrittenPremium,
		DATEDIFF (dd, EffectiveDate, ExpirationDate) -1 TotalDays,
		DATEDIFF (dd, @QuarterEndDate, ExpirationDate) RemainingDays
	From DimPolicy p 
	Where TransactionType		= 'Submission'
			And PolicyStatus	= 'Bound'
			And BaseState		= 'MI'
			And Convert(varchar, p.ExpirationDate, 101) &gt; @QuarterEndDate
			And Convert(varchar, p.EffectiveDate, 101) &lt;= @QuarterEndDate
			And Convert(varchar, p.JobCloseDate, 101) &lt;= @QuarterEndDate
			) subqry
	group by PolicyNumber,TermNumber,RatingPlan
) uep on uep.PolicyNumber = B.PolicyNumber and uep.TermNumber = B.TermNumber and uep.RatingPlan = B.RatingPlan

left join 
(
    SELECT
    P.PolicyNumber
    ,P.TermNumber
    ,sum(case when BT.TransactionTypeCode in ('ChargeWrittenOff') and BT.WriteOffReason = 'uncollectable' then BT.TransactionAmount else 0 end) As UncollectedPremiums
    ,sum(case when BT.TransactionTypeCode = 'PolicyCmsnPayable' then BT.TransactionAmount else 0 end) As ProducerFeesonPremiumWritten
    FROM FactBillingTransaction As F, DimBillingTransaction As BT,DimPolicy As P
    ,DimCompany As CO,DimInvoiceItem As IT
    where F.BillingTransactionID = BT.BillingTransactionID and F.PolicyID = P.PolicyID
    and F.CompanyID = Co.CompanyID and F.InvoiceItemID = it.InvoiceItemID
    --and BT.LedgersideName = 'Debit' 
    and BT.TransactionTypeCode in ('PolicyCmsnPayable','ChargeWrittenOff') 
    and P.PolicyStatus = 'Bound'
    and cast(BT.TransactionDate as date) &gt;= @QuarterStartDate and cast(BT.TransactionDate as date) &lt;= @QuarterEndDate 
    and P.BaseState = 'MI'
    Group by P.PolicyNumber,P.TermNumber
) ucp on ucp.PolicyNumber = B.PolicyNumber and ucp.TermNumber = B.TermNumber
	group by 
		B.RatingPlan
		,PolicyYear</property>
