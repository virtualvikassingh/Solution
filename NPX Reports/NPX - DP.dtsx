                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Declare @QuarterEndDate Date
Declare @QuarterStartDate Date
Declare @PreviousQuarterStartDate Date
Declare @PreviousQuarterEndDate Date

Set @QuarterStartDate	=  ?--  '2019-01-01'
Set @QuarterEndDate	=      ?--  '2019-03-31' 

Set @PreviousQuarterStartDate	= DATEADD(qq, DATEDIFF(qq, 0, @QuarterStartDate) -1, 0)
Set @PreviousQuarterEndDate		= DATEADD (dd, -1, DATEADD(qq, DATEDIFF(qq, 0, @QuarterStartDate), 0));

 
;With CTE_CededPremium as 
(
	Select *,'CT' as BaseState from  udf_NCCICededPremium('CT', @QuarterStartDate ,@QuarterEndDate, @PreviousQuarterStartDate, @PreviousQuarterEndDate)
	UNION ALL
	Select *,'IL' as BaseState from  udf_NCCICededPremium('IL', @QuarterStartDate ,@QuarterEndDate, @PreviousQuarterStartDate, @PreviousQuarterEndDate)
	UNION ALL
	Select *,'SC' as BaseState from  udf_NCCICededPremium('SC', @QuarterStartDate ,@QuarterEndDate, @PreviousQuarterStartDate, @PreviousQuarterEndDate)
	UNION ALL
	Select *,'VA' as BaseState from  udf_NCCICededPremium('VA', @QuarterStartDate ,@QuarterEndDate, @PreviousQuarterStartDate, @PreviousQuarterEndDate)
	UNION ALL
	Select *,'MI' as BaseState from  udf_NCCICededPremium('MI', @QuarterStartDate ,@QuarterEndDate, @PreviousQuarterStartDate, @PreviousQuarterEndDate)
)

,CTE_ChargeCode
as 
(
	 
	Select Policynumber,TermNumber, max(ChargeCode) ChargeCode 
		From 
		(	 
			SELECT Distinct PolicyNumber, TermNumber,IIF(IT.ChargeCode = 'OccupationalDisease', '0210' ,'0110') ChargeCode	 
	
			FROM	FactBillingTransaction As F,  DimPolicy As P, DimInvoiceItem As IT
			where	F.PolicyID = P.PolicyID  and F.InvoiceItemID = IT.InvoiceItemID
			and p.BaseState in ('MI','IL','SC','VA','CT')
		 			and cast(P.EffectiveDate as Date) &lt;= @QuarterEndDate and cast(P.JobCloseDate as Date) &lt;= @QuarterEndDate
					And Not Exists (Select 1 from DimPolicy cp Where p.PolicyNumber = cp.PolicyNumber and p.TermNumber = cp.TermNumber and Cast(cp.EffectiveDate as Date) = Cast(cp.CancellationDate as Date) 
										and cp.ModelNumber = (select max(ModelNumber) From DimPolicy As cp1 where  p.PolicyNumber = cp1.PolicyNumber and p.TermNumber = cp1.TermNumber and Convert(varchar, cp1.JobCloseDate, 101) &lt;= @QuarterEndDate))  -- To include Policies got cancelled and reinstated 


		)A		  
		Group by Policynumber,TermNumber 
)

Select Distinct cp.BaseState as State,
	'NPX' As TransactionCodeC1, 
	'DP' As RecordIdentifierC2,
	Case When cp.BaseState = 'MI' Then
			Case cp.RatingPlan
			When 'Rating Plan A' Then '0310'
			When 'Rating Plan B' Then '0410'
			When 'Rating Plan C' Then '0510'
		  END
		Else  ChargeCode 
	End	as ReportCodeC3,
	IIF(cp.BaseState = 'MI', '15', '12')   as PoolCodeC4,
	'19968' As CarrierCodeC5,
	Left( Replace(convert(varchar, @QuarterEndDate,110),'-',''),4) + Right(Year(@QuarterEndDate),2) QTREndingDateC6,
	 PolicyYear As PolicyYearC7,
	StateCode2Digits + '0' as StateCodeC8, 

	Cast(Sum(Round(GrossWrittenPremium,2))*100 as Decimal(18)) As GrossWrittenPremiumC9,
	Cast(Sum(Round(BeginningDeferredPremium,2))*100 as  Decimal(18)) AS  BeginnningDeferredPremiumC10,

	Cast(Sum(Round(EndingDeferredPremium,2))*100 as  Decimal(18)) AS EndingDeferredPremiumC11, 
	Cast(Sum(Round(PremiumCededToThePool,2))*100  as  Decimal(18)) AS PremiumCededToThePoolC12,
	Replicate(' ', 135) FillerC13
 
From CTE_CededPremium cp 
Left Join CTE_ChargeCode cc on cp.PolicyNumber = cc.PolicyNumber and cc.TermNumber = cp.TermNumber 
Left Join DimState ds on cp.BaseState = ds.StateAbbr
Where cp.BaseState in ('MI','IL','SC','VA','CT')
Group By    cp.PolicyYear, StateCode2Digits,cp.BaseState,
	Case When cp.BaseState = 'MI' Then
			Case cp.RatingPlan
			When 'Rating Plan A' Then '0310'
			When 'Rating Plan B' Then '0410'
			When 'Rating Plan C' Then '0510'
		  END
		Else  ChargeCode
	End</property>
