                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Declare @QuarterEndDate Date
Declare @QuarterStartDate Date
Declare @PreviousQuarterStartDate Date
Declare @PreviousQuarterEndDate Date

Set @QuarterStartDate =      '1/1/2019'
Set @QuarterEndDate   =     '3/31/2019'

Set @PreviousQuarterStartDate	= DATEADD(qq, DATEDIFF(qq, 0, @QuarterStartDate) -1, 0)
Set @PreviousQuarterEndDate		= DATEADD (dd, -1, DATEADD(qq, DATEDIFF(qq, 0, @QuarterStartDate), 0))


;With CTE_CededPremium as 
(
	Select *,'CT' as BaseState from  udf_NCCICededPremium('CT', @QuarterStartDate ,@QuarterEndDate, @PreviousQuarterStartDate, @PreviousQuarterEndDate)
	UNION ALL
	Select *,'IL' as BaseState from  udf_NCCICededPremium('IL', @QuarterStartDate ,@QuarterEndDate, @PreviousQuarterStartDate, @PreviousQuarterEndDate)
	UNION ALL
	Select *,'SC' as BaseState from  udf_NCCICededPremium('SC', @QuarterStartDate ,@QuarterEndDate, @PreviousQuarterStartDate, @PreviousQuarterEndDate)
	UNION ALL
	Select *,'VA' as BaseState from  udf_NCCICededPremium('VA', @QuarterStartDate ,@QuarterEndDate, @PreviousQuarterStartDate, @PreviousQuarterEndDate)
),


CTE_ChargeCode
as 
(
	 Select PolicyNUmber, TermNUmber, Max( ChargeCode) ChargeCode
	 From
		(
			SELECT Distinct PolicyNumber, TermNumber,IIF(IT.ChargeCode = 'OccupationalDisease', '0210' ,'0110') ChargeCode
			FROM	FactBillingTransaction As F,  DimPolicy As P, DimInvoiceItem As IT
			where	F.PolicyID = P.PolicyID  and F.InvoiceItemID = IT.InvoiceItemID
			and p.BaseState in ('IL','SC','VA','CT')
		 			and cast(P.EffectiveDate as Date) &lt;= @QuarterEndDate and cast(P.JobCloseDate as Date) &lt;= @QuarterEndDate
					And Not Exists (Select 1 from DimPolicy cp Where p.PolicyNumber = cp.PolicyNumber and p.TermNumber = cp.TermNumber and Cast(cp.EffectiveDate as Date) = Cast(cp.CancellationDate as Date) 
										and cp.ModelNumber = (select max(ModelNumber) From DimPolicy As cp1 where  p.PolicyNumber = cp1.PolicyNumber and p.TermNumber = cp1.TermNumber and Convert(varchar, cp1.JobCloseDate, 101) &lt;= @QuarterEndDate))  -- To include Policies got cancelled and reinstated 
		) T
	Group By PolicyNumber, TermNumber
),

CTE_UnCollectedPremium as
(

    SELECT
		p.PolicyNumber,
		P.TermNumber,
		Sum(case when BT.TransactionTypeCode in ('ChargeWrittenOff') and BT.WriteOffReason = 'Uncollectable' then BT.TransactionAmount else 0 end) As UncollectedPremiums,
		Sum(case when BT.TransactionTypeCode = 'PolicyCmsnPayable' then BT.TransactionAmount else 0 end) As ProducerFeesOnPremiumWritten
    
	FROM FactBillingTransaction F 
		Inner Join DimBillingTransaction BT on F.BillingTransactionID = BT.BillingTransactionID  
		Inner join DimPolicy p on  F.PolicyID = P.PolicyID
		Inner Join DimInvoiceItem IT on F.InvoiceItemID = it.InvoiceItemID
    
	where P.BaseState In ('CT','IL','SC','VA')
		And P.PolicyStatus		= 'Bound'  	
		And BT.TransactionTypeCode in ('PolicyCmsnPayable','ChargeWrittenOff')    
		And cast(BT.TransactionDate as date) Between @QuarterStartDate and @QuarterEndDate
		Group by P.PolicyNumber,P.TermNumber
),

CTE_TotalLoss as 
(
	Select  
		p.PolicyNumber,   
		p.TermNumber, 	 IIF(TypeofLoss in ('Cumulative Injury (other than disease)','Occupational Disease') , '0210' ,'0110') TypeOfLoss,
		Sum(ISNULL(IndemnityPaid,0) + ISNULL(EmployersLiabilityPaid,0) + ISNULL(MedicalPaid,0)) TotalPaidLosses 
 	 
	From FactClaimTransaction f
		Inner Join DimClaim c on f.ClaimID = c.ClaimID
		Inner Join DimPolicy p on f.PolicyID = p.PolicyID
		Inner Join DimDate d on f.TransactionDateID = d.DateID 
	Where c.ClaimStatus != 'Draft'
		And P.BaseState In ('CT','IL','SC','VA')	
		And D.Date &lt;= @QuarterEndDate

	 Group By  PolicyNumber, TermNumber,  IIF(TypeofLoss in ('Cumulative Injury (other than disease)','Occupational Disease') , '0210' ,'0110')
) 



	Select	'00' State,
	'NP1'	TransactionCodeC1,	
	'T1'	RecordIdentifierC2,	
	'0000'	ReportCodeC3,	-----HC as 0000
	'12'	PoolCodeC4,		
	'19968' CarrierCodeC5,			
	Left( Replace(convert(varchar, @QuarterEndDate,110),'-',''),4) + Right(Year(@QuarterEndDate),2) QTREndingDateC6,		
	'0000' PolicyYearC7,	-----HC as 0000
	'000' StateCodeC8,		-----HC as 000
		
	Cast(Sum(Round(IIF(cc.ChargeCode = '0110', ISNULL(PremiumCededToThePool,0),0),2))*100 as Int)	TotalWrittenPremiumsTraumaticC9,
	Cast(Sum(Round(IIF(cc.ChargeCode = '0210', ISNULL(PremiumCededToThePool,0),0),2))*100 as Int)	TotalWrittenPremiumsOccupationalC10,

	Cast(Sum(Round(IIF(TL.TypeOfLoss = '0110', ISNULL(TotalPaidLosses,0),2),0))*100 as Int) 		TotalLossPaymentsTraumaticC11,
	Cast(Sum(Round(IIF(TL.TypeOfLoss = '0210', ISNULL(TotalPaidLosses,0),2),0))*100 as Int) 		TotalLossPaymentsOccupationalC12,
	Cast(0.228*(Sum(Round(IIF(cc.ChargeCode = '0110',ISNULL(PremiumCededToThePool,0),0),2)) - Sum(Round(IIF(cc.ChargeCode = '0110',ISNULL(UncollectedPremiums,0),0),2)))*100 as INT) 	TotalServicingCarrierAllowancesTraumaticC13,
	Cast(0.228*(Sum(Round(IIF(cc.ChargeCode = '0210',ISNULL(PremiumCededToThePool,0),0),2)) - Sum(Round(IIF(cc.ChargeCode = '0210',ISNULL(UncollectedPremiums,0),0),2)))*100 as INT) 	TotalServicingCarrierAllowancesOccupationalC14,
	Cast(Sum(Round(IIF(cc.ChargeCode = '0110',ISNULL(ProducerFeesOnPremiumWritten,0),0),2))*100 as Int) 	TotalProducerFeesTraumaticC15,
	Cast(Sum(Round(IIF(cc.ChargeCode = '0210',ISNULL(ProducerFeesOnPremiumWritten,0),0),2))*100 as Int) 	TotalProducerFeesOccupationalC16,
	Cast(Sum(Round(IIF(cc.ChargeCode = '0110',ISNULL(UncollectedPremiums,0),0),2))*100 as Int) 	TotalUncollectedPremiumsTraumaticC17,
	Cast(Sum(Round(IIF(cc.ChargeCode = '0210',ISNULL(UncollectedPremiums,0),0),2))*100 as Int) 	TotalUncollectedPremiumsOccupationalC18,
	0 TotalCollectionExpensesTraumaticC19,
	0 TotalCollectionExpensesOccupationalC20,
 
	Replicate(' ',23) FillerC21
	 
	From CTE_CededPremium UE 
	Left Join CTE_ChargeCode cc on cc.PolicyNumber = UE.PolicyNumber and cc.TermNumber = UE.TermNumber
	Left join CTE_UnCollectedPremium UC  on UE.PolicyNumber = UC.PolicyNumber and UE.TermNumber = UC.TermNumber
	Left Join CTE_TotalLoss TL on UE.PolicyNumber = TL.PolicyNumber and UE.TermNumber = TL.TermNumber</property>
