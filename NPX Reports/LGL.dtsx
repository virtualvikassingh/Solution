              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Declare @QuarterEndDate as  date
Declare @QuarterStartDate as date

Set @QuarterStartDate = ?
Set @QuarterEndDate =  ?


Select 
TransactionCode,
PoolCode,
CarrierCode,
QuarterEndingDate,
"Claim Number" ClaimNumber,
PolicyNumber,
StateCode,
ReportType,
OccuranceIndicator,
PolicyEffectiveDate,
AccidentDate,
Left(ClaimStatus,1) ClaimStatus,
sum("Loss Payments - Indemnity")-sum(IndemnityRecovery) AccumulatedLossIndemnity,
sum("Loss Payments - Medical")-sum(MedicalRecovery) AccumulatedLossMedical,
sum("Outstanding Losses - Indemnity") OutstandingIndemnity,
sum("Outstanding Losses  - Medical") OutstandingMedical
from 
(


Select 
	'LGL'	TransactionCode,	
	'15'	PoolCode,
	'19968'	CarrierCode,		
	Replace(Convert(varchar, @QuarterEndDate,110),'-','') QuarterEndingDate,  
	ClaimNumber as "Claim Number", 
	PolicyNumber,
	'210'		StateCode,
	IIF(c.TypeofLoss in ('Cumulative Injury (other than disease)','Occupational Disease'), 'O', 'T') ReportType,
	'0'			OccuranceIndicator,
	Replace(Convert(varchar, EffectiveDate,110),'-','') PolicyEffectiveDate,
	Replace(Convert(varchar, LossDate,110),'-','')		AccidentDate,
	IIF(c.ReopenDate is NOT NULL,'Reopen', ClaimStatus) ClaimStatus,

Sum(Case when F.TransactionType = 'Payment' then ISNULL(F.MedicalPaid,0) else 0 end
    )*100 As "Loss Payments - Medical",
Sum(Case when F.TransactionType = 'Recovery' then ISNULL(F.MedicalRecovery,0)else 0 end
    )*100 As MedicalRecovery,
Sum(Case when F.TransactionType = 'Payment'  
    then ISNULL(F.IndemnityPaid,0) + ISNULL(F.EmployersLiabilityPaid,0) else 0 end 
	)*100 As "Loss Payments - Indemnity",
Sum(Case when F.TransactionType = 'Recovery'  
    then ISNULL(F.IndemnityRecovery,0) + ISNULL(F.EmployerLiabilityRecovery,0) else 0 end 
	)*100 As IndemnityRecovery,
--Sum(Case when F.TransactionType = 'Reserve' 
--   then ISNULL(F.MedicalReserve,0) + ISNULL(F.IndemnityReserve,0) + ISNULL(F.EmployerLiabilityReserve,0) end
--	)*100  As "Total Reserve",
--Sum(F.SubrogationRecovery) SubrogationRecovery,

ISNULL(SUM(Case when (case when  cast(D.Date as date) &gt;= cast(f.TransactionCreateDate as date) then cast(D.Date as date) else 
cast(f.TransactionCreateDate as date) end&lt;=@QuarterEndDate) then ( ISNULL(MedicalReserve,0) 
- ( IIF(F.ErodeReserveInd = 'Y',ISNULL(MedicalPaid,0),0) ))end),0)*100 As "Outstanding Losses  - Medical",

ISNULL(SUM(Case when (case when  cast(D.Date as date) &gt;= cast(f.TransactionCreateDate as date) then cast(D.Date as date) else 
cast(f.TransactionCreateDate as date) end&lt;=@QuarterEndDate) then ( (ISNULL(F.IndemnityReserve,0)  + ISNULL(F.EmployerLiabilityReserve,0)) 
- ( IIF(F.ErodeReserveInd = 'Y',ISNULL(F.IndemnityPaid,0),0) +   IIF(F.ErodeReserveInd = 'Y',
ISNULL(F.EmployersLiabilityPaid,0),0)))end),0)*100 As "Outstanding Losses - Indemnity",

case when  cast(D.Date as date) &gt;= cast(f.TransactionCreateDate as date) then cast(D.Date as date) else cast(f.TransactionCreateDate as date) end as "Financial Date"

From  FactClaimTransaction As F
left join DimClaim As C on F.ClaimID = C.ClaimID 
left join DimExposure As E on F.ExposureID = E.ExposureID
left join DimDate As D on F.TransactionDateID = D.DateID
left join DimPolicy As P on F.PolicyID = P.PolicyId
left join DimCompany As CO on F.CompanyID = CO.CompanyID
where 
P.BaseState = 'MI' and
P.RatingPlan is Not NULL  and  --Michigan can not have Rating Plan value as NULL
P.PolicyNumber &lt;&gt; '-1' and
F.LegacySourceSystem = 'WC' 

Group by ClaimNumber,PolicyNumber,TypeOfLoss,Replace(Convert(varchar, EffectiveDate,110),'-',''),Replace(Convert(varchar, LossDate,110),'-','')	,
IIF(c.ReopenDate is NOT NULL,'Reopen', ClaimStatus),case when  cast(D.Date as date) &gt;= cast(f.TransactionCreateDate as date) then cast(D.Date as date) else cast(f.TransactionCreateDate as date) end

) m
where [Financial Date] &lt;=@QuarterEndDate
group by 
TransactionCode,
PoolCode,
CarrierCode,
QuarterEndingDate,
"Claim Number",
PolicyNumber,
StateCode,
ReportType,
OccuranceIndicator,
PolicyEffectiveDate,
AccidentDate,
Left(ClaimStatus,1)
having Sum([Loss Payments - Medical] + [Loss Payments - Indemnity]) &gt; 10000000
or Sum([Loss Payments - Indemnity]+[Loss Payments - Medical]+[Outstanding Losses - Indemnity]+[Outstanding Losses  - Medical]) &gt;25000000</property>
