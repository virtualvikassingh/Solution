                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Declare @QuarterEndDate Date='2018-03-31';
Declare @QuarterStartDate Date='2018-01-01';


 
;With CTE_UnCollectedPremium as
(
Select			
	EffectiveDate ,
	ExpirationDate,
	PolicyNumber ,
	PrimaryInsuredName ,
	RatingPlan ,
	BaseState,
	State,
	TermNumber,
	Sum(UncollectedPremiums) AS UNCOLLECTEDPREMIUMS
From (

    SELECT
		 PolicyNumber,
		 RatingPlan,
		 convert(varchar(8),Format(EffectiveDate,'yyyyMMdd')) EffectiveDate ,
		 convert(varchar(8),Format(ExpirationDate,'yyyyMMdd')) ExpirationDate,
		 PrimaryInsuredName,
		 p.BaseState,
		 p.State,
		 p.TermNumber,
		Sum(case when BT.TransactionTypeCode in ('ChargeWrittenOff') and BT.WriteOffReason = 'Uncollectable' then BT.TransactionAmount else 0 end) As UncollectedPremiums 
    
	FROM FactBillingTransaction F 
		Inner Join DimBillingTransaction BT on F.BillingTransactionID = BT.BillingTransactionID  
		Inner join DimPolicy p on  F.PolicyID = P.PolicyID
		Inner Join DimInvoiceItem IT on F.InvoiceItemID = it.InvoiceItemID
    
	where P.PolicyStatus		= 'Bound' 
		and F.LegacySourceSystem = 'WC'
		And cast(BT.TransactionDate as date) Between @QuarterStartDate and @QuarterEndDate
		Group by P.PolicyNumber, RatingPlan, convert(varchar(8),Format(EffectiveDate,'yyyyMMdd')),convert(varchar(8),Format(ExpirationDate,'yyyyMMdd')),
		 PrimaryInsuredName,p.BaseState,p.State,p.TermNumber
) m
Group By EffectiveDate,ExpirationDate,PolicyNumber,PrimaryInsuredName,RatingPlan,BaseState,State,TermNumber

)


,CTE_Cancelled As(
Select distinct policynumber,termnumber,convert(varchar(8),Format(CancellationDate,'yyyyMMdd')) CancellationDate from (
select policynumber,termnumber,JobCreateTime,CancellationDate,dense_rank() over(partition by policynumber,termnumber 
order by jobupdatetime desc) rank 
from dimpolicy where policystatus in ('Bound') 
) m where rank=1 and CancellationDate is not null 
)
,CTE_Renewal As(
Select distinct policynumber, 1 as Renewal from dimpolicy where TransactionType ='Renewal' and PolicyStatus ='Bound'
)

,CTE_RenewalExpiration As(
Select * from (
Select Expirationdate,p.PolicyNumber,p.TermNumber, row_number() over (partition by p.PolicyNumber,p.TermNumber order by jobclosedate desc) rank 
from dimpolicy p where p.jobclosedate &lt; 
(select JobCloseDate from dimpolicy p1 where p1.policynumber=p.policynumber and p.TermNumber=p1.termnumber-1 and p1.TransactionType in ('Renewal','Rewrite')
and p1.PolicyStatus='Bound' and p1.policynumber &lt;&gt; 'ARP120006138') and jobclosedate is not null and policynumber is not null and termnumber is not null 
) m where m.rank=1
union all
Select * from (
Select Expirationdate,p.PolicyNumber,p.TermNumber, row_number() over (partition by p.PolicyNumber,p.TermNumber order by jobclosedate desc) rank 
from dimpolicy p where p.jobclosedate &gt;= 
(select JobCloseDate from dimpolicy p1 where p1.policynumber=p.policynumber and p.termnumber=p1.termnumber and p1.TransactionType in('Renewal','Rewrite') 
and p1.PolicyStatus='Bound' and p1.policynumber &lt;&gt; 'ARP120006138')  and jobclosedate is not null and policynumber is not null and termnumber is not null
) n where rank=1
) 

,CTE_AllExpiration As (
Select * from (
select Expirationdate,p.PolicyNumber,p.TermNumber, row_number() over (partition by p.PolicyNumber,p.TermNumber order by jobclosedate desc) rank 
from dimpolicy p where jobclosedate is not null and policynumber is not null and termnumber is not null
) l where rank=1
)

,CTE_GoverningClass As (
 Select * from (
select policynumber,termnumber,GoverningClassCode,row_number() over(partition by policynumber,termnumber
order by JobUpdateTime desc ) rank 
 from DimPolicy where policynumber is not null and termnumber is not null) m where rank=1
 )


select	Distinct 
		'19968'	[Carrier Code],
		'19968'	[CARRIER PUP CODE],
		Case BaseState when 'MI' Then '15' when 'IN' then '13' when 'WI' then '48' when 'NM' then '08' when 'ME' then '16'
			when 'MA' then '18' when 'TN' then '19' else '12' End [POOL CODE],
		Case when RatingPlan='Rating Plan A' then '0300' when RatingPlan='Rating Plan B' then '0400' when RatingPlan='Rating Plan C' then '0500' 
			When Basestate in ('IN','MA','WI') then '0100' when Basestate not in ('MI','IN','WI','NM','ME','MA','TN') and gov.GoverningClassCode in 
			('1016','1005','0164','0168','0156','0161','0162') then '0200' when Basestate not in ('MI','IN','WI','NM','ME','MA','TN') and 
			gov.GoverningClassCode not in ('1016','1005','0164','0168','0156','0161','0162') then '0100' End  As 
		[REPORT CODE],
		convert(varchar(8),Format(@QuarterEndDate,'yyyyMMdd')) [QUARTER END DATE],
		Case BaseState 
			when 'AL' then '01'	when 'AK' then '54' when 'AZ' then '02'	when 'AR' then '03'	when 'CA' then '04' when 'CO' then '05'
			when 'CT' then '06'	when 'DE' then '07'	when 'DC' then '08'	when 'FL' then '09'	when 'GA' then '10' when 'HI' then '52'
			when 'ID' then '11'	when 'IL' then '12'	when 'IN' then '13'	when 'IA' then '14'	when 'KS' then '15'	when 'KY' then '16'
			when 'LA' then '17'	when 'ME' then '18'	when 'MD' then '19'	when 'MA' then '20'	when 'MI' then '21'	when 'MN' then '22'
			when 'MS' then '23'	when 'MO' then '24'	when 'MT' then '25'	when 'NE' then '26'	when 'NV' then '27'	when 'NH' then '28'
			when 'NJ' then '29'	when 'NM' then '30'	when 'NY' then '31'	when 'NC' then '32'	when 'ND' then '33'	when 'OH' then '34'
			when 'OK' then '35'	when 'OR' then '36'	when 'PA' then '37'	when 'RI' then '38'	when 'SC' then '39'	when 'SD' then '40'
			when 'TN' then '41'	when 'TX' then '42'	when 'UT' then '43'	when 'VT' then '44'	when 'VA' then '45'	when 'WA' then '46'
			when 'WV' then '47'	when 'WI' then '48'	when 'WY' then '49'	End [GOVERNING STATE CODE],
		--BaseState [GOVERNING STATE CODE],
		PrimaryInsuredName [NAME OF INSURED],
		po.PolicyNumber [POLICY NUMBER IDENTIFIER],
		EffectiveDate [POLICY EFFECTIVE DATE],
		convert(varchar(8),Format(Coalesce(cnl.CancellationDate,Coalesce(ex.ExpirationDate,eax.ExpirationDate)),'yyyyMMdd')) [POLICY EXPIRATION DATE],
		Case When ren.Renewal=1 then '02' else '01' End [NEW/RENEWAL INDICATOR],
		'N' [MULTISTATE INDICATOR],
		'01' [TRANSACTION TYPE]	,	
		Case when Sum(UNCOLLECTEDPREMIUMS)&lt;0 then convert(varchar(12),format(Sum(UNCOLLECTEDPREMIUMS),'00000000000')) 
		else convert(varchar(12),format(Sum(UNCOLLECTEDPREMIUMS),'000000000000')) end [UNCOLLECTIBLE PREMIUM]
From CTE_UnCollectedPremium po 
Left Join CTE_Cancelled cnl ON cnl.PolicyNumber=po.PolicyNumber and cnl.TermNumber=po.TermNumber  
Left Join CTE_Renewal ren ON ren.PolicyNumber=po.PolicyNumber 
Left Join CTE_RenewalExpiration ex ON ex.PolicyNumber=po.PolicyNumber and ex.TermNumber=po.TermNumber 
Left Join CTE_AllExpiration  eax ON eax.PolicyNumber=po.PolicyNumber and eax.TermNumber=po.TermNumber 
Left Join CTE_GoverningClass gov ON gov.PolicyNumber=po.PolicyNumber and gov.TermNumber=po.TermNumber 
--where po.BaseState='MI'                                              
group by 
Case BaseState when 'MI' Then '15' when 'IN' then '13' when 'WI' then '48' when 'NM' then '08' when 'ME' then '16'
		when 'MA' then '18' when 'TN' then '19' else '12' End,
Case when RatingPlan='Rating Plan A' then '0300' when RatingPlan='Rating Plan B' then '0400' when RatingPlan='Rating Plan C' then '0500' 
		When Basestate in ('IN','MA','WI') then '0100' when Basestate not in ('MI','IN','WI','NM','ME','MA','TN') and gov.GoverningClassCode in 
		('1016','1005','0164','0168','0156','0161','0162') then '0200' when Basestate not in ('MI','IN','WI','NM','ME','MA','TN') and 
		gov.GoverningClassCode not in ('1016','1005','0164','0168','0156','0161','0162') then '0100' End,
Case BaseState 
			when 'AL' then '01'	when 'AK' then '54' when 'AZ' then '02'	when 'AR' then '03'	when 'CA' then '04' when 'CO' then '05'
			when 'CT' then '06'	when 'DE' then '07'	when 'DC' then '08'	when 'FL' then '09'	when 'GA' then '10' when 'HI' then '52'
			when 'ID' then '11'	when 'IL' then '12'	when 'IN' then '13'	when 'IA' then '14'	when 'KS' then '15'	when 'KY' then '16'
			when 'LA' then '17'	when 'ME' then '18'	when 'MD' then '19'	when 'MA' then '20'	when 'MI' then '21'	when 'MN' then '22'
			when 'MS' then '23'	when 'MO' then '24'	when 'MT' then '25'	when 'NE' then '26'	when 'NV' then '27'	when 'NH' then '28'
			when 'NJ' then '29'	when 'NM' then '30'	when 'NY' then '31'	when 'NC' then '32'	when 'ND' then '33'	when 'OH' then '34'
			when 'OK' then '35'	when 'OR' then '36'	when 'PA' then '37'	when 'RI' then '38'	when 'SC' then '39'	when 'SD' then '40'
			when 'TN' then '41'	when 'TX' then '42'	when 'UT' then '43'	when 'VT' then '44'	when 'VA' then '45'	when 'WA' then '46'
			when 'WV' then '47'	when 'WI' then '48'	when 'WY' then '49'	End,
PrimaryInsuredName ,
po.PolicyNumber ,
EffectiveDate ,
Coalesce(cnl.CancellationDate,Coalesce(ex.ExpirationDate,eax.ExpirationDate)) ,
Case When ren.Renewal=1 then '02' else '01' End
--Having Sum(UncollectedPremiums) &gt; 0</property>
