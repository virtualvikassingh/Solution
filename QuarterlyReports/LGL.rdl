  <DataSets>
    <DataSet Name="ldlRpt">
      <Query>
        <DataSourceName>lgl</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@TypeOfLoss">
            <Value>=Parameters!TypeOfLoss.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@QuarterEndDate">
            <Value>=Parameters!AsofDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BaseState">
            <Value>=Parameters!BaseState.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @QuarterEndDate as date = '3/31/2019';
--Declare @BaseState as varchar(10) = 'NCCI'
--declare @TypeOfLoss as varchar(200) = 'Occupational'
 

SELECT
 TransactionCode,
	PoolCode,
	CarrierCode,
	QuarterEndingDate,
	"Claim Number" ClaimNumber,
	PolicyNumber,
	StateCode,
	ReportType,
	OccuranceIndicator,
	PolicyEffectiveDate,
	AccidentDate,

	Left(ClaimStatus,1) ClaimStatus,
	CONVERT(int, SUM("Loss Payments - Indemnity") - SUM(IndemnityRecovery)) AccumulatedLossIndemnity,
	CONVERT(int, SUM("Loss Payments - Medical")-SUM(MedicalRecovery)) AccumulatedLossMedical,
	CONVERT(int, SUM("Outstanding Losses - Indemnity")) OutstandingIndemnity,
	CONVERT(int, SUM("Outstanding Losses  - Medical")) OutstandingMedical
FROM (
	SELECT 
		'LGL'	TransactionCode,	
		 IIF(BaseState = 'MI', '15', '12')   as PoolCode,
		'19968'	CarrierCode,		
		REPLACE(CONVERT(varchar, @QuarterEndDate,110),'-','') QuarterEndingDate,  
		ClaimNumber as "Claim Number", 
		PolicyNumber,
		IIF(BaseState = 'MI', '210', StateCode2Digits + '1' )as  	StateCode,
		IIF(c.TypeofLoss in ('Cumulative Injury (other than disease)','Occupational Disease'), 'O', 'T') ReportType,
		'0'			OccuranceIndicator,
		REPLACE(CONVERT(varchar, EffectiveDate,110),'-','') PolicyEffectiveDate,
		REPLACE(CONVERT(varchar, LossDate,110),'-','')		AccidentDate,
		IIF(c.ReopenDate is NOT NULL,'Reopen', ClaimStatus) ClaimStatus,
	Sum(CASE WHEN F.TransactionType = 'Payment' THEN ISNULL(F.MedicalPaid,0) ELSE 0 end)*100 As "Loss Payments - Medical",
	Sum(CASE WHEN F.TransactionType = 'Recovery' THEN ISNULL(F.MedicalRecovery,0)ELSE 0 end)*100 As MedicalRecovery,
	Sum(CASE WHEN F.TransactionType = 'Payment' THEN ISNULL(F.IndemnityPaid,0) + ISNULL(F.EmployersLiabilityPaid,0) ELSE 0 end)*100 As "Loss Payments - Indemnity",
	Sum(CASE WHEN F.TransactionType = 'Recovery' THEN ISNULL(F.IndemnityRecovery,0) + ISNULL(F.EmployerLiabilityRecovery,0) ELSE 0 end)*100 As IndemnityRecovery,
	ISNULL(SUM(CASE 
					WHEN (
						CASE 
							WHEN CAST(D.Date as date) &gt;= CAST(f.TransactionCreateDate as date) THEN CAST(D.Date as date) 
							ELSE CAST(f.TransactionCreateDate as date) 
						end&lt;=@QuarterEndDate) 
					THEN ( ISNULL(MedicalReserve,0) - ( IIF(F.ErodeReserveInd = 'Y',ISNULL(MedicalPaid,0),0) ))
				end),0)*100 As "Outstanding Losses  - Medical",
	ISNULL(SUM(CASE 
					WHEN (
						CASE 
							WHEN  CAST(D.Date as date) &gt;= CAST(f.TransactionCreateDate as date) THEN CAST(D.Date as date) 
							ELSE CAST(f.TransactionCreateDate as date) 
						END&lt;=@QuarterEndDate) 
					THEN ((ISNULL(F.IndemnityReserve,0) + ISNULL(F.EmployerLiabilityReserve,0)) - ( IIF(F.ErodeReserveInd = 'Y',ISNULL(F.IndemnityPaid,0),0) +   IIF(F.ErodeReserveInd = 'Y',ISNULL(F.EmployersLiabilityPaid,0),0)))
				end),0)*100 As "Outstanding Losses - Indemnity",
	CASE WHEN  CAST(D.Date as date) &gt;= CAST(f.TransactionCreateDate as date) THEN CAST(D.Date as date) ELSE CAST(f.TransactionCreateDate as date) end as "Financial Date"
	FROM  FactClaimTransaction As F
	LEFT JOIN DimClaim As C on F.ClaimID = C.ClaimID and f.TransactionStatus &lt;&gt; 'rejected'
	LEFT JOIN DimExposure As E on F.ExposureID = E.ExposureID
	LEFT JOIN DimDate As D on F.TransactionDateID = D.DateID
	LEFT JOIN DimPolicy As P on F.PolicyID = P.PolicyId
	LEFT JOIN DimCompany As CO on F.CompanyID = CO.CompanyID
	Left Join DimState ds on p.BaseState = ds.StateAbbr
	WHERE P.BaseState in (Select Ltrim(Rtrim(Data)) From dbo.udf_split(Case When @BaseState= 'MI' Then 'MI' When @BaseState = 'NCCI' Then 'CT, IL, SC, VA' When @BaseState = 'ALL' Then 'MI, CT, IL, SC, VA'  END,','))
	--AND ISNULL(c.TypeOfLoss,'') in ( Select ISNULL(tl.TypeOfLoss,'') from CTE_TypeOfLoss tl)	
	AND IIF(TypeofLoss In ('Cumulative Injury (other than disease)','Occupational Disease'),'Occupational', 'Traumatic') in (@TypeOfLoss)
	and P.PolicyNumber &lt;&gt; '-1' 
	Group by ClaimNumber,PolicyNumber,TypeOfLoss,Replace(Convert(varchar, EffectiveDate,110),'-',''),Replace(Convert(varchar, LossDate,110),'-','')	, BaseState,StateCode2Digits,
	IIF(c.ReopenDate is NOT NULL,'Reopen', ClaimStatus),CASE WHEN  CAST(D.Date as date) &gt;= CAST(f.TransactionCreateDate as date) THEN CAST(D.Date as date) ELSE CAST(f.TransactionCreateDate as date) end

) m
WHERE [Financial Date] &lt;= @QuarterEndDate
GROUP BY TransactionCode,PoolCode,CarrierCode,QuarterEndingDate,"Claim Number",PolicyNumber,StateCode,ReportType,OccuranceIndicator,PolicyEffectiveDate,AccidentDate,Left(ClaimStatus,1)
HAVING (Sum([Loss Payments - Medical] + [Loss Payments - Indemnity]) - SUM(MedicalRecovery + IndemnityRecovery))  &gt; 10000000
or (Sum([Loss Payments - Indemnity]+[Loss Payments - Medical]+[Outstanding Losses - Indemnity]+[Outstanding Losses  - Medical]) - SUM(MedicalRecovery + IndemnityRecovery)) &gt; 25000000</CommandText>
      </Query>
