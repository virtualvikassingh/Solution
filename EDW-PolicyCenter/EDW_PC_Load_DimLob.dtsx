		--le.NAME														AS CA7LegalEntity,
		--cpt.TYPECODE												AS CA7PolicyType,
		--cft.TYPECODE												AS CA7Fleet,
		--cft.DESCRIPTION												AS CA7FleetDesc,
		--Case When pc.cr7packagepolicy = 1 then 'Y'
		--	 When pc.cr7packagepolicy = 0 then 'N'
		--End															AS IsCR7PackagePolicy,
		--Case When pc.cr7packagepolicy = 1 then cpc.NAME
		--	 When pc.cr7packagepolicy = 0 then cmc.NAME
		--	 Else Null  
		--End															As CR7CoverageType,
		--Case When pc.cr7packagepolicy = 1 then cp.NAME
		--	 When pc.cr7packagepolicy = 0 then cmp.NAME
		--	 Else Null  
		--End															AS CR7PolicyType,
		--cpa.NAME													AS CR7PredominantActivity,
		--ccd.NAME													AS CR7ClassDescription,
		--cast(crcc.Value   as varchar(10))                           AS CR7ClassCode,
		--ce.NAME														AS CR7EntityType,
		--cm.NAME														AS CR7MajorIndustryGroup,
		--pc.cr7numratableemps1                                       AS CR7TotalNumofRatableEmps,
		--pc.cr7totalnumprem                                          AS CR7TotalNumofPremises,
		cast(minstatejur.NAME as varchar(30))                         AS MinimumPremiumState,
		CAST(concat(pc.BeanVersion,pcxwc7.BeanVersion) AS varchar(20)) AS BeanVersion,
		case when ret.WC7WorkersCompLine is NULL then 'N'
			when ret.WC7WorkersCompLine is Not NULL then 'Y'
	    end                                                           AS RetrospctiveRatingIndicator,
'WC' as LegacySourceSystem

FROM				dbo.pc_policyline AS pc 
                LEFT OUTER JOIN dbo.pctl_glcoverageformtype AS glc ON pc.GLCoverageForm = glc.ID 
				LEFT JOIN dbo.pctl_smallbusinesstype sbType ON sbType.ID = pc.SmallBusinessType
				LEFT JOIN dbo.pctl_policyline pl ON pc.Subtype = pl.ID
				Left join dbo.pc_wcclasscode pcxwc7 ON pcxwc7.ID = pc.GoverningClass --WCGoverningClass 
				--LEFT JOIN dbo.pctl_bp7propertytype pt ON pt.ID = pc.BP7LineBusinessType
				--LEFT JOIN dbo.pctl_ca7risktype rt ON rt.ID = pc.ca7risktype
				--LEFT JOIN pctl_ca7legalentity le ON le.ID = pc.ca7legalentity
				--LEFT JOIN pctl_ca7policytype cpt ON cpt.ID = pc.ca7policytype
				--LEFT JOIN pctl_ca7fleetty cft ON cft.ID = pc.CA7Fleet
				--LEFT JOIN pctl_cr7monolinecoveragetype cmc ON cmc.ID = pc.cr7monolinecoveragetype
				--LEFT JOIN pctl_cr7packagecoveragetype cpc ON cpc.ID = pc.cr7packagecoveragetype
				--LEFT JOIN pctl_cr7monolinepolicytype cmp ON cmp.ID = pc.cr7monolinepolicytype
				--LEFT JOIN pctl_cr7packagepolicytype cp ON cp.ID = pc.cr7packagepolicytype
				--LEFT JOIN pctl_cr7predominantactivity cpa ON cpa.ID = pc.cr7predominantactivity
				--LEFT JOIN pctl_cr7classdescription ccd ON ccd.ID = pc.cr7classdescription
				--LEFT JOIN pctl_cr7entitytype ce ON ce.ID = pc.cr7entitytype
				--LEFT JOIN pctl_cr7majorindustrygroup cm ON cm.ID = pc.cr7majorindustrygroup
				--LEFT JOIN pcx_cr7classcode crcc ON crcc.PredominantActivity = cpa.TYPECODE
				LEFT JOIN dbo.pctl_jurisdiction minstatejur ON minstatejur.ID = pc.WCMinPremJurisdiction_Ext
				Left join  dbo.pcx_wc7retroratingplan  ret on pc.id = ret.WC7WorkersCompLine and ret.BranchID=pc.BranchID
				--Left join dbo.pcx_wc7retroratingplan ret on pc.id = ret.WC7WorkersCompLine


WHERE
(pc.UpdateTime &gt;= DATEADD(d, ?, GETDATE()))
OR
(pcxwc7.UpdateTime &gt;=DATEADD(d, ?, GETDATE()))</property>
