                <component
                  refId="Package\Sequence Container\Name Record Load\OLE SRC - FactWCPremiumTransaction"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="OLE SRC - FactWCPremiumTransaction"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">;with QualifiedPolicy as
(
SELECT distinct p.PolicyID
from FactWCPremiumTransaction F
	left Join DimPolicy P on F.PolicyID = P.PolicyID
	left Join DimLOB L on L.LOBID = F.LOBID
	--05 OCT - VS - Added join on DimState to use IsNCCI column. This column is replacing all the NCCI States e.g. IN, VA, CT, IL etc.
	Left join DimState ds on p.BaseState = ds.StateAbbr

where L.LOBCode in ( 'WorkersCompLine','WC7Line')
	and (
			(p.TransactionType in ('Submission','Policy Change','Rewrite') and (cast(P.JobUpdateTime as date) &gt;? and cast(P.JobUpdateTime as date) &lt;= ?)) OR
			(p.TransactionType ='Renewal' and p.eventname='SendNonRenewalDocuments' and p.BaseState='MI' and (cast(P.JobUpdateTime as date) &gt;DATEADD(day, -1, convert(date,?)) and cast(P.JobUpdateTime as date) &lt;=DATEADD(day, -1, convert(date,?)))) OR
			(cast(p.JobUpdateTime as date) &gt;? and cast(p.JobUpdateTime as date) &lt;=? and p.transactiontype='Renewal' and p.eventname is null) OR
			(cast(p.JobUpdateTime as date) &gt;? and cast(p.JobUpdateTime as date) &lt;=? and p.transactiontype='Renewal' and p.eventname='SendNonRenewalDocuments' and ds.IsNCCI = 1 /*p.BaseState = 'IN'*/)
	)
	and p.PolicyStatus = 'Bound'  	 
 
 
)

,PrimaryNotPartner as --primary insured non partnership
(
  select qp.PolicyID 
  ,pc.PolicyContactID 
  ,pc.PolicyContactID as OwnerEntityPolicyContactID
  ,pc.Contactname 
  ,pc.DBAName
  ,pc.orgtype
  ,pc.ContactTypeName
  ,upper(ltrim(rtrim(coalesce(pc.orgtype,p.OrganizationType)))) as OrganizationType
  from QualifiedPolicy qp
  join DimPolicy p on p.PolicyID = qp.PolicyID
  join PolicyContacts pc on pc.PolicyID = qp.PolicyID 
  where p.OrganizationType not in ('Partnership','LLP','Sole Proprietorship')
  and (pc.ContactRoleName = 'PolicyPriNamedInsured' and pc.orgtype not in ('Partnership','LLP')  --Primary insured, not Partnership or LLP
       or 
	   pc.ContactRoleName = 'PolicyAddlNamedInsured' and pc.orgtype not in ('Partnership','LLP') --Additional insured, not Partnership or LLP
	  )  
)

,PrimarySoleProprietorship as --primary insured non partnership
(
  select qp.PolicyID 
  ,pc.PolicyContactID 
  ,(select PolicyContactID from PolicyContacts primaryc where primaryc.PolicyID = qp.PolicyID and primaryc.ContactRoleName = 'PolicyPriNamedInsured'
  and primaryc.ContactName=p.PrimaryInsuredName) as OwnerEntityPolicyContactID
  ,pc.Contactname 
  ,pc.DBAName
  ,pc.orgtype
  ,pc.ContactTypeName
  ,upper(ltrim(rtrim(coalesce(pc.orgtype,p.OrganizationType)))) as OrganizationType
  --,upper(ltrim(rtrim(p.OrganizationType))) as OrganizationType
  from QualifiedPolicy qp
  join DimPolicy p on p.PolicyID = qp.PolicyID
  join PolicyContacts pc on pc.PolicyID = qp.PolicyID 
  where p.OrganizationType in ('Sole Proprietorship')
  and pc.ContactRoleName in ('WC7ExcludedOwnerOfficer','WC7IncludedOwnerOfficer')
  And RelationshipTitleInternal != 'Spouse'

  union all
  --report dba of primary insured
  select qp.PolicyID 
  ,pc.PolicyContactID 
  ,pc.PolicyContactID as OwnerEntityPolicyContactID
  ,pc.Contactname 
  ,pc.DBAName
  ,pc.orgtype
  ,pc.ContactTypeName
  ,upper(ltrim(rtrim(coalesce(pc.orgtype,p.OrganizationType)))) as OrganizationType
  --,upper(ltrim(rtrim(p.OrganizationType))) as OrganizationType
  from QualifiedPolicy qp
  join DimPolicy p on p.PolicyID = qp.PolicyID
  join PolicyContacts pc on pc.PolicyID = qp.PolicyID 
  where p.OrganizationType in ('Sole Proprietorship')
  and pc.ContactRoleName in ('PolicyPriNamedInsured')
  and pc.DBAName is not null
  
  union all
  --if primary insured not in included&amp;excluded, report it
  select qp.PolicyID 
  ,pc.PolicyContactID 
  ,pc.PolicyContactID as OwnerEntityPolicyContactID
  ,pc.Contactname 
  ,null as DBAName
  ,pc.orgtype
  ,pc.ContactTypeName
  ,upper(ltrim(rtrim(coalesce(pc.orgtype,p.OrganizationType)))) as OrganizationType
  from QualifiedPolicy qp
  join DimPolicy p on p.PolicyID = qp.PolicyID
  join PolicyContacts pc on pc.PolicyID = qp.PolicyID 
  where p.OrganizationType in ('Sole Proprietorship')
  and pc.ContactRoleName in ('PolicyPriNamedInsured')
  and not exists (
      select distinct pc2.Contactname 
      from QualifiedPolicy qp2
      join DimPolicy p2 on p2.PolicyID = qp2.PolicyID
      join PolicyContacts pc2 on pc2.PolicyID = qp2.PolicyID 
      where p2.OrganizationType in ('Sole Proprietorship')
      and pc2.ContactRoleName in ('WC7ExcludedOwnerOfficer','WC7IncludedOwnerOfficer')
	  and qp2.PolicyID = qp.PolicyID
	  and pc2.ContactName = pc.ContactName)

)

,ParmaryPartnerAsCompany as (
  select qp.PolicyID 
  ,pc.PolicyContactID 
  ,case when pc.ContactTypeName = 'Person' and pc.CoverageType = 'Partner' then (select PolicyContactID from PolicyContacts primaryc where primaryc.PolicyID = qp.PolicyID 
  and primaryc.ContactRoleName = 'PolicyPriNamedInsured' and primaryc.ContactName=p.PrimaryInsuredName) else pc.PolicyContactID end as OwnerEntityPolicyContactID
  ,pc.Contactname 
  ,case when pc.ContactRoleName &lt;&gt; 'PolicyPriNamedInsured' and (pc.orgtype in ('Partnership','LLP') or pc.orgtype is null) then null else pc.DBAName end as DBAName
  ,pc.orgtype
  ,pc.ContactTypeName
  ,upper(ltrim(rtrim(coalesce(pc.orgtype,p.OrganizationType)))) as OrganizationType
  from QualifiedPolicy qp
  join DimPolicy p on p.PolicyID = qp.PolicyID
  join PolicyContacts pc on pc.PolicyID = qp.PolicyID 
  where p.OrganizationType in ('Partnership','LLP')
  and p.InsuredContactType = 'Company'
  and (pc.ContactRoleName = 'PolicyPriNamedInsured' and pc.orgtype in ('Partnership','LLP')  --Primary insured, Partnership or LLP
       or 
	   pc.ContactRoleName = 'PolicyAddlNamedInsured' and pc.orgtype not in ('Partnership','LLP') --Additional insured, not Partnership or LLP
	   or
	   pc.ContactTypeName = 'Person' and pc.CoverageType = 'Partner' --Partners
	  )  
)

,ParmaryPartnerAsPersonIN as (
  select qp.PolicyID 
  ,pc.PolicyContactID 
  ,case when pc.ContactTypeName = 'Person' and pc.CoverageType = 'Partner' then (select PolicyContactID from PolicyContacts primaryc where primaryc.PolicyID = qp.PolicyID and primaryc.ContactRoleName = 'PolicyPriNamedInsured') else pc.PolicyContactID end as OwnerEntityPolicyContactID
  ,pc.Contactname 
  ,case when pc.orgtype in ('Partnership','LLP') or pc.orgtype is null then null else pc.DBAName end as DBAName
  ,pc.orgtype
  ,pc.ContactTypeName
  ,upper(ltrim(rtrim(coalesce(pc.orgtype,p.OrganizationType)))) as OrganizationType
  from QualifiedPolicy qp
  join DimPolicy p on p.PolicyID = qp.PolicyID
  join PolicyContacts pc on pc.PolicyID = qp.PolicyID 
  Left join DimState ds on p.BaseState = ds.StateAbbr
  where p.OrganizationType in ('Partnership','LLP')
  and p.InsuredContactType = 'Person'
 -- and p.BaseState = 'IN'
  And ds.IsNCCI = 1 
  and (pc.ContactRoleName = 'PolicyPriNamedInsured' and pc.orgtype in ('Partnership','LLP')  --Primary insured, Partnership or LLP
       or 
	   pc.ContactRoleName = 'PolicyAddlNamedInsured' and pc.orgtype not in ('Partnership','LLP') --Additional insured, not Partnership or LLP
	   or
	   pc.ContactTypeName = 'Person' and pc.CoverageType = 'Partner' --and pc.RelationshipTitleInternal = 'Partner' --Partners
	  )  
)


,ParmaryPartnerAsPersonMI as (
  select qp.PolicyID 
  ,pc.PolicyContactID 
  ,case when pc.ContactTypeName = 'Person' and pc.CoverageType = 'Partner' then (select PolicyContactID from PolicyContacts primaryc where primaryc.PolicyID = qp.PolicyID and primaryc.ContactRoleName = 'PolicyPriNamedInsured') else pc.PolicyContactID end as OwnerEntityPolicyContactID
  ,pc.Contactname 
  ,case when pc.orgtype in ('Partnership','LLP') or pc.orgtype is null then null else pc.DBAName end as DBAName
  ,pc.orgtype
  ,pc.ContactTypeName
 ,upper(ltrim(rtrim(coalesce(pc.orgtype,p.OrganizationType)))) as OrganizationType
  from QualifiedPolicy qp
  join DimPolicy p on p.PolicyID = qp.PolicyID
  join PolicyContacts pc on pc.PolicyID = qp.PolicyID 
  where p.OrganizationType in ('Partnership','LLP')
  and p.InsuredContactType = 'Person'
  and p.BaseState = 'MI'
  and ( 
	   pc.ContactRoleName = 'PolicyAddlNamedInsured' and pc.orgtype not in ('Partnership','LLP') --Additional insured, not Partnership or LLP
	   or
	   pc.ContactTypeName = 'Person' and pc.CoverageType = 'Partner' --and pc.RelationshipTitleInternal = 'Partner' --Partners
	  )  
)

,PrimaryNotPartnerGroupBy as (
  select PolicyID as pgbPolicyID ,OrganizationType as pgbOrganizationType ,Contactname as pgbContactname,min(PolicyContactID) as pgbPolicyContactID ,min(OwnerEntityPolicyContactID) as pgbOwnerEntityPolicyContactID
  from PrimaryNotPartner
  group by PolicyID ,OrganizationType ,Contactname
)

,PrimarySoleProprietorshipGroupBy as (
  select PolicyID as pgbPolicyID ,OrganizationType as pgbOrganizationType ,Contactname as pgbContactname,min(PolicyContactID) as pgbPolicyContactID ,min(OwnerEntityPolicyContactID) as pgbOwnerEntityPolicyContactID
  from PrimarySoleProprietorship
  group by PolicyID ,OrganizationType ,Contactname
)

,ParmaryPartnerAsCompanyGroupBy as (
  select PolicyID as pgbPolicyID ,OrganizationType as pgbOrganizationType ,Contactname as pgbContactname,min(PolicyContactID) as pgbPolicyContactID ,min(OwnerEntityPolicyContactID) as pgbOwnerEntityPolicyContactID
  from ParmaryPartnerAsCompany
  group by PolicyID ,OrganizationType ,Contactname
)

,ParmaryPartnerAsPersonINGroupBy as (
  select PolicyID as pgbPolicyID ,OrganizationType as pgbOrganizationType ,Contactname as pgbContactname,min(PolicyContactID) as pgbPolicyContactID ,min(OwnerEntityPolicyContactID) as pgbOwnerEntityPolicyContactID
  from ParmaryPartnerAsPersonIN
  group by PolicyID ,OrganizationType ,Contactname
)

,ParmaryPartnerAsPersonMIGroupBy as (
  select PolicyID as pgbPolicyID ,OrganizationType as pgbOrganizationType ,Contactname as pgbContactname,min(PolicyContactID) as pgbPolicyContactID ,min(OwnerEntityPolicyContactID) as pgbOwnerEntityPolicyContactID
  from ParmaryPartnerAsPersonMI
  group by PolicyID ,OrganizationType ,Contactname
)

,AllWithDBA as 
(
  select PolicyID ,PolicyContactID ,OwnerEntityPolicyContactID ,OrganizationType ,NameOfInsured ,NameTypeCode
  ,dense_rank() OVER (partition by PolicyID ORDER BY OwnerEntityPolicyContactID) AS NLI
  ,row_number() OVER (partition by PolicyID ,OwnerEntityPolicyContactID ORDER BY seq) AS CSN
  from (
  select PolicyID ,pgbPolicyContactID as PolicyContactID ,pgbOwnerEntityPolicyContactID as OwnerEntityPolicyContactID ,OrganizationType ,case ContactTypeName when 'Person' then substring(Contactname,CHARINDEX(' ',Contactname,1)+1,len(Contactname))+', '+substring(Contactname,1,CHARINDEX(' ',Contactname,1)-1) else  Contactname end as NameOfInsured 
      ,case ContactTypeName when 'Person' then 1 when 'Company' then 2 end as NameTypeCode ,case when PolicyContactID = OwnerEntityPolicyContactID then 1 else 3 end as seq
  from PrimaryNotPartner pri
  join PrimaryNotPartnerGroupBy        on pgbPolicyID = pri.PolicyID and isnull(pgbOrganizationType,'') = isnull(pri.OrganizationType,'') and pgbContactName = pri.ContactName and pgbPolicyContactID = pri.PolicyContactID
  union all
  select PolicyID ,pgbPolicyContactID as PolicyContactID ,pgbOwnerEntityPolicyContactID as OwnerEntityPolicyContactID ,OrganizationType ,DBAName as NameOfInsured ,case ContactTypeName when 'Person' then 1 when 'Company' then 2 end as NameTypeCode ,2 as seq
  from PrimaryNotPartner pri
  join PrimaryNotPartnerGroupBy        on pgbPolicyID = pri.PolicyID and isnull(pgbOrganizationType,'') = isnull(pri.OrganizationType,'') and pgbContactName = pri.ContactName 
  where DBAName is not null
  
  union all
  
  select PolicyID ,PolicyContactID ,OwnerEntityPolicyContactID ,OrganizationType ,case ContactTypeName when 'Person' then substring(Contactname,CHARINDEX(' ',Contactname,1)+1,len(Contactname))+', '+substring(Contactname,1,CHARINDEX(' ',Contactname,1)-1) else  Contactname end as NameOfInsured 
      ,case ContactTypeName when 'Person' then 1 when 'Company' then 2 end as NameTypeCode ,case when PolicyContactID = OwnerEntityPolicyContactID then 1 else 3 end as seq
  from PrimarySoleProprietorship pri 
  join PrimarySoleProprietorshipGroupBy on pgbPolicyID = pri.PolicyID and isnull(pgbOrganizationType,'') = isnull(pri.OrganizationType,'') and pgbContactName = pri.ContactName --and pgbPolicyContactID = pri.PolicyContactID
  where pri.DBAName is null
  union all
  select PolicyID ,pgbPolicyContactID as PolicyContactID ,pgbOwnerEntityPolicyContactID as OwnerEntityPolicyContactID ,OrganizationType ,DBAName as NameOfInsured ,case ContactTypeName when 'Person' then 1 when 'Company' then 2 end as NameTypeCode ,2 as seq
  from PrimarySoleProprietorship pri
  join PrimarySoleProprietorshipGroupBy        on pgbPolicyID = pri.PolicyID and isnull(pgbOrganizationType,'') = isnull(pri.OrganizationType,'') and pgbContactName = pri.ContactName 
  where DBAName is not null

  union all
  
  select PolicyID ,pgbPolicyContactID as PolicyContactID ,pgbOwnerEntityPolicyContactID as OwnerEntityPolicyContactID ,OrganizationType ,case ContactTypeName when 'Person' then substring(Contactname,CHARINDEX(' ',Contactname,1)+1,len(Contactname))+', '+substring(Contactname,1,CHARINDEX(' ',Contactname,1)-1) else  Contactname end as NameOfInsured
        ,case ContactTypeName when 'Person' then 1 when 'Company' then 2 end as NameTypeCode ,case when PolicyContactID = OwnerEntityPolicyContactID then 1 else 3 end as seq
  from ParmaryPartnerAsCompany pri
  join ParmaryPartnerAsCompanyGroupBy  on pgbPolicyID = pri.PolicyID and isnull(pgbOrganizationType,'') = isnull(pri.OrganizationType,'') and pgbContactName = pri.ContactName and pgbPolicyContactID = pri.PolicyContactID
  union all
  select PolicyID ,pgbPolicyContactID as PolicyContactID ,pgbOwnerEntityPolicyContactID as OwnerEntityPolicyContactID ,OrganizationType ,DBAName as NameOfInsured ,case ContactTypeName when 'Person' then 1 when 'Company' then 2 end as NameTypeCode ,2 as seq
  from ParmaryPartnerAsCompany pri
  join ParmaryPartnerAsCompanyGroupBy  on pgbPolicyID = pri.PolicyID and isnull(pgbOrganizationType,'') = isnull(pri.OrganizationType,'') and pgbContactName = pri.ContactName
  where DBAName is not null
  
  union all
  
  select PolicyID ,pgbPolicyContactID as PolicyContactID ,pgbOwnerEntityPolicyContactID as OwnerEntityPolicyContactID ,OrganizationType ,case ContactTypeName when 'Person' then substring(Contactname,CHARINDEX(' ',Contactname,1)+1,len(Contactname))+', '+substring(Contactname,1,CHARINDEX(' ',Contactname,1)-1) else  Contactname end as NameOfInsured
         ,case ContactTypeName when 'Person' then 1 when 'Company' then 2 end as NameTypeCode ,case when PolicyContactID = OwnerEntityPolicyContactID then 1 else 3 end as seq
  from ParmaryPartnerAsPersonIN pri
  join ParmaryPartnerAsPersonINGroupBy on pgbPolicyID = pri.PolicyID and isnull(pgbOrganizationType,'') = isnull(pri.OrganizationType,'') and pgbContactName = pri.ContactName and pgbPolicyContactID = pri.PolicyContactID
  union all
  select PolicyID ,pgbPolicyContactID as PolicyContactID ,pgbOwnerEntityPolicyContactID as OwnerEntityPolicyContactID ,OrganizationType ,DBAName as NameOfInsured ,case ContactTypeName when 'Person' then 1 when 'Company' then 2 end as NameTypeCode ,2 as seq
  from ParmaryPartnerAsPersonIN pri
  join ParmaryPartnerAsPersonINGroupBy on pgbPolicyID = pri.PolicyID and isnull(pgbOrganizationType,'') = isnull(pri.OrganizationType,'') and pgbContactName = pri.ContactName 
  where DBAName is not null
  
  union all
  
  select PolicyID ,pgbPolicyContactID as PolicyContactID ,pgbOwnerEntityPolicyContactID as OwnerEntityPolicyContactID ,OrganizationType ,case ContactTypeName when 'Person' then substring(Contactname,CHARINDEX(' ',Contactname,1)+1,len(Contactname))+', '+substring(Contactname,1,CHARINDEX(' ',Contactname,1)-1) else  Contactname end as NameOfInsured
         ,case ContactTypeName when 'Person' then 1 when 'Company' then 2 end as NameTypeCode ,case when PolicyContactID = OwnerEntityPolicyContactID then 1 else 3 end as seq
  from ParmaryPartnerAsPersonMI pri
  join ParmaryPartnerAsPersonMIGroupBy on pgbPolicyID = pri.PolicyID and isnull(pgbOrganizationType,'') = isnull(pri.OrganizationType,'') and pgbContactName = pri.ContactName and pgbPolicyContactID = pri.PolicyContactID
  union all
  select PolicyID ,pgbPolicyContactID as PolicyContactID ,pgbOwnerEntityPolicyContactID as OwnerEntityPolicyContactID ,OrganizationType ,DBAName as NameOfInsured ,case ContactTypeName when 'Person' then 1 when 'Company' then 2 end as NameTypeCode ,2 as seq
  from ParmaryPartnerAsPersonMI pri
  join ParmaryPartnerAsPersonMIGroupBy on pgbPolicyID = pri.PolicyID and isnull(pgbOrganizationType,'') = isnull(pri.OrganizationType,'') and pgbContactName = pri.ContactName 
  where DBAName is not null
  
  ) qry
)



select qp.PolicyID,
cast(po.PolicyNumber as varchar(18)) as PolicyNumber 
,cast(po.BaseState as varchar(2)) as Policy_State
,'02' as RecordTypeCode
,al.NameTypeCode
,cast(format(isnull(al.NLI,''), '000') as varchar(3)) AS NameLinkIdentifier
,cast(format(isnull(al.CSN,''), '000') as varchar(3)) AS ContinuationSequenceNumber
--,upper(ltrim(rtrim(pct.orgtype))) as OrganizationType
,upper(ltrim(rtrim(coalesce(al.OrganizationType,pct.orgtype,'')))) as OrganizationType 
,case when po.ProfessionalEmployeeType = 'Client Company' then 'C' when po.ProfessionalEmployeeType = 'Professional Employer Organization (PEO)' then 'P'  else ' ' end as PEOCode
,left(isnull(al.NameOfInsured,'')  + replicate(' ',90),90) as InsuredName
,case  when pct.ContactRoleName = 'PolicyPriNamedInsured'   then cast(isnull(replace(PrimaryInsuredFEINID,'-',''), replicate('0',9) )as varchar(9)) 
       when upper(ltrim(rtrim(coalesce(al.OrganizationType,pct.orgtype,''))))='SOLE PROPRIETORSHIP' and pct.ContactTypeName = 'Person' then cast(isnull(replace(PrimaryInsuredFEINID,'-',''), replicate('0',9) )as varchar(9)) 
	   when upper(ltrim(rtrim(coalesce(al.OrganizationType,pct.orgtype,''))))='PARTNERSHIP' and pct.ContactTypeName = 'Person' then cast(isnull(replace(PrimaryInsuredFEINID,'-',''), replicate('0',9) )as varchar(9)) 
       else  cast(isnull(replace(pct.OfficalID,'-',''), replicate('0',9) ) as varchar(9)) 
 end  as FEIN
,'00' as StateCode1
,replicate(' ',15) as StateunemploymentNumber
,'00' as StateCode2
,replicate(' ',15) as StateunemploymentNumber2
,'00' as StateCode3
,replicate(' ',15) as StateunemploymentNumber3
,'00' as StateUnemploymentNumber_RSN
,left(IsNull(pct.OrgTypeOtherDesc, '') + replicate(' ',20),20) as TextforOtherLegalNatureofInsured
,'00' AS NameLinkCounterIdentifier
,IIF( TransactionType ='Policy Change', Right(Convert(varchar(10),po.EffectiveDate ,112),6), Replicate('0',6)) as PolicyChangeEffectiveDate
,IIF( TransactionType ='Policy Change', Right(Convert(varchar(10),po.ExpirationDate,112),6), Replicate('0',6)) as PolicyChangeExpirationDate
from QualifiedPolicy qp
join DimPolicy Po on po.PolicyID = qp.PolicyID
join AllWithDBA al on al.PolicyID = qp.PolicyID
join PolicyContacts pct on pct.PolicyContactID = al.PolicyContactID
 
order by  po.PolicyNumber</property>
