                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset">[dbo].[DimPolicy]</property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">With QualifiedPol as 
(
	Select PolicyID, PolicyNumber, TransactionType, PolicyTransactionEffectiveDate, CreateTime 
	From DimPolicy  
	Where TransactionType in ('Reinstatement') 
		And PolicyStatus='Bound'
		And (cast(JobUpdateTime as date) &gt; ? And cast(JobUpdateTime as date) &lt;= ?)

	UNION	--Get 'Cancelling','Bound' Cancellation based on JobCreateTime, will only report once

	Select PolicyID, PolicyNumber, TransactionType, PolicyTransactionEffectiveDate ,CreateTime 
	From DimPolicy  
	Where TransactionType in ('Cancellation') 
		And PolicyStatus in ('Canceling','Bound','Rescinded')
	  And cast(JobCreateTime as date) &gt; ? And cast(JobCreateTime as date) &lt;= ?

	UNION	--Get 'Rescinded' Cancellation base on JobUpdateTime, consider it as reinstatement in WCPOLS
	
	Select PolicyID, PolicyNumber, 'Reinstatement' as TransactionType, PolicyTransactionEffectiveDate, CreateTime 
	From DimPolicy  
	Where TransactionType in ('Cancellation') 
		And PolicyStatus in ('Rescinded') 
	  And cast(JobUpdateTime as date) &gt; ? And cast(JobUpdateTime as date) &lt;= ?

	  UNION 

	Select pp.PolicyID, pp.PolicyNumber ,'NonRenewal' as TransactionType, cp.PolicyTransactionEffectiveDate, cp.EventUpdateTime CreateTime 
	From DimPolicy cp inner join DimPolicy pp on cp.PreviousPolicyNumber  = pp.PolicyNumber
	Where cp.EventName = 'SendNonRenewalDocuments'
	and cp.TransactionType = 'Renewal'
	And pp.MostRecentModel = 1 	  
	And (cast(cp.EventUpdateTime as date) &gt; ? And cast(cp.EventUpdateTime as date) &lt;= ?)

	Union 

	--creating dummy rienstatement for renewed policies for which alert is sent before 25 days
	Select PolicyID, PolicyNumber, 'Dummy Reinstatement' as TransactionType, PolicyTransactionEffectiveDate ,CreateTime 
	From DimPolicy  
	Where TransactionType in ('Renewal') 
	and BaseState ='MI'
	And PolicyStatus in ('Bound') and eventname='SendNonRenewalDocuments'
	And cast(JobCloseDate as date) &gt; ? And cast(JobCloseDate as date) &lt;= ?
)


,PreviousCancellationDate as 
(
	Select * From
	(
		Select qp.PolicyID, CancellationDate, Row_Number() over (partition by qp.PolicyID order by dp.PolicyTransactionEffectiveDate desc, dp.CreateTime desc) as rk
		From QualifiedPol qp Left join DimPolicy dp 
				on dp.PolicyNumber = qp.PolicyNumber 
				And dp.TransactionType = 'Cancellation' 
				And dp.PolicyStatus='Bound' 
				And dp.PolicyTransactionEffectiveDate &lt;= qp.PolicyTransactionEffectiveDate 
				And dp.CreateTime &lt; qp.CreateTime
		Where qp.TransactionType = 'Reinstatement'
	) qry
Where rk=1
)
--Expiration Date for the case if both policynumber and previouspolicynumber are same
,PrevoiusExpirationDate1 as(
select Policynumber,effectivedate,expirationdate from (
Select Policynumber,effectivedate,expirationdate,jobupdatetime,dense_rank() over (partition by policynumber order by jobupdatetime desc) rank2  from (
select policynumber ,effectivedate,expirationdate,jobupdatetime ,dense_rank() over (partition by policynumber order by effectivedate desc) rank1  from(
select distinct policynumber,effectivedate,expirationdate,JobUpdateTime from dimpolicy where policynumber is not null
) m ) n where n.rank1=2 
)  v where v.rank2=1
) 

--Expiration date for the case if policynumber&lt;&gt; prevoiuspolicynumber
,PrevoiusExpirationDate2 as(
Select Policynumber,effectivedate,expirationdate from (
select policynumber ,effectivedate,expirationdate,JobUpdateTime, dense_rank() over (partition by policynumber order by JobUpdateTime desc) rank  from(
select distinct policynumber,effectivedate,expirationdate,JobUpdateTime 
from dimpolicy where policynumber in (select previouspolicynumber from dimpolicy where policynumber&lt;&gt;PreviousPolicyNumber) 
) m where policynumber is not null) n where n.rank=1 
) 


Select  
 dp.BaseState as State, 
 dp.PolicyNumber,
 dp.PolicyID
,'99' as StateCode
,'08' as RTC
,Case qp.TransactionType 
	When 'Cancellation' then '1'
	When 'Reinstatement' then '2'
	When 'NonRenewal' then '3'
	When 'Dummy Reinstatement' then '2'
	Else '0' end as RC_IDCode --CancelReinstateIDCode

,Case When qp.TransactionType ='Reinstatement' And qp.PolicyTransactionEffectiveDate = dp.EffectiveDate then '1' 
      When qp.TransactionType ='Reinstatement' And qp.PolicyTransactionEffectiveDate != dp.EffectiveDate then '2'
	  When qp.TransactionType ='Dummy Reinstatement' then '3'
 else '0' end as RT_Code -- ReinstateTypeCode

,Left(isnull(PrimaryInsuredName,'') + Replicate(' ',90),90) AS PrimaryInsuredName
 
,Left(Rtrim(ISNULL(AddressLine1,'') + ' ' + ISNULL(AddressLine2,'')) + ', ' + ISNULL(City,'') + ', ' + ISNULL(dp.State,'') + ', ' + ISNULL(PostalCode,'') + Replicate(' ',90),90) AddressLine1

,right('000000'+ isnull(Replace(convert(date, IIF(qp.TransactionType ='NonRenewal', qp.CreateTime, IIF(qp.TransactionType ='Cancellation', dp.JobCreateTime, dp.JobCloseDate)),2),'-',''),''),6) as CancelProcessDate

,format(Rank() over (partition by dp.PolicyNumber  order by  qp.PolicyTransactionEffectiveDate, qp.CreateTime),'00') as Trans_Sequence

,Case When DP.STATE = 'MI' and qp.TransactionType &lt;&gt;'Dummy Reinstatement' then '00'
     -- When DP.STATE = 'IN' And qp.TransactionType = 'Reinstatement' Then '01' 
	  When ds.IsNCCI = 1  And qp.TransactionType = 'Reinstatement' Then '01' 
	  when qp.TransactionType ='Dummy Reinstatement' then '01'
	  Else '00' end as RR_Code	--Reason Reinstate Code

,Case When qp.TransactionType='Reinstatement' And dp.TransactionType = 'Reinstatement' then right('000000'+isnull(Replace(convert(date,re.CancellationDate,2),'-',''),''),6) 
      When qp.TransactionType='Reinstatement' And dp.TransactionType = 'Cancellation'  then right('000000'+isnull(Replace(convert(date,dp.PolicyTransactionEffectiveDate,2),'-',''),''),6) 
	  when qp.TransactionType ='Dummy Reinstatement' then right('000000'+isnull(Replace(convert(date,coalesce(prev1.ExpirationDate,prev2.ExpirationDate),2),'-',''),''),6)
	  Else Replicate('0',6) end as Corr_Cancel_Date 

,Case qp.TransactionType 
	When 'Cancellation' then right('000000'+isnull(Replace(convert(date,dp.CancellationDate,2),'-',''),''),6) 
	When 'Reinstatement' then right('000000'+isnull(Replace(convert(date,dp.PolicyTransactionEffectiveDate,2),'-',''),''),6) 
	When 'NonRenewal' then right('000000'+isnull(Replace(convert(date,dp.ExpirationDate,2),'-',''),''),6)
	when 'Dummy Reinstatement' then right('000000'+isnull(Replace(convert(date,coalesce(prev1.ExpirationDate,prev2.ExpirationDate),2),'-',''),''),6)
	Else Replicate('0',6) end as CR_EffectiveDate

,IIF(qp.TransactionType ='Cancellation', upper(ltrim(rtrim(dp.CancelReasonName))),'') as CancelReasonName

,IIF(qp.TransactionType ='Cancellation', dp.[RefundMethod],'0') as RefundMethod

,Case When dp.CancellationSource ='insured' then 1
      When dp.CancellationSource ='carrier' then 2
	  Else 3 end as SourceCode
,IIF(qp.TransactionType = 'NonRenewal',iif(dp.basestate='MI', '03','05') ,'00') CancelationCode

From QualifiedPol qp
join DimPolicy dp on qp.PolicyID = dp.PolicyID
Left join PreviousCancellationDate re on re.PolicyID = qp.PolicyID
Left Join PrevoiusExpirationDate1 prev1 On prev1.PolicyNumber=qp.PolicyNumber 
Left Join PrevoiusExpirationDate2 prev2 On prev2.PolicyNumber=dp.PreviousPolicyNumber
--05 OCT - VS - Added join on DimState to use IsNCCI column. This column is replacing all the NCCI States e.g. IN, VA, CT, IL etc.
Left join DimState ds on dp.BaseState = ds.StateAbbr
order by dp.policynumber</property>
