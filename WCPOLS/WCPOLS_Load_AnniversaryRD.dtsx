                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">With CTE_FactWC as
	(
		Select distinct PolicyID, c.AnniversaryDate
		From  FactWCPremiumTransaction f join DimWCCoverage c on c.CoverageID = f.CoverageID 
		Where AnniversaryDate is NOT NULL
	)

Select distinct 
 dp.PolicyId
,Left(dp.PolicyNumber,18) PolicyNumber
,Left(dp.BaseState ,2) as PolicyState
,'99' as StateCode
,'09' as RecordType
,'WC000402' AS EndorsementNumber
,' ' as BureauVersionIdentifier
,Isnull(Right(Convert(varchar(8), cf.AnniversaryDate, 112),6),'000000') AnniversaryRD
,cast(dp.PrimaryInsuredName as varchar(34)) as NameOfInsured
,Isnull(Right(Convert(varchar(8), pf.EndorsementEffectiveDate, 112),6),'000000') EndorsementED

From FactPolicyForm pf 
	Inner join CTE_FactWC cf on pf.policyid = cf.Policyid
	Inner join DimPolicy dp  on pf.PolicyID = dp.PolicyID 
	Inner join DimForm df   on pf.FormID = df.FormID
	Inner join WCPolicyEndorsement pe on pe.PolicyID = dp.PolicyID
	--05 OCT - VS - Added join on DimState to use IsNCCI column. This column is replacing all the NCCI States e.g. IN, VA, CT, IL etc.
	Left join DimState ds on dp.BaseState = ds.StateAbbr

Where dp.PolicyStatus = 'Bound'
--moving renewal deatils to next day file
And (
		(dp.TransactionType in ('Submission', 'Policy Change','Rewrite') And (cast(dP.JobUpdateTime as date) &gt; ? and cast(dp.JobUpdateTime as date) &lt;= ?)) OR
		(dp.TransactionType ='Renewal' and dp.eventname='SendNonRenewalDocuments' and dp.BaseState='MI' And (cast(dP.JobUpdateTime as date) &gt;DATEADD(day, -1, convert(date,?)) and cast(dp.JobUpdateTime as date) &lt;=DATEADD(day, -1, convert(date,?)))) OR
		(cast(dp.JobUpdateTime as date) &gt;? and cast(dp.JobUpdateTime as date) &lt;=? and dp.transactiontype='Renewal' and dp.eventname is null) OR
		(cast(dp.JobUpdateTime as date) &gt;? and cast(dp.JobUpdateTime as date) &lt;=? and dp.transactiontype='Renewal' and dp.eventname='SendNonRenewalDocuments' and ds.IsNCCI = 1 /*p.BaseState = 'IN'*/)
	)
And FormNumber like 'WC 00 04 02%'
And pe.PatternCode = 'WC7ExperienceRatingModFactEndorsementCond'


Order by PolicyNumber</property>
