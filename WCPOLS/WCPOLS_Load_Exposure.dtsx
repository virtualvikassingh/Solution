                <component
                  refId="Package\Sequence Container\Exposure Load\OLE SRC - FWCPT exposure"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="OLE SRC - FWCPT exposure"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">with pol as
(
	Select distinct i.policyid ,i.RiskLocationID ,c.CoverageID, 
		c.ClassCode, i.FactPremiumTransactionID ,po.PolicyNumber ,po.TermNumber
	from FactWCPremiumTransaction i 
		 inner join dimlob lob on i.LOBID=lob.LOBID
		 inner join DimWCCoverage c on c.CoverageID=i.CoverageID
		 inner join dimpolicy po on i.PolicyID=po.PolicyID
		 Left Join DimState ds on po.BaseState = ds.StateAbbr
	 where lob.LOBCode in( 'WorkersCompLine', 'WC7Line')  
		 and i.PublicID not in ( select F.publicid from FactWCPremiumTransaction F
							join DimWCCoverage D on F.CoverageID = D.CoverageID
							where F.PublicID like 'wc7cost%' and D.EffectiveDate = D.ExpirationDate
							)
		 and classcode is not null and i.Charges=1
		 and (
				 (po.transactiontype in ('Submission','Cancellation','Policy Change','Issuance','Rewrite','Audit','Reinstatement') and (cast(Po.JobUpdateTime as date) &gt; ? and cast(Po.JobUpdateTime as date) &lt;= ?)) OR
				 (po.transactiontype ='Renewal' and po.eventname='SendNonRenewalDocuments' and po.BaseState = 'MI' and (cast(po.JobUpdateTime as date) &gt;DATEADD(day, -1, convert(date,?)) and cast(po.JobUpdateTime as date) &lt;=DATEADD(day, -1, convert(date,?)))) OR
				 (cast(po.JobUpdateTime as date) &gt;? and cast(po.JobUpdateTime as date) &lt;=? and po.transactiontype='Renewal' and po.eventname is null) OR
				 (cast(po.JobUpdateTime as date) &gt;? and cast(po.JobUpdateTime as date) &lt;=? and po.transactiontype='Renewal' and po.eventname='SendNonRenewalDocuments'  and ds.IsNCCI = 1 /*p.BaseState = 'IN'*/)
			)
)

,NLI as (
	Select *,row_number() OVER (partition by PolicyID ORDER BY minRiskLocationID) AS NLI
	from (
			Select distinct i.PolicyID, rl.EntityName, min(rl.RiskLocationID) as minRiskLocationID
			from FactWCPremiumTransaction i
				join DimRiskLocation rl on rl.RiskLocationID = i.RiskLocationID
			where i.RiskLocationID not in (-1,-2) 
			group by i.PolicyID, rl.EntityName
		) z
)


,ERL as 
(
	SELECT *,row_number() OVER (partition by PolicyID ORDER BY RiskLocationID) AS ERL
	from (
			SELECT distinct i.PolicyID, rl.EntityName, rl.RiskLocationID,rl.AddressLine1
			from FactWCPremiumTransaction i
				join DimRiskLocation rl on rl.RiskLocationID = i.RiskLocationID
			where i.RiskLocationID not in (-1,-2) 
		) z
)

,trans as 
(
	SELECT p.PolicyID,p.ModelDate, p.PolicyNumber,isnull(p.TermNumber,0) as TermNumber,c.ClassCode,c.ClassCodeDescription,
		c.GoverningLaw, isnull(r.LocationNumber,-2) as LocationNumber,c.EffectiveDate,sum(f.BillingAmount) as BillingAmount 
	from FactWCPremiumTransaction f
		inner join  DimPolicy p on  ModelDate is not null and f.PolicyID=p.PolicyID
		inner join (select distinct PolicyNumber, TermNumber from pol) pol on pol.PolicyNumber = p.PolicyNumber and pol.TermNumber = p.TermNumber
		inner join DimWCCoverage c on c.CoverageID=f.CoverageID and c.classcode is not null
		left join DimRiskLocation r on f.RiskLocationID=r.RiskLocationID
	group by p.PolicyID,p.ModelDate,p.PolicyNumber,c.ClassCode,r.LocationNumber,p.TermNumber,c.ClassCodeDescription,c.EffectiveDate,c.GoverningLaw 
)

,billing as
(
	select PolicyID,ModelDate,PolicyNumber,ClassCode,ClassCodeDescription,LocationNumber,EffectiveDate,GoverningLaw,
	sum(BillingAmount) over (partition by termnumber, PolicyNumber,ClassCode,LocationNumber,ClassCodeDescription,governinglaw,EffectiveDate order by ModelDate asc rows UNBOUNDED PRECEDING) as BillingAmount
	from trans 

 )

, FactSum as 
(
	Select  
	  pol.RiskLocationID,
	  isnull(rl.LocationNumber,-2) as LocationNumber,
	  pol.policyid,
	  case when pol.RiskLocationID in (-1,-2) then '001' else format(NLI.NLI,'000') end as NLI,
	  case when pol.RiskLocationID in (-1,-2) then '00001' else format(ERL.ERL,'00000')end as ERL ,
	  cast(ISNULL(po.PolicyNumber,'')as varchar(18)) as PolicyNumber ,
	  '05' as RecordTypeCode,
	  left(isnull(wc.ClassCode,'0000'),4) as Classification_Code,
	  case wc.IsClassCode when 1  then 
		 case when wc.GoverningLaw='Longshore And Harbor Workers C' then '02'
			  when ds.IsNCCI = 1 and isnull(cast(wc.coalmine as varchar(1)),'0')!='0' then '03'
			  when ds.IsNCCI = 1 and isnull(wc.CoverageName,'')='WC7MaritimeCov' then '09'
			  when  Coalesce(rl.State, js.StateAbbr)  = 'MI' then '01'
		 else '01' end
	  else '00' end  as EA_EC_Code,


	  replace(convert(varchar,wc.EffectiveDate,2),'.','') as EP_EffectiveDate,
	  right(REPLICATE('0',10)+ltrim(replace(str(isnull(iif(wc.ClassCode='0930',0,wc.ActualAdjRate),0),11,4),'.','')),10)  as Manual_Charged_Rate,
	  case when wc.IsClassCode=1 then right(REPLICATE('0',12) +LTRIM(REPLACE(STR(isnull(iif(wc.ClassCode='0930',0,iif(wc.BasisType='Per Capita',WC.Basis*100,WC.Basis)),0),13,0),'.','')),12) 
		   else  REPLICATE('0',12) 
	  end AS Estimated_Exposure_Amount   

	  ,right(REPLICATE('0',10)+ltrim(replace(replace(str(isnull(b.BillingAmount,wc.ActualAmount),11,0),'.',''),'-','')),10)
			as Estimated_Premium_Amount 
	  ,case when
	  (   month(po.EffectiveDate)=month(po.ExpirationDate) 
	  and day(po.EffectiveDate)=day(po.ExpirationDate) 
	  and year(po.ExpirationDate)&gt;year(po.EffectiveDate) )
	  or datediff(DAY,po.EffectiveDate,po.ExpirationDate)&gt;=366
	  then '1'  else '3' END AS Exposure_Period_Code,
	  Coalesce(rl.State, js.StateAbbr,'')  as StateCodeLink, 
	  po.BaseState  State,
	  '00' AS  NLCI,
	  IIF( TransactionType ='Policy Change', Right(Convert(varchar(10),po.EffectiveDate ,112),6), Replicate('0',6)) as P_C_Effective_Date,
	  IIF( TransactionType ='Policy Change', Right(Convert(varchar(10),po.ExpirationDate,112),6), Replicate('0',6)) as P_C_Expiration_Date ,
	  wc.ClassCodeDescription
 
	from pol
		join dimpolicy po on pol.PolicyID=po.PolicyID
		 
		join DimWCCoverage wc on wc.CoverageID = pol.CoverageID  and (wc.classcode != '0077' or wc.Basis!=0)
		left join DimRiskLocation rl on rl.RiskLocationID = pol.RiskLocationID
		left join billing b on pol.PolicyID=b.PolicyID and pol.ClassCode=b.ClassCode and isnull(rl.LocationNumber,-2)=b.LocationNumber and b.ClassCodeDescription=wc.ClassCodeDescription and b.EffectiveDate = wc.EffectiveDate and b.GoverningLaw=wc.GoverningLaw
		left join FactWCPremiumTransaction f on f.FactPremiumTransactionID = pol.FactPremiumTransactionID
		Left Join DimState js on f.JurisdictionState = js.Id
		left join NLI on NLI.PolicyID=pol.PolicyID and NLI.EntityName = rl.EntityName
		left join ERL on ERL.PolicyID=pol.PolicyID and ERL.RiskLocationID = pol.RiskLocationID
		Left Join DimState ds on ISNULL(rl.State, js.StateAbbr) = ds.StateAbbr
	where wc.ClassCode not IN ('0900', '0063','0000','9898','9887','9889') 

)

select distinct f.policyid
       ,f.NLI
	   ,IIF(f.State = 'MI',f.ERL,'00000') ERL
	   ,F.State
	   ,f.PolicyNumber
	   ,f.RecordTypeCode
	   ,f.Classification_Code
	   ,f.EA_EC_Code
	   ,f.Manual_Charged_Rate
	   ,f.EP_EffectiveDate
	   ,f.Estimated_Exposure_Amount
       ,f.Estimated_Premium_Amount                            
	   ,f.Exposure_Period_Code
	   ,f.StateCodeLink
	   ,f.NLCI
	   ,f.P_C_Effective_Date
	   ,f.P_C_Expiration_Date 
	   ,f.ClassCodeDescription
from  FactSum f</property>
