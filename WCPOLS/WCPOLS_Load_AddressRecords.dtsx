                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">;With NLI as 
(
	Select distinct i.PolicyID, rl.RiskLocationID , rl.EntityName , ContactRoleName, Format(NLI,'000') as NLI
	from FactWCPremiumTransaction i
		join DimRiskLocation rl on rl.RiskLocationID = i.RiskLocationID 
		left join DimPolicy p on p.PolicyID=i.PolicyID 
		left join ( 
					Select * , rank() over(partition by policyid order by PolicyContactID ) as NLI  
					from (Select distinct policyid,ContactName, InsuredName,PolicyContactID , ContactRoleName 
							from  PolicyContacts
							where ContactTypeName='Company' and ContactRoleName in ('PolicyAddlNamedInsured','PolicyPriNamedInsured' )
						  ) c1
				   ) c on c.PolicyID = p.PolicyID and (c.ContactName = rl.EntityName or  rl.EntityName = c.InsuredName)
	where i.RiskLocationID not in (-1,-2) and NLI is not null
)

,ERL as 
(
	 Select *, Format(row_number() OVER (partition by PolicyID ORDER BY LocationNumber),'00000') AS ERL
	 from (
			SELECT distinct i.PolicyID, rl.EntityName, rl.RiskLocationID,rl.AddressLine1,rl.LocationNumber
			From FactWCPremiumTransaction i
				join DimRiskLocation rl on rl.RiskLocationID = i.RiskLocationID
				join DimPolicy p on p.PolicyID=i.PolicyID 
			where i.RiskLocationID not in (-1,-2)  
				and (rl.EntityName &lt;&gt; p.PrimaryInsuredName or rl.AddressLine1 &lt;&gt; p.AddressLine1)
			
			UNION 

			SELECT distinct p.PolicyID, p.PrimaryInsuredName,rl.RiskLocationID,p.AddressLine1, isnull(rl.LocationNumber,0) as LocationNumber
			From DimPolicy p 
				join FactWCPremiumTransaction i on p.PolicyID=i.PolicyID and i.RiskLocationID not in (-1,-2) 
				left join DimRiskLocation rl on rl.RiskLocationID = i.RiskLocationID and rl.EntityName &lt;&gt; p.PrimaryInsuredName and rl.AddressLine1 &lt;&gt; p.AddressLine1
			where  rl.RiskLocationID is null and rl.LocationNumber is null
		) z  
)

,AD as
(

 SELECT distinct po.PolicyID, po.AddressLine1, po.AddressLine2, po.AddressLine3, po.City, po.State, po.County, po.country, po.PostalCode,  1 as ATC,
	'001' as NLI,
	'00001' as ERL ,
	po.State as sc, 
	ISNULL(f.NumofEmployees,0) as EmployeeCount, po.phone as p , Po.DateAdded, po. DateUpdated ,  ISNULL(pc.NAICSCode,'') AS NAICSCode 
 from DimPolicy po
	inner join PolicyContacts pc on pc.PolicyID = po.PolicyID and pc.ContactRoleName = 'PolicyPriNamedInsured'
	Left join 
			(
				Select  i.policyid, i.RiskLocationID ,rl.EntityName, rl.AddressLine1 ,sum(c.NumofEmployees) as NumofEmployees
				from FactWCPremiumTransaction i
					inner join dimlob lob on i.LOBID = lob.LOBID
					inner join DimWCCoverage c on c.CoverageID = i.CoverageID  and c.ExpirationDate &gt; c.EffectiveDate 
					inner join DimRiskLocation rl on rl.RiskLocationID = i.RiskLocationID
					left join PolicyContacts pc on pc.PolicyID=i.PolicyID and pc.ContactTypeName = 'Company' and pc.ContactName = rl.EntityName and pc.ContactRoleName = 'PolicyPriNamedInsured' 
				where lob.LOBCode in( 'WorkersCompLine', 'WC7Line') 
					and i.RiskLocationID not in (-1,-2) 
					and isnull(c.CovEmpCostType,'') not in ('USLH','Coal Mine Disease Charge')
					and pc.ContactRoleName = 'PolicyPriNamedInsured'
				group by i.policyid, i.RiskLocationID ,rl.EntityName, rl.AddressLine1
				)  f on f.PolicyID = po.PolicyID and f.EntityName = po.PrimaryInsuredName and f.AddressLine1 = po.AddressLine1
  
 UNION ALL

 SELECT distinct f.PolicyID, rl.AddressLine1,rl.AddressLine2,rl.AddressLine3,rl.City,rl.State,rl.County,rl.country ,rl.PostalCode, 2 as ATC,
	 NLI.NLI as NLI,
	 ERL as ERL,
	 rl.State as sc, 
	 isnull(NumofEmployees,0) as EmployeeCount , null as p,  rl.DateAdded, rl. DateUpdated , isnull(pc.NAICSCode,'') as NAICSCode
 from  DimRiskLocation  rl
	 join 
		(
			Select  i.policyid, i.RiskLocationID , sum(c.NumofEmployees) as NumofEmployees
			from FactWCPremiumTransaction i
			  inner join dimlob lob on i.LOBID = lob.LOBID
			  inner join DimWCCoverage c on c.CoverageID=i.CoverageID  and c.ExpirationDate &gt; c.EffectiveDate 
			where lob.LOBCode in( 'WorkersCompLine', 'WC7Line') and RiskLocationID not in (-1,-2) and isnull(c.CovEmpCostType,'') not in ('USLH','Coal Mine Disease Charge')
			group by i.policyid, i.RiskLocationID  
		)  f   on f.RiskLocationID = rl.RiskLocationID 
	left join dimpolicy p on p.PolicyID=f.PolicyID
	LEFT join PolicyContacts pc on pc.PolicyID=f.PolicyID and  rl.EntityName=pc.ContactName and pc.ContactRoleName in ('PolicyPriNamedInsured','PolicyAddlNamedInsured')  
	left join NLI on NLI.PolicyID=f.PolicyID and NLI.EntityName = rl.EntityName
	left join ERL on ERL.PolicyID=f.PolicyID and ERL.RiskLocationID = f.RiskLocationID 


 UNION ALL

 SELECT distinct  f.PolicyID, ag.AddressLine1,ag.AddressLine2,ag.AddressLine3,ag.City,ag.State,ag.County,ag.country ,ag.PostalCode, 5 as ATC, 
 '999' AS NLI, '99999' as ERL, 'AL' as sc, 0 as EmployeeCount , null as p,Pr.DateAdded, pr. DateUpdated, 
 '' as NAICSCode
 from [dbo].[DimProducer] pr
		inner join 
			(
				SELECT distinct i.policyid, i.ProducerID ,i.AgencyID  
				from FactWCPremiumTransaction i
				  inner join dimlob lob on i.LOBID=lob.LOBID
				where lob.LOBCode in( 'WorkersCompLine', 'WC7Line')
			)  f on f.ProducerID=pr.ProducerID
		join DimAgency ag on ag.AgencyID = f.AgencyID and AgencyName not in ('NA','Unknown','Unassigned')

) 

,AD3 as 
(
 SELECT DISTINCT i.PolicyID, 'PO Box 40767' as AddressLine1, 'Lansing' as City, 'MI' as State, null as County, 
		'United States' as country ,'489017967' as PostalCode, 3 as ATC, '999' AS NLI, '99999' as ERL, 'AL' as sc,
		 0 as EmployeeCount , null as p , isnull(pc.NAICSCode,'') as  NAICSCode
 from FactWCPremiumTransaction i
  inner join dimlob lob on i.LOBID = lob.LOBID and lob.LOBCode in( 'WorkersCompLine', 'WC7Line') 
  left join PolicyContacts pc on pc.PolicyID = i.PolicyID and pc.ContactRoleName = 'PolicyPriNamedInsured' and NAICSCode IS NOT NULL
  
)

