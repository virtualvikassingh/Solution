SELECT DISTINCT 
	ad.PolicyID, 
	cast(dp.PolicyNumber as varchar(18)) as PolicyNumber,
	'03'as record_type_code, 
     cast(ad.ATC as varchar(1)) as Address_Type_Code, 
	 '1'AS Address_Structure_Code, 
	case when upper(isnull(ad.AddressLine1,'')) like 'C/O%' 
			then cast(left(dbo.FN_REMOVE_SPECIAL_CHARACTER(isnull(ad.AddressLine2,'')+isnull(' '+ad.AddressLine3,'')) + replicate(' ',60),60) as varchar(60))
          when upper(isnull(ad.AddressLine2,'')) like 'C/O%' 
            then cast(left(dbo.FN_REMOVE_SPECIAL_CHARACTER(isnull(ad.AddressLine1,'')+isnull(' '+ad.AddressLine3,'')) + replicate(' ',60),60) as varchar(60))
	      else cast(left(dbo.FN_REMOVE_SPECIAL_CHARACTER(isnull(ad.AddressLine1,'')+isnull(' '+ad.AddressLine2,'')+isnull(' '+ad.AddressLine3,''))+ replicate(' ',60),60)as varchar(60))
	end AS address_Street,
	cast(left(isnull(ad.City,'')+replicate(' ',30),30)as varchar(30) ) as address_City,
    cast(isnull(ad.State,'NA')as varchar(2)) as address_state,  
	cast(left(replace(isnull(ad.PostalCode,''),'-','')+replicate(' ',9),9)as varchar(9)) as address_ZipCode, 
	cast(ad.NLI as varchar(3)) as name_linked_identifier , 
	cast(ISNULL(ad.sc,'NA') as varchar(2))as SC , 
	cast(ad.ERL as varchar(5)) as Exposure_Record_Link, 
	cast(format(ad.EmployeeCount,'000000')as varchar(6)) as Number_of_Employee, 
	'00' as NameLinkCounterIdentifier, 
	cast(left(ad.NAICSCode+replicate(' ',6),6) as varchar(6)) as IndustryCode,
	cast(format(isnull(p,0), '0000000000')as varchar(10))as phone, Cast(dp.BaseState as varchar(2))Policy_State,
	IIF( TransactionType ='Policy Change', Right(Convert(varchar(10),dp.EffectiveDate ,112),6), Replicate('0',6)) as PolicyChangeEffectiveDate,
	IIF( TransactionType ='Policy Change', Right(Convert(varchar(10),dp.ExpirationDate,112),6), Replicate('0',6)) as PolicyChangeExpirationDate
from  ad 
	inner join DimPolicy dp on ad.PolicyID = dp.PolicyID
	--05 OCT - VS - Added join on DimState to use IsNCCI column. This column is replacing all the NCCI States e.g. IN, VA, CT, IL etc.
	Left Join DimState ds on ad.State = ds.StateAbbr  
where PolicyNumber is not null 
	and ad.policyid not in (-1,-2) 
	and ad.ERL is not null 
	and ad.NLI is not null 
	and ((cast(dp.JobUpdateTime as date) &gt; ? and cast(dp.JobUpdateTime as date) &lt;= ? and transactiontype in ('Submission','Cancellation','Policy Change','Issuance','Rewrite','Audit','Reinstatement'))  or 
	(cast(dp.JobUpdateTime as date) &gt; DATEADD(day, -1, convert(date,?)) and cast(dp.JobUpdateTime as date) &lt;= DATEADD(day, -1, convert(date,?)) and transactiontype='Renewal' and dp.eventname='SendNonRenewalDocuments' and dp.BaseState = 'MI') or 
	(cast(dp.JobUpdateTime as date) &gt;? and cast(dp.JobUpdateTime as date) &lt;=? and transactiontype='Renewal' and dp.eventname is null) Or
	(cast(dp.JobUpdateTime as date) &gt;? and cast(dp.JobUpdateTime as date) &lt;=? and transactiontype='Renewal' and dp.eventname='SendNonRenewalDocuments'  and ds.IsNCCI = 1 /*and dp.BaseState='IN'*/ ))  

union all     

select distinct ad3.PolicyID, 
       cast(dp.PolicyNumber as varchar(18)) as PolicyNumber,   
      '03'as record_type_code, 
       cast(ad3.ATC as varchar(1)) as Address_Type_Code, 
	   '1'AS Address_Structure_Code, 
	   cast(left(isnull(ad3.AddressLine1,'') + replicate(' ',60),60)as varchar(60)) AS address_Street,
	   cast(left(isnull(ad3.City,'') + replicate(' ',30),30)as varchar(30) ) as address_City,
       cast(isnull(ad3.State,'NA')as varchar(2)) as address_state,  
	   cast(left(isnull(ad3.PostalCode,'') + replicate(' ',9),9)as varchar(9)) as address_ZipCode, 
	   cast(ad3.NLI as varchar(3)) as name_linked_identifier , 
	   cast(ISNULL(ad3.sc,'NA') as varchar(2))as SC , 
	   cast(ad3.ERL as varchar(5)) as Exposure_Record_Link, 
	   cast(format(ad3.EmployeeCount,'000000')as varchar(6)) as Number_of_Employee, 
	   '00' as NameLinkCounterIdentifier, 
	   cast(left(ad3.NAICSCode+replicate(' ',6),6) as varchar(6)) as IndustryCode,
	   cast(format(isnull(p,0), '0000000000')as varchar(10))as phone, Cast(dp.BaseState as varchar(2))Policy_State,
	   IIF( TransactionType ='Policy Change', Right(Convert(varchar(10),dp.EffectiveDate ,112),6), Replicate('0',6)) as PolicyChangeEffectiveDate,
	   IIF( TransactionType ='Policy Change', Right(Convert(varchar(10),dp.ExpirationDate,112),6), Replicate('0',6)) as PolicyChangeExpirationDate
from  ad3
inner join DimPolicy dp on ad3.PolicyID = dp.PolicyID</property>
