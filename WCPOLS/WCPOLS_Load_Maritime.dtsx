                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">with QualiPol
as
(
select distinct w.PolicyID, p.PolicyNumber, p.BaseState, p.PrimaryInsuredName, f.FormNumber, W.EndorsementEffectiveDate EffectiveDate
                ,case p.TransactionType when 'Submission' then 1 when 'Policy Change' then 2 else 0 end as Ind
From FactPolicyForm w
		Inner Join DimPolicy p on w.PolicyID = p.PolicyID
		Inner Join DimForm f on w.formid = f.FormID 
		left join  (select *   ,  row_number() over (partition by policyid order by effectivedate)  as i   from 
                      (select distinct f.PolicyID, wc.EffectiveDate from DimWCCoverage  wc
                       inner join FactWCPremiumTransaction f on wc.CoverageID=f.CoverageID ) c 
					) as e on e.PolicyID=w.PolicyID and e.i=1
	--05 OCT - VS - Added join on DimState to use IsNCCI column. This column is replacing all the NCCI States e.g. IN, VA, CT, IL etc.
	Left join DimState ds on p.BaseState = ds.StateAbbr

	Where f.FormNumber like 'WC 00 02 01 _'
		And f.IsEndorsement = 1
		and (
				(p.TransactionType in ('Submission','Policy Change','Rewrite')  And (cast(P.JobUpdateTime as date) &gt; ? and cast(p.JobUpdateTime as date) &lt;= ?)) OR
				(p.TransactionType ='Renewal' and p.eventname='SendNonRenewalDocuments' and p.BaseState='MI' And (cast(P.JobUpdateTime as date) &gt;DATEADD(day, -1, convert(date,?)) and cast(p.JobUpdateTime as date) &lt;=DATEADD(day, -1, convert(date,?)))) OR
				(cast(p.JobUpdateTime as date) &gt;? and cast(p.JobUpdateTime as date) &lt;=? and p.transactiontype='Renewal' and p.eventname is null) OR
				(cast(p.JobUpdateTime as date) &gt;? and cast(p.JobUpdateTime as date) &lt;=? and p.transactiontype='Renewal' and p.eventname='SendNonRenewalDocuments' and ds.IsNCCI = 1 /*p.BaseState = 'IN'*/)
		)
		and p.PolicyStatus = 'Bound' 
)


SELECT 
 qp.PolicyID
, left(isnull(qp.PolicyNumber,'')+replicate(' ',18),18) as PolicyNumber
, qp.BaseState as PolicyState
, left(isnull(qp.PrimaryInsuredName,'')+replicate(' ',34), 34) as NameInsured
, '21' as RecordType
, 'WC000201' as Endorsment_Num
, isnull(substring(qp.formnumber,13,1),' ')  as BureauVI
, right(replicate('0',10)+isnull(c.[MaritimeAccidentLimit],''),10) as MeritimeAccidentLimit
, right(replicate('0',10)+isnull(c.[MaritimeDiseaseLimit],''),10) as MeritimeDiseaseLimit
, right(replicate('0',10)+replace(isnull(substring(c.[MaritimePremiumAmount],1,CHARINDEX('.',c.[MaritimePremiumAmount])),''),'.',''),10) AS MeritimePremiumAmount
, left(isnull(c.[MaritimeWorkDescription],'')+replicate(' ',120),120) as MaritimeWorkDescription
,format(row_number() over (partition by qp.policyid order by c.effectivedate),'00') as Endorse_SeqNum
,right(replace(convert(date,c.EffectiveDate,2),'-',''),6)    as effect_date

from  QualiPol  qp 
inner join ( SELECT PolicyID, MaritimeAccidentLimit,MaritimeDiseaseLimit,MaritimePremiumAmount,MaritimeWorkDescription,EffectiveDate, DataEffectiveDate
FROM (select distinct f.PolicyID, c.MaritimeAccidentLimit,c.MaritimeDiseaseLimit,c.MaritimePremiumAmount,c.MaritimeWorkDescription,c.EffectiveDate,DENSE_RANK() OVER 
(PARTITION BY POLICYID ORDER BY DATAEFFECTIVEDATE DESC) RANK,
c.DataEffectiveDate from DimWCCoverage c join FactWCPremiumTransaction f on f.CoverageID=c.CoverageID and c.IsClassCode=1 and c.CoverageName='WC7MaritimeCov' ) M WHERE M.rank =1
) c on qp.PolicyID=c.PolicyID</property>
