from [dbo].[Stage_WCPOLS_USLH] where x=1 

union all

select C.policyid, C.PolicyNumber, c.PolicyState,C.PolicyName,c.EffectiveDate,c.RecordType,c.EndorsmentNum,c.BureauVI
     , c.B +cast(isnull(s.enstate,'00') as varchar(2))+right('0000'+isnull(s.Factor,''),4)  as B  , s.y ,s.x  from trans as c
 join [dbo].[Stage_WCPOLS_USLH] S  on S.policyid=C.policyid and s.y=c.y  AND S.x=c.x+1 
