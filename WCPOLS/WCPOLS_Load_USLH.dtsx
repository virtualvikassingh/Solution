                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">With QualiPol
as
(
select distinct w.PolicyID,p.PolicyNumber,p.BaseState,p.PrimaryInsuredName,f.FormNumber, W.EndorsementEffectiveDate EffectiveDate  
From FactPolicyForm w
		Inner Join DimPolicy p on w.PolicyID = p.PolicyID
		Inner Join DimForm f on w.formid = f.FormID 
			--05 OCT - VS - Added join on DimState to use IsNCCI column. This column is replacing all the NCCI States e.g. IN, VA, CT, IL etc.
		Left join DimState ds on p.BaseState = ds.StateAbbr
		left join  (select *,  row_number() over (partition by policyid order by effectivedate)  as RowNumber  
					from 
                      (
						Select distinct f.PolicyID, wc.EffectiveDate  
						from DimWCCoverage  wc Inner join FactWCPremiumTransaction f on wc.CoverageID = f.CoverageID ) c 
					) as e on e.PolicyID = w.PolicyID and e.RowNumber = 1
	Where f.FormNumber like 'WC 00 01 06 _'
		And f.IsEndorsement = 1
		And (
			(p.TransactionType in ('Submission','Policy Change','Rewrite') And (cast(P.JobUpdateTime as date) &gt; ? and cast(p.JobUpdateTime as date) &lt;= ?)) OR
			(p.TransactionType ='Renewal'  and p.eventname='SendNonRenewalDocuments' and p.BaseState='MI' And (cast(P.JobUpdateTime as date) &gt;DATEADD(day, -1, convert(date,?)) and cast(p.JobUpdateTime as date) &lt;=DATEADD(day, -1, convert(date,?)))) OR
			(cast(p.JobUpdateTime as date) &gt;? and cast(p.JobUpdateTime as date) &lt;=? and p.transactiontype='Renewal' and p.eventname is null) OR
			(cast(p.JobUpdateTime as date) &gt;? and cast(p.JobUpdateTime as date) &lt;=? and p.transactiontype='Renewal' and p.eventname='SendNonRenewalDocuments' and ds.IsNCCI = 1 /*p.BaseState = 'IN'*/)
		)
		and p.PolicyStatus = 'Bound'
		
)

select *,ROW_NUMBER() over(partition by PolicyID, Col_Spliter_X order by ScheduleNumber ) as Row_Spliter_Y
 from (
		select 
		 qp.PolicyID
		,qp.PolicyNumber
		,cast(qp.Basestate  as varchar(50)) as PolicyState
		,cast(qp.PrimaryInsuredName as varchar(34)) as PrimaryInsuredName
		,qp.EffectiveDate
		,'19' as RecordType
		,'WC000106' as Endorsment_Num
		, substring(formnumber,13,1)  as BureauVI
		,en.State AS EnState
		,EndorsementValue2
		,right('0000'+replace(ltrim(str(EndorsementValue2,10,1)),'.',''),4) as Factor
		,en.ScheduleNumber 
		,ntile(25) over (partition by qp.PolicyID order by en.ScheduleNumber ) as Col_Spliter_X
		from  WCPolicyEndorsement en inner join QualiPol  qp on qp.PolicyID = en.PolicyID 
		where  PatternCode='WC7LongshoreAndHarborWorkersCompensationActRatCond' 

) AS SourceTable</property>
