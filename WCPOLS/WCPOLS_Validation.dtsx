                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">With CTE_Message as 
(
	Select ID, PolicyPeriodID, EventName, SendTime, SendLockedTime, CreationTime, 1 BeanVersion 
	from pc_message  
	Where EventName in ('SendNonRenewalDocuments')
)


 SELECT  
	 pp.ID as PolicyGWRowNumber
	 ,cast(upper(ltrim(rtrim(pp.PolicyNumber))) as varchar(18)) as PolicyNumber
	 ,Case When job.Name in  ('Submission','Rewrite') then '01'
			When job.Name = 'Renewal' then '02'
		  when job.Name in ('Cancellation','Reinstatement')  then '05'
		  when job.Name = 'Policy Change' then '08'
		  else '00' End as TransactionCode
	,jb.UpdateTime
	,'00' as JoinType

from dbo.pc_policyperiod  pp
	inner Join dbo.pc_job jb On pp.JobID = jb.ID
	inner Join dbo.pctl_job job On jb.Subtype = job.ID
	inner join dbo.pctl_policyperiodstatus pps on pp.Status=pps.ID
	inner Join dbo.pc_policy po With(NoLock) On po.ID = pp.PolicyID
	inner join dbo.pctl_jurisdiction jup on pp.BaseState = jup.ID
	inner join dbo.pc_policyline pl on pp.id=pl.branchid AND pp.EditEffectiveDate &gt;= COALESCE(pl.EFFECTIVEDATE,'1900-01-01') and pp.EditEffectiveDate &lt; COALESCE(pl.EXPIRATIONDATE,'9999-12-31')

where job.NAME in ( 'Submission','Renewal','Reinstatement','Rewrite')
	and pps.name='Bound'  
	and po.IssueDate is not null
	and cast(jb.UpdateTime as date) &gt;  ? and cast(jb.UpdateTime as date) &lt;= ?
	and Left(jup.TYPECODE,2) in ('MI','IN') --BaseState
	and pl.patterncode in ('WorkersCompLine','WC7Line') 

UNION--------NonRenewal

SELECT  Distinct 
	pp.ID as PolicyGWRowNumber
	,cast(upper(ltrim(rtrim(pp.PolicyNumber))) as varchar(18)) as PolicyNumber
	,'05' as TransactionCode
	,jb.UpdateTime
	,'00' as JoinType	
	 
FROM dbo.pc_policyperiod  cp  
	inner join dbo.pc_policyperiod  pp on cp.BasedOnID = pp.id
	inner Join dbo.pc_job jb On cp.JobID = jb.ID
	inner Join dbo.pctl_job job On jb.Subtype = job.ID
	inner join dbo.pctl_policyperiodstatus pps on cp.Status = pps.ID
	inner Join dbo.pc_policy po With(NoLock) On po.ID = cp.PolicyID
	Inner join CTE_Message mes on cp.id = mes.PolicyPeriodID 
	inner join dbo.pctl_jurisdiction jup on cp.BaseState = jup.ID
	inner join dbo.pc_policyline pl on cp.id = pl.branchid AND cp.EditEffectiveDate &gt;= COALESCE(pl.EFFECTIVEDATE,'1900-01-01') and cp.EditEffectiveDate &lt; COALESCE(pl.EXPIRATIONDATE,'9999-12-31')
WHERE  job.NAME in ('Renewal')
	and mes.EventName in ('SendNonRenewalDocuments')
	and po.IssueDate is not null
	and Left(jup.TYPECODE,2) in ('MI','IN') --BaseState
	and pl.patterncode in ('WorkersCompLine','WC7Line')
	--and cp.policynumber = 'ARP12000531001' 
	and cast( mes.CreationTime as date) &gt;  ? and cast( mes.CreationTime as date) &lt;= ?


UNION
--Get 'Quoted','Cancelling','Bound' Cancellation base on JobCreateTime, will only report once
SELECT  
  pp.ID as PolicyGWRowNumber
 ,cast(upper(ltrim(rtrim(pp.PolicyNumber))) as varchar(18)) as PolicyNumber
 ,Case When job.Name = 'Submission' then '01'
		When job.Name = 'Renewal' then '02'
      when job.Name in ('Cancellation','Reinstatement')  then '05'
	  when job.Name = 'Policy Change' then '08'
	  else '00' End as TransactionCode
,jb.UpdateTime
,'00' as JoinType

from dbo.pc_policyperiod  pp
	inner Join dbo.pc_job jb On pp.JobID = jb.ID
	inner Join dbo.pctl_job job On jb.Subtype = job.ID
	inner join dbo.pctl_policyperiodstatus pps on pp.Status=pps.ID
	inner Join dbo.pc_policy po With(NoLock) On po.ID = pp.PolicyID
	inner join dbo.pctl_jurisdiction jup on pp.BaseState = jup.ID
	inner join dbo.pc_policyline pl on pp.id=pl.branchid AND pp.EditEffectiveDate &gt;= COALESCE(pl.EFFECTIVEDATE,'1900-01-01') and pp.EditEffectiveDate &lt; COALESCE(pl.EXPIRATIONDATE,'9999-12-31')
where job.NAME in ('Cancellation')
	and pps.name in ('Canceling','Bound','Rescinded')  
	--and po.IssueDate is not null
	and cast(jb.CreateTime as date) &gt;  ? and cast(jb.CreateTime as date) &lt;= ?
	and Left(jup.TYPECODE,2) in ('MI','IN') --BaseState
	and pl.patterncode in ('WorkersCompLine','WC7Line') 

UNION
--Get 'Withdrawn','Rescinded' Cancellation base on JobUpdateTime, will only get those which change to 'Withdrawn','Rescinded' not in the same day as JobCreateTime, consider them as reinstatement in WCPOLS
SELECT  
  pp.ID as PolicyGWRowNumber
 ,cast(upper(ltrim(rtrim(pp.PolicyNumber))) as varchar(18)) as PolicyNumber
 ,Case When job.Name = 'Submission' then '01'
		When job.Name = 'Renewal' then '02'
      when job.Name in ('Cancellation','Reinstatement')  then '05'
	  when job.Name = 'Policy Change' then '08'
	  else '00' End as TransactionCode
,jb.UpdateTime
,'00' as JoinType
from dbo.pc_policyperiod  pp
	inner Join dbo.pc_job jb On pp.JobID = jb.ID
	inner Join dbo.pctl_job job On jb.Subtype = job.ID
	inner join dbo.pctl_policyperiodstatus pps on pp.Status=pps.ID
	inner Join dbo.pc_policy po With(NoLock) On po.ID = pp.PolicyID
	inner join dbo.pctl_jurisdiction jup on pp.BaseState = jup.ID
	inner join dbo.pc_policyline pl on pp.id=pl.branchid AND pp.EditEffectiveDate &gt;= COALESCE(pl.EFFECTIVEDATE,'1900-01-01') and pp.EditEffectiveDate &lt; COALESCE(pl.EXPIRATIONDATE,'9999-12-31')
where job.NAME in ('Cancellation')
	and pps.name in ('Rescinded')  
	and cast(jb.CreateTime as date) &lt; cast(jb.UpdateTime as date) and cast(jb.UpdateTime as date) &gt;  ? and cast(jb.UpdateTime as date) &lt;= ?
	and Left(jup.TYPECODE,2) in ('MI','IN') --BaseState
	and pl.patterncode in ('WorkersCompLine','WC7Line') 

union 

SELECT PolicyGWRowNumber ,PolicyNumber ,TransactionCode ,UpdateTime
,'00' as JoinType
from (
		select  
		pp.ID as PolicyGWRowNumber
		,cast(upper(ltrim(rtrim(pp.PolicyNumber))) as varchar(18)) as PolicyNumber
		,Case when Left(jup.TYPECODE,2) = 'IN' then '08'
			 when Left(jup.TYPECODE,2) = 'MI' and ct.TypeCode in ('Rating','Additional') then '08'
			 when Left(jup.TYPECODE,2) = 'MI' and ct.TypeCode = 'NonRating' then '10'
			 when Left(jup.TYPECODE,2) = 'MI' and ct.TypeCode is null then '10'
			 else '00' 
		End as TransactionCode
		,jb.UpdateTime
		,row_number() over (partition by pp.PolicyNumber order by Cast(pp.editeffectivedate As DateTime) desc ,pp.ModelDate desc) as rn
		from dbo.pc_policyperiod  pp
			inner Join dbo.pc_job jb On pp.JobID = jb.ID
			inner Join dbo.pctl_job job On jb.Subtype = job.ID
			inner join dbo.pctl_policyperiodstatus pps on pp.Status=pps.ID
			inner Join dbo.pc_policy po With(NoLock) On po.ID = pp.PolicyID
			inner join dbo.pctl_jurisdiction jup on pp.BaseState = jup.ID
			inner join dbo.pc_policyline pl on pp.id=pl.branchid AND pp.EditEffectiveDate &gt;= COALESCE(pl.EFFECTIVEDATE,'1900-01-01') and pp.EditEffectiveDate &lt; COALESCE(pl.EXPIRATIONDATE,'9999-12-31')
			left join dbo.pctl_wcpolicychangetypeext ct on jb.WCPolicyChangeTypeExt=ct.ID
		where job.NAME in ('Policy Change')
			and pps.name='Bound'  
			and po.IssueDate is not null
			and cast(jb.UpdateTime as date) &gt;  ? and cast(jb.UpdateTime as date) &lt;= ?
			and Left(jup.TYPECODE,2) in ('MI','IN') --BaseState
			and pl.patterncode in ('WorkersCompLine','WC7Line') 
	) qry
where rn = 1

union --Z1


SELECT  
 pp.ID as PolicyGWRowNumber
,cast(upper(ltrim(rtrim(pp.PolicyNumber))) as varchar(18)) as PolicyNumber
,'17' as TransactionCode
,ci.UpdateTime
,'Z1' as JoinType

from (select ID from (Select ID, ROW_NUMBER() Over (Partition  by PolicyNumber order by CreateTime desc) RowNumber From dbo.pc_policyperiod where ModelDate is NOT NULL) z where RowNumber = 1) ppa
	inner join dbo.pc_policyperiod  pp on pp.ID = ppa.ID
	inner Join dbo.pc_job jb On pp.JobID = jb.ID
