                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">With CTE_MI_SoleProprietorship as  --For endorsements, we will only exclude the transaction (in Exclusion we exclude the entire Policy)
(
	Select Distinct w.FactPolicyFormID,w.FormID,f.FormNumber,d.PolicyID
	From FactPolicyForm w
		Inner Join DimPolicy d on w.PolicyID = d.PolicyID
		Inner Join DimForm f on w.formid = f.FormID 
	Where   d.BaseState ='MI' 
	And	 d.OrganizationType = 'Sole proprietorship' 
	And	 Left(Replace(FormNumber,' ',''),8) = 'WC000308' 
),

Exclusion As (
	Select Distinct  p.PolicyId
 
	From WCPolicyEndorsement w
	 Inner Join DimPolicy p on w.PolicyID = p.PolicyID and p.policyid not in (select PolicyID from CTE_MI_SoleProprietorship)
	 Inner Join FactPolicyForm f on f.PolicyID = p.PolicyID
	 Inner Join DimForm d on f.FormID = d.FormID
	 	--05 OCT - VS - Added join on DimState to use IsNCCI column. This column is replacing all the NCCI States e.g. IN, VA, CT, IL etc.
	Left join DimState ds on p.BaseState = ds.StateAbbr

	Where w.PatternCode = 'WC7PartnersOfficersAndOthersExclEndorsementExcl'
	and FormNumber Like 'WC 00 03 08%'
	AND (
			(p.TransactionType in ('Submission', 'Policy Change','Rewrite') And (cast(p.JobUpdateTime as date) &gt; ? and cast(p.JobUpdateTime as date) &lt;= ?)) OR
			(p.TransactionType ='Renewal' and p.eventname='SendNonRenewalDocuments' and p.BaseState='MI' And (cast(p.JobUpdateTime as date) &gt;DATEADD(day, -1, convert(date,?)) and cast(p.JobUpdateTime as date) &lt;=DATEADD(day, -1, convert(date,?)))) OR
			(cast(p.JobUpdateTime as date) &gt;? and cast(p.JobUpdateTime as date) &lt;=? and p.transactiontype='Renewal' and p.eventname is null) OR
			(cast(p.JobUpdateTime as date) &gt;? and cast(p.JobUpdateTime as date) &lt;=? and p.transactiontype='Renewal' and p.eventname='SendNonRenewalDocuments'  and ds.IsNCCI = 1 /*p.BaseState = 'IN'*/)
		)	
	and PolicyStatus = 'Bound'
	

),


T as 
(
	Select Distinct 
		IsEndorsement,
		d.BaseState as PolicyState,  
		d.PolicyNumber, 
		f.FormNumberCode,
		f.FormNumber,	
		d.PolicyId,
		Replicate (' ',11) CarrierVersionIdentifier,
		Left(Replace(FormNumber,' ',''),8) EndorsementNumber,
		IIF(Right(FormNumber,1) NOT LIKE '%[0-9]%' , Right(FormNumber,1), ' ') BureauVersionIdentifier, 
		IIF(Right(Left(Replace(FormNumber,' ',''),4),2)	 in ('00','99','89'), '00',Right(Left(Replace(FormNumber,' ',''),4),2))StateCode,
		Dense_Rank() Over(Partition By PolicyNumber Order by IIF(Right(Left(Replace(FormNumber,' ',''),4),2)	 in ('00','99','89'), '00',Right(Left(Replace(FormNumber,' ',''),4),2))) RNK,
		Cast(d.PolicyID as varchar(10)) + Cast(Dense_Rank() Over(Partition By PolicyNumber Order by IIF(Right(Left(Replace(FormNumber,' ',''),4),2)	 in ('00','99','89'), '00',Right(Left(Replace(FormNumber,' ',''),4),2))) as varchar(3)) PublicId,
		IIF( TransactionType ='Policy Change', Right(Convert(varchar(10),d.EffectiveDate ,112),6), Replicate('0',6)) as PolicyChangeEffectiveDate,
		IIF( TransactionType ='Policy Change', Right(Convert(varchar(10),d.ExpirationDate,112),6), Replicate('0',6)) as PolicyChangeExpirationDate
			
	From FactPolicyForm w
		Inner Join DimPolicy d on w.PolicyID = d.PolicyID --AND d.BaseState = 'IN'
		Inner Join DimForm f on w.formid = f.FormID 
			--05 OCT - VS - Added join on DimState to use IsNCCI column. This column is replacing all the NCCI States e.g. IN, VA, CT, IL etc.
	Left join DimState ds on d.BaseState = ds.StateAbbr
	Where FormNumberCode like 'wc%'
		And f.IsEndorsement = 1 
		And Right(Left(Replace(FormNumber,' ',''),4),2)	 LIKE '%[0-9]%' 
		And (
			 (d.TransactionType in ('Submission','Policy Change','Rewrite') And ( cast(d.JobUpdateTime as date) &gt; ? and cast(d.JobUpdateTime as date) &lt;= ?)	) OR
			 (d.TransactionType ='Renewal' and d.eventname='SendNonRenewalDocuments' and d.BaseState='MI' And (cast(d.JobUpdateTime as date) &gt;DATEADD(day, -1, convert(date,?)) and cast(d.JobUpdateTime as date) &lt;=DATEADD(day, -1, convert(date,?)))) OR
			 (cast(d.JobUpdateTime as date) &gt;? and cast(d.JobUpdateTime as date) &lt;=? and d.transactiontype='Renewal' and d.eventname is null) OR
			 (cast(d.JobUpdateTime as date) &gt;? and cast(d.JobUpdateTime as date) &lt;=? and d.transactiontype='Renewal' and d.eventname='SendNonRenewalDocuments' 	 and ds.IsNCCI = 1 /*p.BaseState = 'IN'*/)
			)
		and PolicyStatus = 'Bound' 	
		 and ds.IsNCCI = 1 
	    	

UNION ALL

	Select Distinct 
		IsEndorsement,
		d.BaseState as PolicyState,  
		d.PolicyNumber, 
		f.FormNumberCode,
		f.FormNumber,	
		d.PolicyId,
		Replicate (' ',11) CarrierVersionIdentifier,
		Left(Replace(FormNumber,' ',''),8) EndorsementNumber,
		IIF(Right(FormNumber,1) NOT LIKE '%[0-9]%' , Right(FormNumber,1), ' ') BureauVersionIdentifier, 
		IIF(Right(Left(Replace(FormNumber,' ',''),4),2)	 in ('00','99','89'), '00',Right(Left(Replace(FormNumber,' ',''),4),2))StateCode,
		Dense_Rank() Over(Partition By PolicyNumber Order by IIF(Right(Left(Replace(FormNumber,' ',''),4),2)	 in ('00','99','89'), '00',Right(Left(Replace(FormNumber,' ',''),4),2))) RNK,
		Cast(d.PolicyID as varchar(10)) + Cast(Dense_Rank() Over(Partition By PolicyNumber Order by IIF(Right(Left(Replace(FormNumber,' ',''),4),2)	 in ('00','99','89'), '00',Right(Left(Replace(FormNumber,' ',''),4),2))) as varchar(3)) PublicId,
		IIF( TransactionType ='Policy Change', Right(Convert(varchar(10),d.EffectiveDate ,112),6), Replicate('0',6)) as PolicyChangeEffectiveDate,
		IIF( TransactionType ='Policy Change', Right(Convert(varchar(10),d.ExpirationDate,112),6), Replicate('0',6)) as PolicyChangeExpirationDate
			
	From FactPolicyForm w
		Inner Join DimPolicy d on w.PolicyID = d.PolicyID
		Inner Join DimForm f on w.formid = f.FormID 
			--05 OCT - VS - Added join on DimState to use IsNCCI column. This column is replacing all the NCCI States e.g. IN, VA, CT, IL etc.
	Left join DimState ds on d.BaseState = ds.StateAbbr
	Where FormNumberCode like 'wc%'
		And f.IsEndorsement = 1
		And d.BaseState = 'MI'
		And Right(Left(Replace(FormNumber,' ',''),4),2)	 LIKE '%[0-9]%' 
		And (
			(d.TransactionType in ('Submission','Policy Change','Rewrite') And (cast(d.JobUpdateTime as date) &gt; ? and cast(d.JobUpdateTime as date) &lt;= ?)) OR
			(d.TransactionType='Renewal' and d.eventname='SendNonRenewalDocuments' and d.BaseState='MI' And (cast(d.JobUpdateTime as date) &gt;DATEADD(day, -1, convert(date,?)) and cast(d.JobUpdateTime as date) &lt;=DATEADD(day, -1, convert(date,?)))) OR
			(cast(d.JobUpdateTime as date) &gt;? and cast(d.JobUpdateTime as date) &lt;=? and d.transactiontype='Renewal' and d.eventname is null) OR
			(cast(d.JobUpdateTime as date) &gt;? and cast(d.JobUpdateTime as date) &lt;=? and d.transactiontype='Renewal' and d.eventname='SendNonRenewalDocuments'	 and ds.IsNCCI = 1 /*p.BaseState = 'IN'*/)
		)
		and PolicyStatus = 'Bound'
		And Left(Replace(FormNumber,' ',''),8) &lt;&gt; 'WC000310' 
		And FormNumberCode NOT LIKE 'WC99%' 
		and w.FactPolicyFormID not in (SELECT FactPolicyFormID from CTE_MI_SoleProprietorship)		
 
 )

 Select T.* from T where EndorsementNumber &lt;&gt;'WC000308'
 Union all
 select T.*  from T where EndorsementNumber='WC000308' and policyid in   (select PolicyId from exclusion)</property>
