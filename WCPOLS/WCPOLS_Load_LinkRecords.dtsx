SELECT   fpt.PolicyID, dp.GWRowNumber,
	'19968' as CompanyCode,
	Cast(dp.PolicyNumber as varchar(18)) as PolicyNumber,
	Cast(isnull(replace(convert(varchar, dp.EffectiveDate,2),'.',''),'      ') as varchar(6)) as EffectiveDate,
	Case when PolicyStatus ='Canceling' then 
	Right('000'+ isnull(SUBSTRING(cast(datepart(YY,dp.JobCreateTime)as varchar(4)),3,4),''),2) + right('000'+ isnull(cast(datepart(DY,dp.JobCreateTime) as varchar(3)),''),3) 
	else Right('000'+ isnull(SUBSTRING(cast(datepart(YY,dp.JobCloseDate)as varchar(4)),3,4),''),2) + right('000'+ isnull(cast(datepart(DY,dp.JobCloseDate) as varchar(3)),''),3) end  as DataEffectiveDate ,
	Case When dp.TransactionType = 'Submission' then '01'
	  when dp.TransactionType in ('Renewal')  then '02'
      when dp.TransactionType in ('Cancellation','Reinstatement')  then '05'
	  when dp.TransactionType = 'Policy Change' then '08'	 
	Else '00' End as TransactionCode,
	Cast(isnull(dp.BaseState,'  ') as varchar(2)) as State,
	'00'  as JoinType 
FROM FactPolicyTransactionChange fpt inner join DimPolicy dp on fpt.PolicyID = dp.PolicyID
join DimLOB L on fpt.LOBID=l.LOBID
where TransactionType in( 'Cancellation') 
and PolicyStatus in ('Canceling','Bound','Rescinded')
and cast(dp.JobCreateTime as date) &gt; ? and cast(dp.JobCreateTime as date) &lt;= ?
--and dp.BaseState in ('MI','IN')
and L.LOBCode in ( 'WorkersCompLine','WC7Line') 

