                <component
                  refId="Package\Sequence Container\DFT Load Link data\OLE SRC - FactPolicyTransactionChange"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="OLE SRC - FactPolicyTransactionChange"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">With CTE AS
(

	SELECT count(*) Ratings_count , PolicyNumber  ,convert(date,dp.JobUpdateTime) JobUpdateTime
	FROM FactPolicyTransactionChange fpt inner join DimPolicy dp on fpt.PolicyID = dp.PolicyID
		join DimLOB L on fpt.LOBID=l.LOBID

	where TransactionType in ('Policy Change') 
		and PolicyStatus = 'Bound' 
		and dp.IssueDate is not null 
		and (cast(dp.JobUpdateTime as date) &gt; ? and cast(dp.JobUpdateTime as date) &lt;= ?) 
