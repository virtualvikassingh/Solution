SELECT   fpt.PolicyID, dp.GWRowNumber,
	'19968' as CompanyCode,
	Cast(dp.PreviousPolicyNumber as varchar(18)) as PolicyNumber,
	Cast(isnull(replace(convert(varchar,coalesce(prev1.EffectiveDate,prev2.EffectiveDate),2),'.',''),'      ') as varchar(6)) as EffectiveDate,
	Right('000'+ isnull(SUBSTRING(cast(datepart(YY,dp.JobCloseDate)as varchar(4)),3,4),''),2) + right('000'+ isnull(cast(datepart(DY,dp.JobCloseDate) as varchar(3)),''),3)  as DataEffectiveDate ,
	'11' as TransactionCode,
	Cast(isnull(dp.BaseState,'  ') as varchar(2)) as State,
	'00'  as JoinType
	 
FROM FactPolicyTransactionChange fpt 
	inner join DimPolicy dp on fpt.PolicyID = dp.PolicyID
	inner join DimLOB L on fpt.LOBID=l.LOBID
	Left Join (
				select Policynumber,effectivedate,expirationdate 
				from (
					Select Policynumber,effectivedate,expirationdate,jobupdatetime,dense_rank() over (partition by policynumber order by jobupdatetime desc) rank2  
					from (
						select policynumber ,effectivedate,expirationdate,jobupdatetime ,dense_rank() over (partition by policynumber order by effectivedate desc) rank1  
						from(
							select distinct policynumber,effectivedate,expirationdate,JobUpdateTime from dimpolicy where policynumber is not null
							) m 
						) n where n.rank1=2 
					)  v where v.rank2=1
			  ) prev1 On Prev1.policynumber = dp.policynumber
	Left Join (
				Select Policynumber,effectivedate,expirationdate
				from (
					select policynumber ,effectivedate,expirationdate,JobUpdateTime, dense_rank() over (partition by policynumber order by JobUpdateTime desc) rank
					from(
						select distinct policynumber,effectivedate,expirationdate,JobUpdateTime 
						from dimpolicy where policynumber in (select previouspolicynumber from dimpolicy where policynumber&lt;&gt;PreviousPolicyNumber) 
						) m where policynumber is not null
					 ) n where n.rank=1 
				) prev2 On prev2.policynumber=dp.PreviousPolicyNumber

where TransactionType in( 'Renewal') 
	and PolicyStatus = 'Bound' 
	and eventname='SendNonRenewalDocuments'
	and dp.IssueDate is not null 
	and (cast(dp.JobCloseDate  as date) &gt; ? and cast(dp.JobCloseDate as date) &lt;= ?) 
	and dp.BaseState ='MI'
	and L.LOBCode in ( 'WorkersCompLine','WC7Line') 

