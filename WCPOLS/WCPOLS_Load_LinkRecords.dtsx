select PolicyID,GWRowNumber,CompanyCode,PolicyNumber,EffectiveDate,DataEffectiveDate,Case when Ratings_Count&gt;=1 and TransactionCode='10' then '08' else TransactionCode end as TransactionCode,State,JoinType
from (
SELECT   fpt.PolicyID, dp.GWRowNumber,
	'19968' as CompanyCode,
	Cast(dp.PolicyNumber as varchar(18)) as PolicyNumber,
	Cast(isnull(replace(convert(varchar, dp.EffectiveDate,2),'.',''),'      ') as varchar(6)) as EffectiveDate,
	Right('000'+ isnull(SUBSTRING(cast(datepart(YY,dp.JobCloseDate)as varchar(4)),3,4),''),2) + right('000'+ isnull(cast(datepart(DY,dp.JobCloseDate) as varchar(3)),''),3)  as DataEffectiveDate ,
	Case when ds.IsNCCI =  1 then '08'
	     when dp.BaseState = 'MI' and dp.PolicyChangeType in ('Rating','Additional') then '08'
	     when dp.BaseState = 'MI' and dp.PolicyChangeType = 'NonRating' then '10'
		 when dp.BaseState = 'MI' and dp.PolicyChangeType is null then '10'
	     else '00' 
	End as TransactionCode,
	Cast(isnull(dp.BaseState,'  ') as varchar(2)) as State,
	'00'  as JoinType,
	row_number() over (partition by dp.PolicyNumber,convert(date,dp.JobUpdateTime) order by dp.PolicyTransactionEffectiveDate desc ,dp.ModelDate desc) as rn,
	ISNULL(CTE.Ratings_count,0) Ratings_count
FROM FactPolicyTransactionChange fpt inner join ( select * from DimPolicy where (JobDescription &lt;&gt; 'Bulk Policy Owner Change' or JobDescription is null) ) dp on fpt.PolicyID = dp.PolicyID
join DimLOB L on fpt.LOBID=l.LOBID
Left join DimState ds on dp.BaseState = ds.StateAbbr
LEFT JOIN CTE ON CTE.PolicyNumber = dp.PolicyNumber and CTE.JobUpdateTime = Convert(date,dp.JobUpdateTime)

where TransactionType in ('Policy Change') 
and PolicyStatus = 'Bound' 
and dp.IssueDate is not null 
and (cast(dp.JobUpdateTime as date) &gt; ? and cast(dp.JobUpdateTime as date) &lt;= ?) 
--and dp.BaseState in ('MI','IN')
and L.LOBCode in ( 'WorkersCompLine','WC7Line') 
) qry
where rn = 1


