                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">With 

CTE_Compliance as  
(
	Select *,
	Cast(PolicyId  as varchar(10)) + Cast(Dense_rank() over (Partition by PolicyNumber order by PartitionDate) as varchar(2)) PublicId,
	Dense_Rank() over (Partition by PolicyNumber order by PartitionDate) RNK,
	Row_Number() over (Partition by PolicyNumber order by CreateTime) RowNumber
	from (
		 Select Distinct 
		 dp.PolicyId,
		 dp.PolicyNumber, 
		 Left(dp.State,2) PolicyState,  
		 IIF(ISNULL(ComplianceDate,'12/31/9999') &gt;= NonComplianceDate, NonComplianceDate, ISNULL(ComplianceDate,'12/31/9999')) PartitionDate,
		 pa.ComplianceDate, 
		 pa.NonComplianceDate,
		 Cast(pa.OutstandingAmount as Money) OutstandingAmount, 
		 pa.CreateTime, 
		 pa.UpdateTime,

		Case When ComplianceDate is NULL Then '1'
			When ComplianceDate &gt;= NonComplianceDate Then '2'
			When ComplianceDate &lt; NonComplianceDate Then '1'
			When ComplianceDate is NOT NULL and Exists (Select 1 from DimPolicyAudit pa1 Where pa1.PolicyID = pa.PolicyID And pa1.ComplianceDate IS NULL) Then '1'
			When Cast(pa.UpdateTime as Date) &gt; Cast(pa.CreateTime as Date) Then '3'
		end as ComplianceNotificationCode,

		Case When pa.Category = 'Audit'			And pa.NCReason = 'Audit'							Then '04'
			 When pa.Category = 'Loss Control'  And pa.NCReason = 'Loss Control or Inspections'		Then '05'
			 When pa.Category = 'Non Payment'   And pa.NCReason = 'Amount Billed at Final Audit'	Then '01'
			 When pa.Category = 'Non Payment'	And pa.NCReason = 'Dispute Resolved; Debt Not Paid' Then '02'
			 When pa.Category = 'Non Payment'	And pa.NCReason = 'Default on Payment Plan'			Then '03'
			 When pa.Category = 'Non Payment'	And pa.NCReason = 'Other'							Then '98'
			 When pa.Category = 'Other'			And pa.NCReason = 'Other'							Then '99'
		 End as NCReasonCode,

		Case When pa.Category = 'Audit'			And pa.CRReason = 'Audit'							Then '06'
			 When pa.Category = 'Loss Control'  And pa.CRReason = 'Loss Control or Inspections'		Then '07'
			 When pa.Category = 'Non Payment'	And pa.CRReason = 'NonPayment'						Then '97'
			 When pa.Category = 'Other'			And pa.CRReason = 'Other'							Then '99'
		End as CRReasonCode

		From DimPolicyAudit pa 
			inner join DimPolicy dp on pa.PolicyId = dp.PolicyId
			--05 OCT - VS - Added join on DimState to use IsNCCI column. This column is replacing all the NCCI States e.g. IN, VA, CT, IL etc.
			Left join DimState ds on dp.BaseState = ds.StateAbbr
		Where pa.IsActive = 1 
		and  ds.IsNCCI = 1 
	    And (cast(pa.UpdateTime as date) &gt; ? and cast(pa.UpdateTime as date) &lt;= ?)
)T 
) ,

CTE_MinCreateTime
as 
(
	Select PublicId, 
	Min(CreateTime) MinCreateTime
	From CTE_Compliance
	Group by PublicId
),

CTE_Main
as
(
	Select 
		RowNumber, 
		RNK, 
		PolicyId, 
		PolicyNumber, 
		c.PublicId, 
		PolicyState,
		OutstandingAmount,
		ComplianceNotificationCode,
		Right(ISNULL(Convert(varchar(8), ComplianceDate,112),'000000'),6) ComplianceDate,
		Right(ISNULL(Convert(varchar(8), NonComplianceDate,112),'000000'),6) NonComplianceDate, 	
		IIF(ComplianceNotificationCode = 2, IIF(c.CreateTime  = m.MinCreateTime, CRReasonCode, NULL),'00') PrimaryComplianceReasonCode,
		IIF(ComplianceNotificationCode = 2, IIF(c.CreateTime != m.MinCreateTime, CRReasonCode, NULL),'00') AdditionalComplianceReasonCode,
		IIF(ComplianceNotificationCode = 1, IIF(c.CreateTime  = m.MinCreateTime, NCReasonCode, NULL),'00') PrimaryNonComplianceReasonCode,
		IIF(ComplianceNotificationCode = 1, IIF(c.CreateTime != m.MinCreateTime, NCReasonCode, NULL),'00') AdditionalNonComplianceReasonCode

	From CTE_Compliance c Left join CTE_MinCreateTime m on c.PublicId = m.PublicId
)

Select  
	RNK, 
	RowNumber,
	PublicId, 
	PolicyId, 
	PolicyNumber, 
	PolicyState, 
	ComplianceDate, 
	NonComplianceDate, 
	ComplianceNotificationCode,
	ISNULL(PrimaryComplianceReasonCode,'00') PrimaryComplianceReasonCode, 
	ISNULL(PrimaryNonComplianceReasonCode,'00') PrimaryNonComplianceReasonCode, 	
	ISNULL(Lead(AdditionalComplianceReasonCode)    Over(Partition by PublicId Order by RowNumber),'00') AdditionalComplianceReasonCode,
	ISNULL(Lead(AdditionalNonComplianceReasonCode) Over(Partition by PublicId Order by RowNumber),'00') AdditionalNonComplianceReasonCode,
	Case When PrimaryNonComplianceReasonCode in ('01', '02', '03', '98') 
		 Then Cast(Format(Sum(OutstandingAmount) over(Partition by PublicId),'0000000000') as varchar(10)) Else Replicate('0',10) End CurrentOutstandingAmount
From CTE_Main</property>
