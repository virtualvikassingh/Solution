                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">With CTE_MI_SoleProprietorship as 
(
	Select Distinct d.PolicyID --For Exclusion, we wont report the entire policy if this record exists
	From FactPolicyForm w
		Inner Join DimPolicy d on w.PolicyID = d.PolicyID
		Inner Join DimForm f on w.formid = f.FormID 
	Where   d.BaseState = 'MI'
		And upper(d.OrganizationType) = upper('Sole proprietorship')
		And upper(Left(Replace(FormNumber,' ',''),8)) = upper('WC000308') 
)

Select Distinct  p.PolicyId,
ISNULL(p.PolicyNumber,Replicate(' ',18)) PolicyNumber,
ISNULL(p.BaseState,Replicate(' ',2)) PolicyState,
ISNULL(p.PrimaryInsuredName,Replicate(' ',34)) NameOfInsured,
ISNULL(w.EndorsementName,Replicate(' ',60)) NameOfPersonTobeExcluded,
Case w.CoverageDescription 
	When 'Officer'			Then 'O'
	When 'Partner'			Then 'P'
	When 'Sole proprietor'	Then 'S'
Else 'X' End as DescriptorCode, OrganizationType,
ISNULL(Right(Convert(varchar(10),F.EndorsementEffectiveDate ,112),6),'000000') EndorsementEffectiveDate 

From WCPolicyEndorsement w
 Inner Join DimPolicy p on w.PolicyID = p.PolicyID and p.policyid not in (select PolicyID from CTE_MI_SoleProprietorship)
 Inner Join FactPolicyForm f on f.PolicyID = p.PolicyID
 Inner Join DimForm d on f.FormID = d.FormID
 --05 OCT - VS - Added join on DimState to use IsNCCI column. This column is replacing all the NCCI States e.g. IN, VA, CT, IL etc.
 Left join DimState ds on p.BaseState = ds.StateAbbr

Where w.PatternCode = 'WC7PartnersOfficersAndOthersExclEndorsementExcl'
and FormNumber Like 'WC 00 03 08%'
AND (
		(p.TransactionType in ('Submission', 'Policy Change','Rewrite') And (cast(p.JobUpdateTime as date) &gt; ? and cast(p.JobUpdateTime as date) &lt;= ?) ) OR
		(p.TransactionType ='Renewal' and p.eventname='SendNonRenewalDocuments' and p.BaseState='MI' And (cast(p.JobUpdateTime as date) &gt;DATEADD(day, -1, convert(date,?)) and cast(p.JobUpdateTime as date) &lt;=DATEADD(day, -1, convert(date,?)))) OR
		(cast(p.JobUpdateTime as date) &gt;? and cast(p.JobUpdateTime as date) &lt;=? and p.transactiontype='Renewal' and p.eventname is null) OR
		(cast(p.JobUpdateTime as date) &gt;? and cast(p.JobUpdateTime as date) &lt;=? and p.transactiontype='Renewal' and p.eventname='SendNonRenewalDocuments'	 and ds.IsNCCI = 1 /*p.BaseState = 'IN'*/)
	)
and PolicyStatus = 'Bound'


 Order by PolicyNumber, Policyid</property>
