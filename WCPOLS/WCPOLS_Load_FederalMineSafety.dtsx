                  refId="Package\Sequence Container\Load WCPOLS_FederalMineSafety\OLE_SRC WCPolicyEndorsement"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="OLE_SRC WCPolicyEndorsement"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Select Distinct  
		p.PolicyId,  
		Left(ISNULL(p.PolicyNumber,Replicate(' ',18)),18) PolicyNumber,	
		Left(ISNULL(p.BaseState,Replicate(' ',2)),2) PolicyState,
		'WC000102' EndorsementNumber,
		IIF(Len(FormNumber) &gt; 11, Right(FormNumber,1),' ') BureauVersionIdentifier, 
		Replicate (' ',11) CarrierVersionIdentifier,
		 
		Left(Cast(Coalesce(s1.StateCode, s2.StateCode,0) as varchar(2)) + Replicate(0,100),100)  State_Code,
		Left(ISNULL(p.PrimaryInsuredName,Replicate(' ',34)),34) NameOfInsured,
		GetDate() InsertDate,
		'27' RecordTypeCode, 
		ISNULL(Right(Convert(varchar(10),F.EndorsementEffectiveDate ,112),6),'000000') EndorsementEffectiveDate 
		 
	From WCPolicyEndorsement w
		Inner Join DimPolicy p on w.PolicyID = p.PolicyID
		Inner Join FactPolicyForm f on p.PolicyID = f.PolicyID
		Inner Join DimForm d on f.FormID = d.FormID		
		Left Join FactWCPremiumTransaction t on p.PolicyId = t.Policyid 
		Left Join DimState s1 on EndorsementValue2 = s1.StateAbbr
		Left Join DimState s2 on EndorsementValue2 = s2.State
		--05 OCT - VS - Added join on DimState to use IsNCCI column. This column is replacing all the NCCI States e.g. IN, VA, CT, IL etc.
		Left join DimState ds on p.BaseState = ds.StateAbbr

	Where   w.PatternCode = 'WC7FedMineSafety'
		AND (
				(p.TransactionType in ('Submission' ,'Policy Change','Rewrite')	And (cast(p.JobUpdateTime as date) &gt; ? and cast(p.JobUpdateTime as date) &lt;= ?)) OR
				(p.TransactionType ='Renewal' and p.eventname='SendNonRenewalDocuments' and p.BaseState='MI' And (cast(p.JobUpdateTime as date) &gt;DATEADD(day, -1, convert(date,?)) and cast(p.JobUpdateTime as date) &lt;=DATEADD(day, -1, convert(date,?)))) OR
				(cast(p.JobUpdateTime as date) &gt;? and cast(p.JobUpdateTime as date) &lt;=? and p.transactiontype='Renewal' and p.eventname is null) OR
				(cast(p.JobUpdateTime as date) &gt;? and cast(p.JobUpdateTime as date) &lt;=? and p.transactiontype='Renewal' and p.eventname='SendNonRenewalDocuments' and ds.IsNCCI = 1 /*p.BaseState = 'IN'*/)
			)
		and PolicyStatus = 'Bound'
		AND FormNumber Like 'WC 00 01 02%'
		And IsEndorsement = 1</property>
