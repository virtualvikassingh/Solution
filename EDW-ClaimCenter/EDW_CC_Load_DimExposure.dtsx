            <component
              refId="Package\DFT - DimExposure\OLE_SRC - Get Staging Data"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="OLE_SRC - Get Staging Data"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT 
    ex.PublicID       as PublicID
  , ex.CloseDate
  , ex.SettleDate
  , jur.Name AS JurisdictionState
  , isnull(et.TypeCode,'')   as ExposureCode
  , isnull(et.Name,'')       as ExposureTypeName
  , cov.ExposureLimit
  , isnull(ct.TypeCode,'')   as CoverageType
  , isnull(cs.TypeCode,'')   as CoverageSubType
  , ex.CreateTime     as ExposureCreateDate
  , ex.ClaimOrder     as ExposureNumber
  , coalesce(co.NameDenorm, isnull(co.FirstNameDenorm,'') + ' ' + isnull(co.LastNameDenorm,''))                      as ClaimantName
  , cast(case when es.typecode = 'open' and ex.reopendate is not null then 'reopen' else es.name end as varchar(20)) as ExposureStatus
  , cast(isnull(lp.name,'')   as varchar(50))        as loss_party
  , cast(isnull(seg.name,'') as varchar(60))         as segment             
  , cast(isnull(vl.name,'')  as varchar(50))         as validationlevel    
  , NULL as ongoingflag
  , cast(isnull(st.name,'')  as varchar(50)) as handlingstrategy
  , ex.AverageWeeklyWages          
  , ex.daysinweek
  , cast(concat(ex.BeanVersion, cov.BeanVersion ,co.BeanVersion) as varchar(20)) as BeanVersion
  , ex.Id as GWRowNumber
  , case when ex.Retired != 0 then 0 else 1 end as IsActive
  , cast(cov.PolicySystemId as varchar(50)) PolicySystemID
  , cast(substring(cov.policySystemId, charindex('.',cov.PolicySystemId,1)+1,len(cov.policysystemid)-(charindex(':',reverse(cov.PolicySystemId))+charindex('.',cov.PolicySystemId,1))) as varchar(50))as PolicySystemCoverageType
  , substring (cov.policysystemid,charindex(':',cov.PolicySystemId,1)+1,len(cov.policysystemid)) as PolicySystemCoverageID
  , cov.Deductible
  , ct.Name as CoverageTypeName
  , cs.Name as CoverageSubTypeName
  ,'WC' as LegacySourceSystem
 
FROM 
	cc_exposure ex 
	LEFT JOIN cctl_jurisdiction		jur ON ex.JurisdictionState= jur.ID  
	LEFT JOIN cc_coverage			cov ON ex.CoverageID = cov.ID     and cov.retired = 0
	LEFT JOIN cctl_exposuretype		et  ON ex.ExposureType = et.ID    
	LEFT JOIN cctl_coveragetype		ct  ON ex.PrimaryCoverage = ct.ID 
	LEFT JOIN cctl_coveragesubtype	cs	ON ex.CoverageSubType = cs.ID  
	LEFT JOIN cc_contact			co  ON co.ID = ex.ClaimantDenormID and co.retired = 0
    LEFT JOIN cctl_exposurestate	es  ON es.ID = ex.state
	LEFT JOIN cctl_losspartytype	lp  ON ex.lossParty = lp.id
	LEFT JOIN cctl_validationlevel	vl	ON ex.validationlevel = vl.id
	LEFT JOIN cctl_claimsegment		seg ON ex.Segment = seg.ID 
	LEFT JOIN cctl_claimstrategy	st  ON ex.Strategy = st.id
 
 
WHERE
	(    ex.UpdateTime &gt;= dateadd(d,?,cast(getdate() as date))
		OR cov.UpdateTime &gt;= dateadd(d,?,cast(getdate() as date))
		OR  co.UpdateTime &gt;= dateadd(d,?,cast(getdate() as date))
	)</property>
