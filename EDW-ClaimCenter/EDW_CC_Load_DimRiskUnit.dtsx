                  refId="Package\SEQC - Transformation\DFT - DimRiskUnit\OLE_SRC - Claim Center"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="OLE_SRC - Claim Center"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT ru.PublicID
     , CAST(ru.RUNumber AS INT)                AS [RUNumber]
	 , CASE WHEN ru.VehicleID IS NOT NULL THEN CAST(ru.RUNumber AS INT) ELSE NULL END AS [VehicleNumber]
     , CAST(bu.BuildingNumber AS VARCHAR(50))  AS [BuildingNumber] 
	 , CAST(cc.Code           AS VARCHAR(10))  AS [ClassCode]
	 , CAST(cc.[Description]  AS VARCHAR(100)) AS [ClassCodeDescription]
	 , CAST(ru.[Description]  AS VARCHAR(100)) AS [RiskUnitDescription]
     , CAST(cru.Name          AS VARCHAR(50))  AS [RiskUnittype] 
	 , CAST(ca.AddressLine1   AS VARCHAR(60))  AS [AddressLine1] 
	 , CAST(ca.AddressLine2   AS VARCHAR(60))  AS [AddressLine2]  
	 , CAST(ca.City           AS VARCHAR(50))  AS [City] 
     , CAST(ca.County         AS VARCHAR(50))  AS [County]     
	 , CAST(cts.Name          AS VARCHAR(30))  AS [state] 
	 , CAST(ca.PostalCode     AS VARCHAR(20))  AS [PostalCode]
	 , CAST(cat.Name          AS VARCHAR(30))  AS [AddressType] 
	 , CAST( LocationNumber  AS VARCHAR(5))                         AS [LocationNumber]
	 , CASE WHEN PrimaryLocation != 1 then 'N' else 'Y' end AS [isPrimaryLocation] 
	 , CAST(CONCAT(ru.BeanVersion, bu.BeanVersion, ve.BeanVersion, cc.BeanVersion, cp.BeanVersion, ca.BeanVersion) AS VARCHAR(20)) AS [BeanVersion]
	 , ru.Id                                  AS GWRowNumber
	 , CASE WHEN ru.Retired != 0 THEN 0 ELSE 1 END AS IsActive
 	 , substring (cp.policysystemid,charindex(':',cp.PolicySystemId,1)+1,len(cp.policysystemid)) as PolicyLocationID
	 , cast(substring(ru.policySystemId, charindex('.',ru.PolicySystemId,1)+1,len(ru.policysystemid)-(charindex(':',reverse(ru.PolicySystemId))+charindex('.',ru.PolicySystemId,1))) as varchar(50))as CoverageType
     , substring (ru.policysystemid,charindex(':',ru.PolicySystemId,1)+1,len(ru.policysystemid)) as CoverageID

from cc_riskunit ru  
LEFT JOIN cc_building bu       ON bu.ID	= ru.BuildingID   and bu.Retired = 0
LEFT JOIN cc_vehicle ve        ON ve.ID = ru.VehicleId    and ve.Retired = 0
LEFT JOIN cc_classcode cc      ON cc.ID = ru.ClassCodeID  and cc.Retired = 0
LEFT JOIN cc_policylocation cp ON cp.ID = ru.PolicyLocationID and cp.Retired = 0
LEFT JOIN cc_address ca        ON ca.id = cp.AddressID        and ca.Retired = 0
LEFT JOIN cctl_state cts       ON cts.id = ca.[state]           
LEFT JOIN cctl_addresstype cat ON cat.id = ca.AddressType  
LEFT JOIN cctl_riskunit cru    ON cru.ID = ru.Subtype
Where   
(bu.UpdateTime &gt;= DATEADD(d,?,CAST(GETDATE() AS DATE))
 OR
 ru.UpdateTime &gt;= DATEADD(d,?,CAST(GETDATE() AS DATE))
 OR 
 cc.UpdateTime &gt;= DATEADD(d,?,CAST(GETDATE() AS DATE))
)</property>
