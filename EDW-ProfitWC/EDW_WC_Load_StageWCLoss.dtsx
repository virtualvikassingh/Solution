,RiskLocation
AS
(
select DISTINCT PolicyID, f.RiskLocationID, rl.AddressLine1, rl.AddressLine2, rl.City, rl.County, rl.PostalCode
from FactWCPremiumTransaction f
Join DimRiskLocation rl ON f.RiskLocationID = rl.RiskLocationID
)
/*,ExposureCount
AS
(
       select claimid, AccountID, f.PolicyID, CompanyID, ProducerID, AgencyID, ProductID, 
          COALESCE(rl.RiskLocationID,-1) as RiskLocationAggID,
       month(pol.EffectiveDate) PolicyMonth, year(pol.EffectiveDate) PolicyYear,
       b.MonthofYear LossMonth, b.Year LossYear,
       sum(CASE WHEN st.PublicID = 'Opened' and fd.MonthofYear = MonthP and fd.Year = YearP
              THEN 1 ELSE 0
       END) ReportedCount,
       sum(CASE WHEN st.PublicID in ('Opened','Reopened') and f.ToDate is null and fd.MonthofYear &lt;= MonthP and fd.Year &lt;= YearP
              THEN 1 ELSE 0
       END) OpenCount,
       sum(CASE WHEN st.PublicID in ('Closed') and f.CloseIndicator = 'wpayment'
                       and fd.MonthofYear = MonthP and fd.Year = YearP
              THEN 1
              WHEN st.PublicID in ('Reopened') and f.CloseIndicator = 'wpayment'
                       and fd.MonthofYear = MonthP and fd.Year = YearP
              THEN -1
              ELSE 0
       END) ClosedwPayment,
       sum(CASE WHEN st.PublicID in ('Closed') and f.CloseIndicator = 'wopayment'
                       and fd.MonthofYear = MonthP and fd.Year = YearP
              THEN 1
              WHEN st.PublicID in ('Reopened') and f.CloseIndicator = 'wopayment'
                       and fd.MonthofYear = MonthP and fd.Year = YearP
              THEN -1
              ELSE 0
       END) ClosedwoPayment
       from FactClaimStatusChange f
       JOIN DimPolicy pol ON f.PolicyID = pol.PolicyID
       JOIN DimLossLocation ll ON f.LossLocationID = ll.LossLocationID
       JOIN DimStatus st ON f.StatusID = st.StatusID
       JOIN DimDate fd ON f.FromDateID = fd.DateID
       JOIN DimDate b ON f.LossDateID = b.DateID
       LEFT JOIN RiskLocation rl ON 
                     ISNULL(rl.AddressLine1,'-1') = ISNULL(ll.AddressLine1,'-1')
                     AND ISNULL(rl.AddressLine2,'-1') = ISNULL(ll.AddressLine2,'-1')
                     AND ISNULL(rl.City,'-1') = ISNULL(ll.City,'-1')
                     AND ISNULL(rl.County,'-1') = ISNULL(ll.County,'-1')
                     AND ISNULL(rl.PostalCode,'-1') = ISNULL(ll.PostalCode,'-1')
                     AND rl.PolicyID = f.PolicyID
       JOIN DateParam dp on dp.JoinCondition = 1   
       WHERE 1=1
       and pol.ProductCode = 'WC7WorkersComp'
	   and fd.MonthofYear &lt;= MonthP and fd.Year &lt;= YearP
	 
GROUP BY claimid,
AccountID, f.PolicyID, CompanyID, ProducerID, AgencyID, ProductID
, COALESCE(rl.RiskLocationID,-1),
month(pol.EffectiveDate) , year(pol.EffectiveDate), b.MonthofYear, b.Year
)
*/
,Reserve
AS
(
select FactClaimTransactionID, f.claimid,
       AccountID,
       CompanyID,
       ProducerID,
       AgencyID,
       ProductID,
       f.PolicyID,
       ex.PolicySystemCoverageId as lkp_WCCoverageID,
       rl.RiskLocationID as RiskLocationAggID,
    month(pol.EffectiveDate) PolicyMonth,
       year(pol.EffectiveDate) PolicyYear,
    b.MonthofYear LossMonth,
       b.Year LossYear
    , ClaimNumber
       , ExposureNumber
       , ct.TransactionType
    , ct.CostTypeName
    , ct.CostCategoryName
    , f.TransactionAmount      
    , f.TransactionCreateDate
	, Year(C.ReportedDate) as ReportedYear, month(C.ReportedDate) as ReportedMonth
    , CONVERT(nvarchar(6), f.TransactionCreateDate, 112) as TransactionMonth   
    , row_number () over (partition by   ClaimNumber
                                        ,ExposureNumber 
                                        ,ct.TransactionType
                                        ,ct.CostTypeName
                                        order by
                                         ClaimNumber
                                        ,ExposureNumber
                                        ,Transactioncreatedate
                                        ,ct.TransactionType
                                        ,ct.CostTypeName
                                        ) as Row1
FROM FactClaimTransaction f
JOIN DimClaim C on C.ClaimID = f.ClaimID
JOIN DimClaimTransaction Ct on Ct.ClaimTransactionID = f.ClaimTransactionID
JOIN DimPolicy pol ON f.PolicyID = pol.PolicyID
JOIN DimExposure ex ON f.ExposureID = ex.ExposureID
JOIN DimDate b ON f.LossDateID = b.DateID
JOIN DimLossLocation ll ON f.LocationID = ll.LossLocationID
JOIN DateParam dp on dp.JoinCondition = 1   
       LEFT JOIN RiskLocation rl ON 
                     ISNULL(rl.AddressLine1,'-1') = ISNULL(ll.AddressLine1,'-1')
                     AND ISNULL(rl.AddressLine2,'-1') = ISNULL(ll.AddressLine2,'-1')
                     AND ISNULL(rl.City,'-1') = ISNULL(ll.City,'-1')
                     AND ISNULL(rl.County,'-1') = ISNULL(ll.County,'-1')
                     AND ISNULL(rl.PostalCode,'-1') = ISNULL(ll.PostalCode,'-1')
                     AND rl.PolicyID = f.PolicyID
where 1=1
and CONVERT(nvarchar(6), dateadd(m,0,Transactioncreatedate), 112) &lt; TrxCurrMonth
and Ct.TransactionType = 'Reserve' and CostTypeName = 'Loss' and LOBCode = 'WorkersCompLine'
)

,Reserve_2
AS
(
select 
       AccountID, claimid,
       rsrv.PolicyID,
       CompanyID,
       ProducerID,
       AgencyID,
       ProductID,
       COALESCE(rsrv.lkp_WCCoverageID,-1) as lkp_WCCoverageID,
       COALESCE(rsrv.RiskLocationAggID,-1) as RiskLocationAggID,
    PolicyMonth,
       PolicyYear,
    LossMonth,
       LossYear,
	   ReportedYear, 
	   ReportedMonth,
    rsrv.TransactionMonth,
       sum(CASE WHEN Row1 = 1 AND TransactionMonth = TrxPastMonth THEN rsrv.TransactionAmount ELSE 0 
       END) as InitialLossReserve,
       sum(CASE WHEN Row1 &gt; 1 AND TransactionMonth = TrxPastMonth THEN rsrv.TransactionAmount ELSE 0 
       END )as ChangeLossReserve
from Reserve rsrv
JOIN DateParam dp on dp.JoinCondition = 1   
group by
       AccountID, claimid,
       rsrv.PolicyID,
       CompanyID,
       ProducerID,
       AgencyID,
       ProductID,
       COALESCE(rsrv.lkp_WCCoverageID,-1),
       COALESCE(rsrv.RiskLocationAggID,-1),
    PolicyMonth,
       PolicyYear,
    LossMonth,
       LossYear,
	   ReportedYear, 
	   ReportedMonth,
    rsrv.TransactionMonth
)

,OtherMetrics
as
(
select 
       AccountID, f.claimid,
       f.PolicyID,
       CompanyID,
       ProducerID,
       AgencyID,
       ProductID,
       COALESCE(ex.PolicySystemCoverageId,-1) as lkp_WCCoverageID,
       COALESCE(rl.RiskLocationID,-1) as RiskLocationAggID,
    month(pol.EffectiveDate) PolicyMonth,
       year(pol.EffectiveDate) PolicyYear,
    b.MonthofYear LossMonth,
       b.Year LossYear,
       	Year(C.ReportedDate) as ReportedYear, month(C.ReportedDate) as ReportedMonth,
    CONVERT(nvarchar(6), F.TransactionCreateDate, 112) as TransactionMonth    
       ,sum(CASE WHEN CONVERT(nvarchar(6), f.TransactionCreateDate, 112) = TrxPastMonth THEN TotalLossPaid
              ELSE 0
       END) as TotalLossPaid
       ,sum(CASE WHEN (LossReserve - TotalLossPaid) &gt; 0 then  (LossReserve - TotalLossPaid)
              ELSE 0
       END) as LossOSReserveFinancial
       ,sum(LossReserve - TotalLossPaid) as LossOSReserveNatural

    ,sum(CASE WHEN CONVERT(nvarchar(6), f.TransactionCreateDate, 112) = TrxPastMonth THEN DCCExpenseReserve
              ELSE 0
       END) as DCCExpenseReserve
    ,sum(CASE WHEN CONVERT(nvarchar(6), f.TransactionCreateDate, 112) = TrxPastMonth THEN AOExpenseReserve
              ELSE 0
       END) as AOExpenseReserve
    ,sum(CASE WHEN CONVERT(nvarchar(6), f.TransactionCreateDate, 112) = TrxPastMonth THEN DCCExpensePaid
              ELSE 0
       END) as DCCExpensePaid
    ,sum(CASE WHEN CONVERT(nvarchar(6), f.TransactionCreateDate, 112) = TrxPastMonth THEN AOExpensePaid
              ELSE 0
       END) as AOExpensePaid

       ,sum(CASE WHEN (DCCExpenseReserve - DCCExpensePaid) &gt; 0 then  (DCCExpenseReserve - DCCExpensePaid)
              ELSE 0
       END) as DCCExpenseOSReserveFinancial
       ,sum(DCCExpenseReserve - DCCExpensePaid) as DCCExpenseOSReserveNatural
       ,sum(CASE WHEN (AOExpenseReserve - AOExpensePaid) &gt; 0 then  (AOExpenseReserve - AOExpensePaid)
              ELSE 0
       END) as AOExpenseOSReserveFinancial
       ,sum(AOExpenseReserve - AOExpensePaid) as AOExpenseOSReserveNatural
	  ,SUM(SalvageRecovery) AS SalvageRecovery
	  , SUM(SubrogationRecovery) AS SubrogationRecovery
from FactClaimTransaction F
JOIN DimClaim C ON f.ClaimID= C.ClaimID
JOIN DimPolicy pol ON f.PolicyID = pol.PolicyID
JOIN DimExposure ex ON f.ExposureID = ex.ExposureID
JOIN DimDate b ON f.LossDateID = b.DateID
JOIN DimLossLocation ll ON f.LocationID = ll.LossLocationID
JOIN DateParam dp on dp.JoinCondition = 1   
       LEFT JOIN RiskLocation rl ON 
                     ISNULL(rl.AddressLine1,'-1') = ISNULL(ll.AddressLine1,'-1')
                     AND ISNULL(rl.AddressLine2,'-1') = ISNULL(ll.AddressLine2,'-1')
                     AND ISNULL(rl.City,'-1') = ISNULL(ll.City,'-1')
                     AND ISNULL(rl.County,'-1') = ISNULL(ll.County,'-1')
                     AND ISNULL(rl.PostalCode,'-1') = ISNULL(ll.PostalCode,'-1')
                     AND rl.PolicyID = f.PolicyID
WHERE 1=1
and pol.ProductCode = 'WC7WorkersComp'
and CONVERT(nvarchar(6), f.TransactionCreateDate, 112) &lt; TrxCurrMonth
GROUP BY 
       AccountID, f.claimid,
       f.PolicyID,
       CompanyID,
       ProducerID,
       AgencyID,
       ProductID,
       COALESCE(ex.PolicySystemCoverageId,-1),
       COALESCE(rl.RiskLocationID,-1),
    month(pol.EffectiveDate),
       year(pol.EffectiveDate),
    b.MonthofYear,
       b.Year,
       	Year(C.ReportedDate),
		month(C.ReportedDate),
		CONVERT(nvarchar(6), F.TransactionCreateDate, 112)
)
select
       COALESCE(OM.ClaimID,R2.ClaimID) as ClaimID,
       COALESCE(OM.AccountID,R2.AccountID) as AccountID,
       COALESCE(OM.PolicyID,R2.PolicyID) as PolicyID,
       COALESCE(OM.CompanyID,R2.CompanyID) as CompanyID,
       COALESCE(OM.ProducerID,R2.ProducerID) as ProducerID,
       COALESCE(OM.AgencyID,R2.AgencyID) as AgencyID,
       COALESCE(OM.ProductID,R2.ProductID) as ProductID,
       COALESCE(OM.lkp_WCCoverageID,R2.lkp_WCCoverageID) as WCCoverageID,
       COALESCE(OM.RiskLocationAggID,R2.RiskLocationAggID) as RiskLocationAggID,
    COALESCE(OM.PolicyMonth,R2.PolicyMonth) as PolicyMonth,
       COALESCE(OM.PolicyYear,R2.PolicyYear) as PolicyYear,
    COALESCE(OM.LossMonth,R2.LossMonth) as LossMonth,
       COALESCE(OM.LossYear,R2.LossYear) as LossYear,
       	COALESCE(OM.ReportedYear,R2.ReportedYear) as ReportedYear, 
		COALESCE(OM.ReportedMonth,R2.ReportedMonth) as ReportedMonth,
       (R2.InitialLossReserve) as InitialLossReserve,
       (R2.ChangeLossReserve) as ChangeLossReserve,
       (OM.TotalLossPaid) as TotalLossPaid,
       (LossOSReserveFinancial) as LossOSReserveFinancial,
       (LossOSReserveNatural) as LossOSReserveNatural,

       (OM.DCCExpenseReserve) as DCCExpenseReserve,
       (OM.AOExpenseReserve) as AOExpenseReserve,
       (OM.DCCExpensePaid) as DCCExpensePaid,
       (OM.AOExpensePaid) as AOExpensePaid,

       (OM.DCCExpenseOSReserveFinancial) as DCCExpenseOSReserveFinancial,
       (OM.DCCExpenseOSReserveNatural) as DCCExpenseOSReserveNatural,
       (OM.AOExpenseOSReserveFinancial) as AOExpenseOSReserveFinancial,
       (OM.AOExpenseOSReserveNatural) as AOExpenseOSReserveNatural,
	   MonthP as AccountingMonth,
	   YearP as AccountingYear,
	   (OM.SalvageRecovery) AS SalvageRecovery,
	   (OM.SubrogationRecovery) AS SubrogationRecovery
FROM OtherMetrics OM
FULL OUTER JOIN Reserve_2 R2 ON
       OM.AccountID = R2.AccountID
       AND OM.ClaimID = R2.ClaimID
       AND OM.PolicyID = R2.PolicyID
       AND OM.CompanyID = R2.CompanyID
       AND OM.ProducerID = R2.ProducerID
       AND OM.AgencyID = R2.AgencyID
       AND OM.ProductID = R2.ProductID
       AND OM.lkp_WCCoverageID = R2.lkp_WCCoverageID
       AND OM.RiskLocationAggID = R2.RiskLocationAggID
    AND OM.PolicyMonth = R2.PolicyMonth
       AND OM.PolicyYear = R2.PolicyYear
    AND OM.LossMonth = R2.LossMonth
       AND OM.LossYear = R2.LossYear
    AND OM.ReportedMonth = R2.ReportedMonth
       AND OM.ReportedYear = R2.ReportedYear
JOIN DateParam dp on dp.JoinCondition = 1</property>
