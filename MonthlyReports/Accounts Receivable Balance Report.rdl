      <Query>
        <DataSourceName>EDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Month">
            <Value>=Parameters!Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select Cast(Date as Date) AsofDate from DimDate where Year= @Year and MonthofYear = @Month and IsLastDayOfMonth = 1</CommandText>
      </Query>
