    <DataSet Name="AssignedRiskBalance">
      <Query>
        <DataSourceName>AssignedRiskBalance</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AsofDate">
            <Value>=Parameters!AsofDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @AsofDate datetime = getdate();
With CTE_Policy as 
(
	Select 				distinct
						A.AccountNumber, 
						PolicyNumber,
						PP.TermNumber,
						p.IssueDate
	From 				PolicyCenter.dbo.PC_PolicyPeriod PP 
	inner Join 	PolicyCenter.dbo.pctl_policyperiodstatus as PS on pp.Status = PS.ID
	inner Join 	PolicyCenter.dbo.pc_job as PJ on PP.JobID = PJ.ID
	Inner Join 	PolicyCenter.dbo.pc_policy as P on PP.PolicyID = P.ID
	Inner Join 	PolicyCenter.dbo.pc_account as A on P.AccountID = A.ID
	Where 
						PS.TYPECODE	= 'Bound'
	and 				pp.PublicID not in 
						(select PublicID  from PolicyCenter.dbo.PC_PolicyPeriod PP
						 where Cast(pp.PeriodStart as Date)= Cast(pp.CancellationDate as DAte) 
						)
	--And cast(PP.PeriodStart as date) &lt;= @AsofDate  ----To Inlude Advanced Payments
    --And Cast(PJ.CloseDate as date)  &lt;= @AsofDate   ----To Inlude Advanced Payments
	
)

,CTE_EffectivePolicy as 
(	
	Select 
						BCTL_UWCOMPANY.NAME AS BRANDNAME,
						pcju.TYPECODE + ' - ARS' As 'MarketTypeBrand',
						 bctl_uwcompany.Name As "Undewriting Company",
						Case when pcju.TYPECODE = 'MI' then '15' when pcju.TYPECODE = 'IN' then '13' WHEN pcju.TYPECODE in ('SC', 'VA', 'IL', 'CT') then '12' else 'NA' end As "Pool Code",
						BC_POLICYPERIOD.UNDERWRITER,
						BC_PRODUCER.NAME AS PRODUCERNAME,
						BC_PRODUCERCODE.CODE AS PRODUCERCODE,
						BC_ACCOUNT.ACCOUNTNAME,
						BC_ACCOUNT.ACCOUNTNUMBER,
						Case when C.Name IS Null then C.FirstName + ' ' + C.LastName else C.Name end  AS POLICYNAME,
						BC_POLICYPERIOD.POLICYNUMBER,
						BC_POLICYPERIOD.TermNumber,
						BC_POLICYPERIOD.POLICYPEREFFDATE AS POLICYEFFDATE,
						BC_POLICYPERIOD.POLICYPEREXPIRDATE AS POLICYPEREXPIRDATE,
						 bctl_accountsegment.NAME As "Segment",
						(BC_CONTACT.FIRSTNAME + ' ' + BC_CONTACT.LASTNAME) PAYER_NAME
		
	from   				BC_POLICYPERIOD
	JOIN 				BC_POLICY ON  BC_POLICY.ID =  BC_POLICYPERIOD.POLICYID
	join 				bctl_jurisdiction pcju on bc_policyperiod.RiskJurisdiction = pcju.ID
	left join  			bc_policyperiodcontact PC on  bc_policyperiod.ID = PC.PrimaryPolicyPeriodID
	join  				bc_CONTACT C on  C.ID = PC.ContactID  and PC.ID &lt;&gt; PC.Retired
	JOIN 				BC_ACCOUNT ON  BC_ACCOUNT.ID =  BC_POLICY.ACCOUNTID
	left join  			bctl_accountsegment on  bc_account.segment =  bctl_accountsegment.ID
	Left Join  			BCTL_UWCOMPANY ON  BC_POLICYPERIOD.UWCOMPANY =  BCTL_UWCOMPANY.ID
	LEFT JOIN 			BC_ACCOUNTCONTACT ON  BC_ACCOUNTCONTACT.ACCOUNTID= BC_ACCOUNT.ID  AND PRIMARYPAYER=1
	LEFT OUTER JOIN 	BC_CONTACT ON  BC_ACCOUNTCONTACT.CONTACTID =  BC_CONTACT.ID
	LEFT JOIN    		BC_POLICYCOMMISSION ON  BC_POLICYCOMMISSION.PRIMARYPOLICYPERIODID=  BC_POLICYPERIOD.ID
	LEFT JOIN 			BC_PRODUCERCODE ON BC_PRODUCERCODE.ID = BC_POLICYCOMMISSION.PRODUCERCODEID
	LEFT  JOIN 			BC_PRODUCER     ON BC_PRODUCERCODE.PRODUCERID =  BC_PRODUCER.ID
		--where cast(BC_POLICYPERIOD.POLICYPEREFFDATE as date) &lt;=@AsofDate ----To Inlude Advanced Payments
		
)

,CTE_PolicyTransaction as
(
	Select 				acc.AccountNumber,
						PP.PolicyNumber,
						pp.TermNumber,
						DATEPART(yyyy,trx.TransactionDate)*100 + DATEPART(m,trx.TransactionDate) As 'Month',
						Sum(case when trx.Subtype = 54 and cp.ID not in (6,20) and lds.NAME = 'Credit' then trx.Amount else 0 end) as 'Written Premium',
						Sum(case when trx.Subtype = 54 and tacptr.TAccountType in (4,1)  and  cp.ID = 20 and lds.NAME = 'Credit' then trx.Amount else 0 end) as 'Surcharge',
						Sum(case when trx.Subtype = 54 and tacptr.TAccountType in (4,1)  and  cp.ID = 6 and lds.NAME = 'Credit' then trx.Amount else 0 end) as 'Tax',
						Sum(case when trx.Subtype = 22 then trx.Amount else 0 end) as 'Policy Level Paid',
						-1* Sum(case When cast(trx.TransactionDate as date) &lt;= @AsofDate
						And cast(trx.TransactionDate as date) &lt;  cast(PP.POLICYPEREFFDATE as date) 
						And @AsofDate &lt;  cast(PP.POLICYPEREFFDATE as date)
						And trx.Subtype = 22
						Then (cast(IsNull(trx.Amount,0) as Decimal(10,2))) else 0 end) As AdvancedPremium,
						Sum(case when trx.Subtype = 30 and lds.NAME = 'Credit' then trx.Amount else 0 end) as 'Writeoff'
	from 				BC_lineitem li
	JOIN 				BC_transaction trx ON li.TransactionID = trx.ID
	JOIN 				bctl_transaction btrx ON trx.Subtype = btrx.ID
	JOIN 				BC_taccount tac ON li.TAccountID = tac.ID
	JOIN 				bctl_ledgerside lds ON li.Type = lds.ID
	JOIN 				BC_TAccountPattern tacptr ON tac.TAccountPatternID = tacptr.ID
	join 				bc_policyperiod as pp on tac.TAccountContainerID = pp.HiddenTAccountContainerID
	Join 				bc_policy as p on pp.PolicyID = p.ID
	JOIN 				bc_account as acc on p.AccountID = acc.ID
	JOIN 				bc_chargepattern cp on tacptr.ChargePatternID = cp.ID
	where 
						Cast(trx.TransactionDate As Date) &lt; = @AsofDate
						and trx.Subtype in (54,22,30) 
	group by 			acc.AccountNumber,
						PP.PolicyNumber,
						PP.TermNumber,
						DATEPART(yyyy,trx.TransactionDate)*100 + DATEPART(m,trx.TransactionDate)
)

-------------------------------------------------------------------------------
,CTE_Consolidated As 
(
	Select 
					Temp1.ReportingCompany,
					Temp1.MarketTypeBrand,
					Temp1.PRODUCERCODE,
					Temp1.AccountNumber,
					Temp1.PolicyNumber,
					Temp1.TermNumber,
					Temp1.PolicyEffectiveDate,
					Temp1.IssueDate,
					Temp1.Month,
					Temp1.[Written Premium],
					Temp1.Surcharge,
					Temp1.Tax,
					Temp1.[Policy Level Paid],
					Temp1.Writeoff,
					Case when Temp1.[Premium Beg Balance] = 0 then 0 
					     else(LAG(Temp1.[Premium Amount - Ending Balance]) over (partition by Temp1.PolicyNumber,Temp1.TermNumber order by Temp1.Month)) 
						 end As "Premium Amount - Beginning Balance",
					Temp1.[Premium Amount - Ending Balance]
	from
		(
		Select 
					Temp.Name As 'ReportingCompany' ,
					Temp.MarketTypeBrand,
					Temp.PRODUCERCODE,
					Temp.AccountNumber,
					Temp.PolicyNumber,
					Temp.TermNumber,
					Temp.PolicyEffectiveDate,
					Temp.IssueDate,
					Temp.Month,
					Temp.[Written Premium],
					Temp.Surcharge,
					Temp.Tax,
					Temp.[Policy Level Paid],
					Temp.Writeoff,
					Case when Temp.Month = IssueMonth then 0 else NUll end As 'Premium Beg Balance',
					Sum( Temp.[Written Premium] + Temp.Surcharge + Temp.Tax - Temp.[Policy Level Paid] - Temp.Writeoff ) over (partition by Temp.PolicyNumber,Temp.TermNumber 
					order by Temp.Month)  As "Premium Amount - Ending Balance"
		from 
			(
			Select 
					EP.[Undewriting Company] Name,EP.MarketTypeBrand,EP.PRODUCERCODE,
					EP.AccountNumber As AccountNumber,
					EP.PolicyNumber As PolicyNumber,
					EP.TermNumber,
					Cast(EP.POLICYEFFDATE As Date) As PolicyEffectiveDate,
					Cast(P.IssueDate As Date) As IssueDate,
					DATEPART(yyyy,P.IssueDate)*100 + DATEPART(m,P.IssueDate) As IssueMonth,
					PT.Month As Month,
					ISNULL(PT.[Written Premium],0) As 'Written Premium',
					ISNULL(PT.Surcharge,0) As 'Surcharge',
					ISNULL(PT.Tax,0) As 'Tax',
					ISNULL(PT.[Policy Level Paid],0) As 'Policy Level Paid',
					ISNULL(PT.Writeoff,0) As 'Writeoff'

			from
			CTE_Policy As P
			Inner Join 		CTE_EffectivePolicy EP ON P.PolicyNumber = EP.PolicyNumber and P.AccountNumber = EP.AccountNumber and EP.TermNumber =  P.TermNumber
			Left Join 		CTE_PolicyTransaction PT ON EP.PolicyNumber = PT.PolicyNumber and EP.TermNumber = PT.TermNumber
			) Temp

		) As Temp1
)
------------------------------------------------------------------------------------

--Select * From CTE_Consolidated order by AccountNumber,PolicyNumber,Month

,CTE_AccountReceivableFinal As
(
	Select
					ConsviewFinal.ReportingCompany,
					ConsviewFinal.MarketTypeBrand,
					ConsviewFinal.PRODUCERCODE,
					ConsviewFinal.AccountNumber,
					ConsviewFinal.TermNumber,
					ConsviewFinal.PolicyNumber,
					ConsviewFinal.PolicyEffectiveDate,
					ConsviewFinal.IssueDate,
					DATEPART(yyyy,@AsofDate)*100 + DATEPART(m,@AsofDate) As 'Month',
					Sum(ConsviewFinal.[Premium Amount - Beginning Balance]) As 'Premium Amount - Beginning Balance',
					Sum(ConsviewFinal.[Written Premium]) As 'Written Premium',Sum(ConsviewFinal.Surcharge) As 'Surcharge',Sum(ConsviewFinal.Tax) As 'Tax',
					Sum(ConsviewFinal.[Policy Level Paid]) As 'Policy Level Paid',
					--Sum(ConsviewFinal.[Payment Received]) As 'Payment Received',
					--Sum(ConsviewFinal.[Reversed Payment]) As 'Reversed Payment',Sum(ConsviewFinal.[Funds Transferred]) As 'Funds Transferred',
					--Sum(ConsviewFinal.RefundAmount) As 'RefundAmount',Sum(ConsviewFinal.RefundsVoided) As 'RefundsVoided',
					Sum(ConsviewFinal.Writeoff) As 'Writeoff',
					Sum(ConsviewFinal.[Premium Amount - Ending Balance]) As 'Premium Amount - Ending Balance'
	From
		(
		Select 
					Consview.ReportingCompany,
					Consview.MarketTypeBrand,
					Consview.PRODUCERCODE,
					Consview.AccountNumber,
					Consview.PolicyNumber,
					Consview.TermNumber,
					Consview.PolicyEffectiveDate,
					Consview.IssueDate,
					Consview.Month,
					Case when Consview.Month = DATEPART(yyyy,@AsofDate)*100 + DATEPART(m,@AsofDate)  then Consview.[Written Premium] else 0 end As 'Written Premium',
					Case when Consview.Month = DATEPART(yyyy,@AsofDate)*100 + DATEPART(m,@AsofDate)  then Consview.Surcharge else 0 end As 'Surcharge',
					Case when Consview.Month = DATEPART(yyyy,@AsofDate)*100 + DATEPART(m,@AsofDate)  then Consview.Tax else 0 end As 'Tax',
					Case when Consview.Month = DATEPART(yyyy,@AsofDate)*100 + DATEPART(m,@AsofDate)  then Consview.[Policy Level Paid] else 0 end As 'Policy Level Paid',
					Case when Consview.Month = DATEPART(yyyy,@AsofDate)*100 + DATEPART(m,@AsofDate)  then Consview.Writeoff else 0 end As 'Writeoff',
					Case when Max( ConsView.Month) over (partition by ConsView.PolicyNumber,ConsView.TermNumber ) = DATEPART(yyyy,@AsofDate)*100 + DATEPART(m,@AsofDate)  
					    then (Case when Max( ConsView.Month) over (partition by ConsView.PolicyNumber,ConsView.TermNumber ) =  ConsView.Month then ConsView.[Premium Amount - Beginning Balance] else 0  end)
					    else (Case when Max( ConsView.Month) over (partition by ConsView.PolicyNumber,ConsView.TermNumber ) =  ConsView.Month then ConsView.[Premium Amount - Ending Balance] else 0  end) end As 'Premium Amount - Beginning Balance',
					Case when Max( ConsView.Month) over (partition by ConsView.PolicyNumber,ConsView.TermNumber ) =  ConsView.Month then ConsView.[Premium Amount - Ending Balance]     else 0  end As "Premium Amount - Ending Balance"
		From 		CTE_Consolidated As ConsView
		) As ConsviewFinal
	group by 		ConsviewFinal.ReportingCompany,
					ConsviewFinal.MarketTypeBrand,
					ConsviewFinal.PRODUCERCODE,
					ConsviewFinal.AccountNumber,
					ConsviewFinal.TermNumber,
					ConsviewFinal.PolicyNumber,
					ConsviewFinal.PolicyEffectiveDate,
					ConsviewFinal.IssueDate
)


Select * From 		CTE_AccountReceivableFinal</CommandText>
