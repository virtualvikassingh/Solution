              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">WITH InvoiceSteam AS
(
	select ins.ID
	,ins.CreationOrder
,tlp.NAME as Periodicity
,tlins.NAME as InvoiceStreamType
--,ins.OverridingFirstAnchorDate
--,ins.OverridingSecondAnchorDate
--,ins.OverridingInvoicingLeadTime
--,tlbdb.NAME as OverridingBillOrDueDateBillingType
,ins.BeanVersion
,ins.UpdateTime
	from bc_invoicestream ins
	left join bctl_periodicity tlp on tlp.ID = ins.Periodicity
    left join bctl_invoicestream tlins on tlins.ID = ins.Subtype
    left join bctl_billdateorduedatebilling tlbdb on tlbdb.ID = ins.OverridingBillOrDueDateBilling
)
select 
Cast(dt.InvoiceNumber  as Varchar(50)) as InvoiceNumber
,dt.EventDate as InvoiceDate
,dt.PaymentDueDate as InvoiceDueDate
,dt.CreateTime as InvoiceCreateDate
,Cast(tlis.NAME as Varchar(100)) as InvoiceStatus
,case when dt.Amount = dt.AmountDue then 'unpaid' when dt.Amount = 0 then 'fullypaid' when dt.Amount &lt;&gt; dt.AmountDue then 'partiallypaid' end as PaidStatus
,dt.Amount as InvoiceAmount
,dt.UnappliedAmount as UnappliedAmount
,dt.ColUnappliedAmount as ColUnappliedAmount
,dt.AmountDue as InvoiceDueAmount
,dt.OutstandingAmount as TotalDueAmount
,case dt.AdHoc when 1 then 'Y' when 0 then 'N' end as IsAdHoc
,Cast(acct.AccountNumber + '-0' + convert(varchar,ins.CreationOrder)
as Varchar(50)) as InvoiceStreamNumber
,Cast(ins.Periodicity as Varchar(50)) as Periodicity
,Cast(ins.InvoiceStreamType as Varchar(50)) as  InvoiceStreamType 
,dt.PublicID
,dt.ID as GWRowNumber
,cast(concat(dt.BeanVersion ,acct.BeanVersion ,ins.BeanVersion) as varchar(20)) as BeanVersion
,case dt.Retired when 0 then 1 else 0 end as IsActive
,'WC' as LegacySourceSystem

from bc_invoice dt
left join bctl_invoicestatus tlis on tlis.ID = dt.Status
left join bc_account acct on acct.ID = dt.AccountID
left join InvoiceSteam ins on ins.ID = dt.InvoiceStreamID
WHERE
(dt.UpdateTime &gt;= dateadd(d,?,cast(getdate() as date))
OR
acct.UpdateTime &gt;= dateadd(d,?,cast(getdate() as date))
OR
ins.UpdateTime &gt;= dateadd(d,?,cast(getdate() as date))
)</property>
