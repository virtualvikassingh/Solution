                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">; with policylkp as
(
select chg.ID
,case when pp.ID is null then -2 else isnull(pp.TermNumber ,-1) end as TermNumber
,case when pp.ID is null then '-2' else isnull(pp.PolicyNumber ,'-1') end  as PolicyNumber
,CASE WHEN tlbi.name = 'Policy Issuance' THEN 'Submission' ELSE tlbi.name END AS TransactionType
, tlbi.TYPECODE
,cast(CASE WHEN tlbi.TYPECODE in ('Issuance', 'Rewrite','Renewal')
                 THEN pp.PolicyPerEffDate 
                 ELSE CASE WHEN ModificationDate is NOT NULL THEN ModificationDate ELSE ChargeDate END
         END as DATE) as ChargeDate
from bc_invoiceitem dt
left join bc_policyperiod pp on pp.ID = dt.PolicyPeriodID
left join bc_charge chg on chg.ID = dt.ChargeID
left join bc_billinginstruction bi on bi.ID = chg.BillingInstructionID
left join bctl_billinginstruction tlbi on tlbi.ID = bi.Subtype
left join bc_chargepattern chgp on chgp.ID = chg.ChargePatternID
)



SELECT * FROM 
(
	SELECT ISNULL(acct.PublicID,'-1')												AS lkp_BillingAccountID
	 , case when pp.ID is null then -2 else isnull(pp.TermNumber ,-1) end			AS TermNumber
	 , case when pp.ID is null then '-2' else isnull(pp.PolicyNumber ,'-1') end		AS PolicyNumber
	 , CAST(CASE WHEN pp.ID IS NULL THEN '9999-12-31' ELSE pp.PolicyPerEffDate   END AS DATE) AS  EffectiveDate
	 , CAST(CASE WHEN pp.ID IS NULL THEN '9999-12-31' ELSE pp.PolicyPerExpirDate END AS DATE) AS  ExpirationDate
	 , ISNULL(inv.PublicID,'-1')							AS lkp_InvoiceID
	 , ISNULL(invi.PublicID,'-1')							AS lkp_InvoiceItemID
	 , ISNULL(bd.PublicID,'-1')							    AS lkp_PaymentDistributionID
	 , ISNULL(bmr.PublicID,'-2')							AS lkp_PaymentReceivedID
	 , ISNULL(paypl.PublicID,'-1')							AS lkp_PaymentPlanID
	 , ISNULL(billpl.PublicID,'-1')							AS lkp_BillingPlanID
	 , CAST(CASE WHEN bmr.PublicID IS NOT NULL THEN ISNULL(bmr.ReceivedDate,'1900-01-01') ELSE ISNULL(bmr.ReceivedDate,'1900-01-02') END AS DATE) AS lkp_ReceivedDateID
	 , ISNULL(uwc.TYPECODE,'-1')							AS lkp_CompanyCode
	 , ISNULL(prdc.PublicID,'-1')						    AS lkp_AgencyID
	 , ISNULL(pcod.Code,'-1')							    AS lkp_ProducerID
	 , ISNULL(dt.CommissionAmountToApply,0)					AS CommissionAmount
	 , ISNULL(dt.GrossAmountToApply,0)					    AS GrossAmount
	 , dt.ID											    AS GWRowNumber
	 , CAST(dt.BeanVersion		 AS VARCHAR(20))		    AS BeanVersion
	 , ISNULL(ur.PublicID, '-1')							AS lkp_UpdateUserID
	 , CAST(ISNULL(dt.ReversedDate ,'1900-01-02') AS DATE)  AS lkp_ReversalDateID
	 , CAST(ISNULL(tlbi.NAME,'') AS VARCHAR(50))			AS DistributionItemType
	 , CAST(ISNULL(did.NAME,'')  AS VARCHAR(50))			AS DispositionType
     , dt.ExecutedDate
	 , dt.PublicID 
	 , plkp.TransactionType
	 , plkp.ChargeDate as TransactionEffectiveDate,
	 'WC' as LegacySourceSystem
FROM bc_basedistitem dt
LEFT JOIN bc_basedist bd			   ON bd.ID               = COALESCE(dt.ActiveDistID ,dt.ReversedDistID)
LEFT JOIN bc_invoiceitem invi	       ON invi.ID             = dt.InvoiceItemID
LEFT JOIN bc_invoice inv		       ON inv.ID              = invi.InvoiceID
LEFT JOIN bc_account acct		       ON acct.ID             = inv.AccountID
LEFT JOIN bc_policyperiod pp	       ON pp.ID               = dt.PolicyPeriodID
LEFT JOIN bc_policy p			       ON pp.PolicyID         = p.ID
LEFT JOIN bc_plan paypl			       ON paypl.ID            = pp.PaymentPlanID
LEFT JOIN bc_plan billpl               ON billpl.ID           = acct.BillingPlanID
LEFT JOIN bc_basemoneyreceived bmr     ON bmr.BaseDistID      = bd.ID
LEFT JOIN bc_policycommission pcom     ON pcom.PolicyPeriodID = pp.ID AND pcom.Role = 1 AND pcom.EndDate IS NULL
LEFT JOIN bc_producercode pcod         ON pcod.ID             = pcom.ProducerCodeID
LEFT JOIN bc_producer prdc		       ON prdc.ID             = pcod.ProducerID
LEFT JOIN bc_user ur				   ON ur.ID               = dt.UpdateUserID
LEFT JOIN bctl_distitemdisposition did ON dt.Disposition      = did.ID
LEFT JOIN bctl_basedistitem tlbi       ON dt.Subtype          = tlbi.ID
LEFT JOIN bctl_uwcompany uwc on pp.UWCompany = uwc.ID
left join policylkp plkp on plkp.ID = invi.ChargeID
 WHERE dt.UpdateTime &gt;= DATEADD(D,?,CAST(GETDATE() AS DATE))
) AS O

Order by 3,2</property>
