                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
isnull(acct.PublicID,'-1') as lkp_BillingAccountID
,isnull(billpl.PublicID,'-1') as lkp_BillingPlanID
,isnull(dt.PublicID,'-1') as lkp_PaymentReceivedID
,cast(isnull(dt.ReceivedDate,'1900-01-01') as date) as lkp_ReceivedDateID
,cast(isnull(dt.ReversalDate,'1900-01-02') as date) as lkp_ReversalDateID
,case when dt.BaseDistID is not null then isnull(bd.PublicID,'-1') else isnull(bd.PublicID,'-2') end as lkp_PaymentDistributionID
,isnull(ur.PublicID, '-1') as lkp_UpdateUserID
,isnull(dt.Amount,0) as ReceivedAmount
,dt.ID as GWRowNumber
,cast(dt.BeanVersion as varchar(20)) as BeanVersion
, dt.PublicID
,'WC' as LegacySourceSystem
from bc_basemoneyreceived dt
left join bc_basedist bd on dt.BaseDistID = bd.ID
left join bc_account acct on acct.ID = dt.AccountID
left join bc_plan billpl on billpl.ID = acct.BillingPlanID
left join bc_user ur on ur.ID = dt.UpdateUserID
WHERE 
dt.UpdateTime &gt;= dateadd(d,?,cast(getdate() as date))</property>
