              description="OLE DB Source"
              name="OLE_SRC - BC_InvoiceItem"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT * FROM 
(
select acct.AccountNumber
	,isnull(acct.PublicID,'-1') as lkp_BillingAccountID
	,case when pp.ID is null then -2 else isnull(pp.TermNumber ,-1) end as TermNumber
	, case when pp.ID is null then '-2' else isnull(pp.PolicyNumber ,'-1') end  as PolicyNumber
	, CAST(CASE WHEN pp.ID IS NULL THEN '9999-12-31' ELSE pp.PolicyPerEffDate   END AS DATE) AS  EffectiveDate
	, CAST(CASE WHEN pp.ID IS NULL THEN '9999-12-31' ELSE pp.PolicyPerExpirDate END AS DATE) AS  ExpirationDate
	,isnull(inv.PublicID,'-1') as lkp_InvoiceID
	,isnull(dt.PublicID,'-1') as lkp_InvoiceItemID
	,Case when dt.PolicyPeriodID is not NULL Then isnull(paypl.PublicID,'-1')
	Else '-2'
	End as lkp_PaymentPlanID
	,isnull(billpl.PublicID,'-1') as lkp_BillingPlanID
	,cast(isnull(dt.EventDate,'1900-01-01') as date) as lkp_InvoiceItemDateID 
	,cast(Case when dt.PolicyPeriodID is not NULL Then isnull(uwc.TYPECODE,'-1') 
	Else '-2'
	End as varchar)  as lkp_CompanyCode
	,Case when dt.PolicyPeriodID is not NULL Then isnull(prdc.PublicID,'-1') 
	Else '-2'
	End as lkp_AgencyID
	, Case when dt.PolicyPeriodID is not NULL Then isnull(pcod.Code,'-1')
	Else '-2'
	End as lkp_ProducerID
	,isnull(dt.Amount                         ,0)    as InvoiceAmount
	,isnull(dt.PaidAmount   ,0)    as PaidAmount
	,isnull(dt.PrimaryCommissionAmount   ,0)    as PrimaryCommissionAmount
	,isnull(dt.GrossAmountWrittenOff          ,0)    as GrossAmountWriteOff 
	,isnull(dt.PromisedAndPaidAmount   ,0) as PromisedAndPaidAmount 
	,isnull(dt.PrimaryPaidCommission   ,0) as PrimaryPaidCommission 
	,isnull(dt.PrimaryWrittenOffCommission   ,0) as PrimaryWrittenOffCommission 
	,isnull(dt.PromisedCommission   ,0) as PromisedCommission 
	,isnull(dt.PrimaryCmsnPayableAmount   ,0) as PrimaryCmsnPayableAmount 
	,dt.ID as GWRowNumber
	,cast(dt.BeanVersion as varchar(20)) as BeanVersion
	,dt.publicId,
	'WC' as LegacySourceSystem
	from bc_invoiceitem dt
	left join bc_invoice inv on inv.ID = dt.InvoiceID
	left join bc_account acct on acct.ID = inv.AccountID
	left join bc_policyperiod pp on pp.ID = dt.PolicyPeriodID
	left join bc_policy p on pp.PolicyID=p.ID
	left join bc_plan paypl on paypl.ID = pp.PaymentPlanID
	left join bc_plan billpl on billpl.ID = acct.BillingPlanID
	left join bc_policycommission pcom on pcom.PolicyPeriodID = pp.ID and pcom.Role = 1 and pcom.EndDate is null
	left join bc_producercode pcod on pcod.ID = pcom.ProducerCodeID
	left join bc_producer prdc on prdc.ID = pcod.ProducerID
	left JOIN bctl_uwcompany uwc on pp.UWCompany = uwc.ID
	WHERE dt.UpdateTime &gt;= DATEADD(D,?,CAST(GETDATE() AS DATE))
	 
) AS O</property>
