                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
cast(dt.CreateTime as datetime) as  InvoiceItemCreateDate
,chgp.ChargeName as InvoiceCategory
,Cast(tlbi.NAME  as Varchar(100)) as PolicyContext
,Cast(tliit.NAME as Varchar(100)) as InvoiceItemType
,Cast(tlcs.NAME as Varchar(100)) as ChargeHoldStatus
,chg.ChargeDate
,Cast(tlchgp.NAME as Varchar(100)) as ChargePatternType
,chgp.ChargeCode
,Cast(tlit.NAME as Varchar(255)) as InvoiceTreatmentName
,case chgp.InternalCharge when 1 then 'Y' when 0 then 'N' end as IsInternalCharge
,Cast(tlcc.NAME as Varchar(100)) as ChargeCategory

,dt.PublicID
,dt.ID as GWRowNumber
,cast(concat(dt.BeanVersion ,chg.BeanVersion ,chgp.Beanversion ,bi.BeanVersion) as varchar(20)) as BeanVersion
,case dt.Retired when 0 then 1 else 0 end as IsActive
,dt.eventDate as EventDate
,'WC' as LegacySourceSystem

from bc_invoiceitem dt
left join bc_charge chg on chg.ID = dt.ChargeID
left join bc_chargepattern chgp on chgp.ID = chg.ChargePatternID
left join bc_billinginstruction bi on bi.ID = chg.BillingInstructionID
left join bctl_billinginstruction tlbi on tlbi.ID = bi.Subtype
left join bctl_invoiceitemtype tliit on tliit.ID = dt.Type
left join bctl_chargeholdstatus tlcs on tlcs.ID = chg.HoldStatus
left join bctl_chargepattern tlchgp on tlchgp.ID = chgp.Subtype
left join bctl_invoicetreatment tlit on tlit.ID = chgp.InvoiceTreatment
left join bctl_chargecategory tlcc on tlcc.ID = chgp.Category
WHERE
(dt.UpdateTime &gt;= dateadd(d,?,cast(getdate() as date))
OR
chg.UpdateTime &gt;= dateadd(d,?,cast(getdate() as date))
OR
chgp.UpdateTime &gt;= dateadd(d,?,cast(getdate() as date))
OR
bi.UpdateTime &gt;= dateadd(d,?,cast(getdate() as date))
)</property>
