      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{6558366F-DE5C-421B-863D-389847506381}"
          SQLTask:SqlStatementSource="With CTE_Exclude as &#xA;(&#xA;&#x9;SELECT a.ReportID &#xA;&#x9;FROM MDCReport a&#xA;&#x9;INNER JOIN MDCReport b &#xA;&#x9;&#x9;ON a.ClaimNumber = b.ClaimNumber &#xA;&#x9;&#x9;AND a.ServiceDate = b.ServiceDate &#xA;&#x9;&#x9;AND a.AmountChargedbyProvider = b.AmountChargedbyProvider &#xA;&#x9;&#x9;AND a.BillIdentificationNumber &lt;&gt; b.BillIdentificationNumber&#xA;&#x9;&#x9;AND a.BillIdentificationNumber IN (SELECT BillIdentificationNumber FROM MDCReport GROUP BY BillIdentificationNumber HAVING SUM(CAST(PaidAmount AS money)) = 0) &#xA;&#xA;&#x9; UNION&#xA;&#xA;&#x9; Select  ReportId &#xA;&#x9; From &#xA;&#x9;&#x9;(&#xA;&#x9;&#x9;&#x9;SELECT a.ReportID, Row_Number() Over (Partition By a.ClaimNumber, a.ServiceDate,a.AmountChargedbyProvider,a.BillIdentificationNumber,a.LineIdentificationNumber Order by CASE WHEN CAST(a.PaidAmount AS money) = 0 THEN 0 ELSE 1 END) RowNumber&#xA;&#x9;&#x9;&#x9;FROM MDCReport a&#xA;&#x9;&#x9;&#x9;INNER JOIN MDCReport b &#xA;&#x9;&#x9;&#x9;&#x9;ON a.ClaimNumber = b.ClaimNumber &#xA;&#x9;&#x9;&#x9;&#x9;AND a.ServiceDate = b.ServiceDate &#xA;&#x9;&#x9;&#x9;&#x9;AND a.AmountChargedbyProvider = b.AmountChargedbyProvider &#xA;&#x9;&#x9;&#x9;&#x9;AND a.BillIdentificationNumber = b.BillIdentificationNumber &#xA;&#x9;&#x9;&#x9;&#x9;And a.LineIdentificationNumber = b.LineIdentificationNumber&#x9;&#x9;&#x9; &#xA;&#x9;&#x9;&#x9;&#x9;and a.ReportId != b.ReportId&#xA;&#x9;&#x9;) T&#xA;&#x9;&#x9;Where RowNumber = 1&#xA;&#xA;&#x9;UNION&#xA;&#xA;&#x9;SELECT a.ReportID&#xA;&#x9;FROM MDCReport a&#xA;&#x9;WHERE BillIdentificationNumber LIKE '%9000000'&#xA;&#x9;&#x9;AND CAST(PaidAmount AS money) = 0&#xA; )&#xA; &#xA; Update r&#xA; Set Include = IIF(e.ReportId is NULL, 'Y', 'N')&#xA; From MDCReport r Left Join CTE_Exclude e on r.ReportId = e.ReportId&#xA; " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
