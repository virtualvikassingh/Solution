              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="SRC - FactClaims"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">WITH CTE_RecoveryCode As
(
	Select ClaimID, Cast(Replace(Right('00' + Cast(Sum(RecoveryCode) as varchar(2)),2),'00','01')as varchar(2)) RecoveryCode
	From (
		Select Distinct ClaimID, 
			Case RecoveryCategory 
				When 'Subrogation' Then 3
				When 'Second Injury Fund' Then 2
			Else 0 End RecoveryCode

		From FactClaimTransaction f  
		Where TransactionType = 'Recovery'
	) T
	 
	Group By ClaimID
),


CTE_BodyPartsCount as 
(
	Select ClaimId, Count(1) CountOfBodyParts
	From LossBodyParts
	Group by ClaimID

),

 
CTE_LossDetails as 
(
Select   
	 
	'5' RecordTypeCodeC1,
	' ' ReserveForFutureUseC2,
	Left(ISNULL(c.ClassCode,'0000'),4) EmployementClassCodeC3,
	Replicate (' ',4) ReserveForFutureUseC4,
	'0001' ClaimCountC5,
	dbo.udf_MakeFixedWidthStringIN(Convert(varchar(6),c.LossDate, 12),'0' ,6) AccidentDateC6,
	dbo.udf_MakeFixedWidthStringIN(c.ClaimNumber,'', 12) ClaimNumberC7,
	Case When c.ClaimStatus = 'Open'  And c.ReopenDate IS NULL  then '0'							 
		 When c.ClaimStatus = 'Closed' And c.CloseDate &lt;= DATEADD(mm, DATEDIFF(mm, 0, dbo.udf_GetDate()), 0) then '1'							 
		 When ep.State = 'MI' And c.ClaimStatus = 'Open'  Then  
															Case When c.ReopenDate &lt;= DATEADD(mm, DATEDIFF(mm, 0, dbo.udf_GetDate()), 0) Then '0'		-- Claim was reopened before cutoff and it remained in open state till cutoff date
																 When c.ReopenDate &gt;  DATEADD(mm, DATEDIFF(mm, 0, dbo.udf_GetDate()), 0) Then '1' END	-- Claim was reopened after cutoff so assuming it was closed before or on the cutoff date
		 When ep.State = 'IN' And c.ClaimStatus = 'Open'  Then
															Case When c.ReopenDate  &lt;= DATEADD(mm, DATEDIFF(mm, 0, dbo.udf_GetDate()), 0) Then '2'		-- Claim was reopened before cutoff and it remained in open state till cutoff date
																 When c.ReopenDate  &gt;  DATEADD(mm, DATEDIFF(mm, 0, dbo.udf_GetDate()), 0) Then '1' END	-- Claim was reopened after cutoff so assuming it was closed before or on the cutoff date
	Else '0' END ClaimStatusCodeC8,
	
	Replicate('0',5) WeeklyWageAmountC9,
	
	Right(Min(
		Case When CostType = 'Medical'  and (Select count(1) from FactClaimTransaction f1 where CostType = 'Indemnity' 
		and f1.ClaimID = f.claimId )=0 Then '606'   
		Else case
			When CostCategory = 'Death benefits' Then '101'
			When CostCategory = 'Permanent total disability' Then '202'
			When CostCategory in ('Temporary total disability', 'Temporary partial disability') Then '405'
			When CostCategory = 'Permanent partial disability' and ep.State = 'MI' Then '405'
			When CostCategory = 'Permanent partial disability' and ep.State = 'IN' Then '309'
			End 
		End
	
		) over (partition By CLaimNumber),2) InjuryCodeC10, 

	ISNULL(CatastropheNumber,'00') CatastropheNumberC11,
	  
	IIF(f.CostType = 'Indemnity' and f.TransactionType = 'Reserve', f.TransactionAmount,0) -
	IIF(f.CostType = 'Indemnity' and f.TransactionType = 'Recovery', f.TransactionAmount ,0) 
	IncurredIndemnityAmountC12,

	IIF(f.CostType = 'Medical' and f.TransactionType = 'Reserve', f.TransactionAmount,0) -
	IIF(f.CostType = 'Medical' and f.TransactionType = 'Recovery', f.TransactionAmount ,0) 
	IncurredMedicalAmountC13,

	Replicate('0',8) RecoveryDateC14,
	Replicate(' ',3) ReserveForFutureUseC15,
	Replicate(' ',9) CaseNoAssignedByStateC16,
	'R' UpdateTypeCodeC17,
	' ' ReservedForFutureUserC18,
		 
	Case When FederalStatus In (
						'State Act',
						'Voluntary Comp',
						'Migrant and Seasonal Agricultural Worker Act',
						'Non-appropriated Fund Instrumentality''s Act',
						'Fed Empl Liab Act',
						'Exposure Rated Stop Gap',
						'Outer Continental Shelf Act',
						'Defense Base Act',
						'Maritime Coverage',
						'Limited Maritime')
		Then '01'
			 
		When FederalStatus = 'U.S.L. &amp; H.' Then '02' 

		When FederalStatus = 'Fed Coal Mine Act' AND ep.State != 'MI' Then '03'
	Else '01' END as LossCoveragActCodeC19,

	Case c.TypeOfLoss
		when 'Traumatic Injury' Then '01'
		When 'Occupational Disease' Then '02'
		When 'Cumulative Injury (other than disease)' Then '03'			
	Else '00' End TypeOfLossCodeC20,

	ISNULL(rc.RecoveryCode,'01') TypeOfRecoveryCodeC21 ,
		
	IIF(c.IsEmployerLiabilityClaim = 'Y', '02', '01') TypeOfClaimCodeC22, 
		 

	Case When f.SettlementCode  = 'Aggrement Stipulated' Then '03'
		When  f.SettlementCode   = 'Award' Then '04'
		When  c.CompensabilityDecision  = 'Denied' Then '05'
	  end TypeOfSettlementCodeC23,

	Replicate('0', 7) TotalIncurredVocationalAmountC24,

	c.JurisdictionState,

	'00' ManagedCareOrgTypeCodeC26,	 

	upper(ISNULL(IIF(bpc.CountOfBodyParts &gt; 1, 'Multiple Body Parts', l.DetailBodyPart ),'00xyz00')) PartOfBody,
		 
	upper(ISNULL(l.NatureOfInjury,'00xyz00')) TypeOfInjury,
		 
	upper(ISNULL(AccidentType,'00xyz00')) CauseOfInjury, 
		 		 
	upper(Left(ISNULL(cl.Occupation,' ') + Replicate(' ',18), 18)) OccupationalDescriptionC30,  

	IIF(CostCategory = 'Vocational' and f.TransactionType = 'Payment', 'Y','N') VocationalRehabilitationIndicatorC31,
		
	IIF(LineCategory Like '%Lump Sum%', 'Y','N') LumpSumIndicatorC32,

	IIF(ClosedOutcome = 'Fraud','02','00') FraudulentClaimCodeC33, 

	Replicate(' ',2) ReservedForFutureUseC34,

	IIF(f.CostType = 'Indemnity' and f.TransactionType = 'Payment', f.TransactionAmount ,0) -
	IIF(f.CostType = 'Indemnity' and f.TransactionType = 'Recovery', f.TransactionAmount ,0) 
	PaidIndemnityAmountC35,  

	IIF(f.CostType = 'Medical' and f.TransactionType = 'Payment', f.TransactionAmount ,0) -
	IIF(f.CostType = 'Medical' and f.TransactionType = 'Recovery', f.TransactionAmount ,0) 
	PaidMedicalAmountC36,  

	IIF(f.CostType = 'Expense' and f.TransactionType = 'Payment' and LineCategory = 'Claimant Attorney Fees' , f.TransactionAmount ,0) ClaimantAttorneyFeesIncurredAmountC37,  
	IIF(f.CostType = 'Expense' and f.TransactionType = 'Payment' and LineCategory in ('External Employer Attorney Fees','Internal Employer Attorney Fees'), f.TransactionAmount ,0) EmployerAttorneyFeesIncurredAmountC38, 
	IIF(RecoveryCategory = 'Deductible Recovery' and f.TransactionType = 'Recovery', f.TransactionAmount ,0) DeductibleReimbursementAmountC39,  

	Replicate('0',9) TotalGrossIncurredAmountC40,

	Replicate(' ',2) ReservedForFutureUseC41,

	IIF(f.CostType = 'Expense' and f.TransactionType = 'Payment', f.TransactionAmount ,0) -
	IIF(f.CostType = 'Expense' and f.TransactionType = 'Recovery', f.TransactionAmount ,0) 
	PaidALAEAmountC42,  

	IIF(f.CostType = 'Expense' and f.TransactionType = 'Reserve', f.TransactionAmount ,0) +
	IIF(f.CostType = 'Expense' and f.TransactionType = 'Payment', f.TransactionAmount ,0) -
	IIF(f.CostType = 'Expense' and f.TransactionType = 'Recovery', f.TransactionAmount ,0) 
	IncurredALAEAmountC43,  

	'000' ScheduledIndemnityC44,

	IIF(f.TransactionType = 'Recovery' and RecoveryCategory = 'Subrogation', f.TransactionAmount ,0)SubrogationRecovery,  
	IIF(c.CompensabilityDecision  = 'Denied', 1, 0)   CompensabilityDenied,
		 
	p.PolicyID,
	p.PolicyNumber, 
	p.TermNumber,
	ep.ReportMonth,
	ep.ReportYear,
	cast(ep.State as varchar(2)) ExposureState

From FactClaimTransaction f 
	Inner Join DimPolicy p on f.PolicyID = p.PolicyID
	inner Join DimClaim c on f.ClaimId = c.ClaimId
	inner join DimClaimant cl on f.ClaimantID = cl.ClaimantID
	Inner Join DimExposure e on f.ExposureID = e.ExposureID
	Left  Join LossBodyParts l on c.ClaimID = l.ClaimID
	inner join VW_WCStatEffectivePolicies ep on ep.PolicyNumber = p.PolicyNumber and ep.TermNumber = p.TermNumber 
	Left  Join CTE_RecoveryCode rc on f.ClaimID = rc.ClaimID	
	Left  Join CTE_BodyPartsCount bpc on c.ClaimID = bpc.ClaimID
	 
	
	
Where 1 = 1 
	And p.EffectiveDate &lt; DATEADD(mm, DATEDIFF(mm, 0, dbo.udf_GetDate()), 0)
	and p.JobCloseDate &lt;  DATEADD(mm, DATEDIFF(mm, 0, dbo.udf_GetDate()), 0)
	And p.PolicyStatus in ('Bound','Completed')
	and c.ClaimCreateDate &lt; DATEADD(mm, DATEDIFF(mm, 0, dbo.udf_GetDate()), 0)
	AND ISNULL(c.ClosedOutcome, 'NULL') NOT IN ('Duplicate','Mistake')
	and f.TransactionCreateDate &lt; DATEADD(mm, DATEDIFF(mm, 0, dbo.udf_GetDate()), 0)

 )
 
 Select  
	RecordTypeCodeC1,
	ReserveForFutureUseC2, 
	EmployementClassCodeC3, 
	ReserveForFutureUseC4, 
	ClaimCountC5,
	AccidentDateC6, 
	ClaimNumberC7, 
	ClaimStatusCodeC8, 
	WeeklyWageAmountC9,  
	InjuryCodeC10,  
	CatastropheNumberC11, 
	Cast(Format(ABS(Cast(Round(ISNULL(Sum(IncurredIndemnityAmountC12),0),2) as INT)),'000000000')as varchar(9))IncurredIndemnityAmountC12, 
	Cast(Format(ABS(Cast(Round(ISNULL(Sum(IncurredMedicalAmountC13),0),2) as INT)),'000000000')as varchar(9)) IncurredMedicalAmountC13,
	RecoveryDateC14,
	ReserveForFutureUseC15,
	CaseNoAssignedByStateC16, 
	UpdateTypeCodeC17 , 
	ReservedForFutureUserC18,
	LossCoveragActCodeC19,
	TypeOfLossCodeC20,
	TypeOfRecoveryCodeC21, 
	TypeOfClaimCodeC22, 
	ISNULL(Min(TypeOfSettlementCodeC23),'00') TypeOfSettlementCodeC23, 
	TotalIncurredVocationalAmountC24, 
	JurisdictionState,
	ManagedCareOrgTypeCodeC26, 
	PartOfBody,
	TypeOfInjury,
	CauseOfInjury,
	OccupationalDescriptionC30, 
	VocationalRehabilitationIndicatorC31, 
	LumpSumIndicatorC32, 
	FraudulentClaimCodeC33, 
	ReservedForFutureUseC34, 
	Cast(Format(ABS(Cast(Round(ISNULL(Sum(PaidIndemnityAmountC35),0),2) as INT)),'000000000')as varchar(9))  PaidIndemnityAmountC35,
	Cast(Format(ABS(Cast(Round(ISNULL(Sum(PaidMedicalAmountC36) , 0),2) as INT)),'000000000')as varchar(9)) PaidMedicalAmountC36, 
	Cast(Format(ABS(Cast(Round(ISNULL(Sum(ClaimantAttorneyFeesIncurredAmountC37), 0),2) as INT)), '000000000')as varchar(9))  ClaimantAttorneyFeesIncurredAmountC37, 
	Cast(Format(ABS(Cast(Round(ISNULL(Sum(EmployerAttorneyFeesIncurredAmountC38) ,0),2) as INT)),'000000000')as varchar(9)) EmployerAttorneyFeesIncurredAmountC38, 
	Cast(Format(ABS(Cast(Round(ISNULL( Sum(DeductibleReimbursementAmountC39) ,0),2) as INT)),    '000000000')as varchar(9))DeductibleReimbursementAmountC39, 
	TotalGrossIncurredAmountC40,
	ReservedForFutureUseC41, 
	Cast(Format(ABS(Cast(Round(ISNULL(SUM(PaidALAEAmountC42),0),2) as INT)),    '000000000') as varchar(9))PaidALAEAmountC42, 
	Cast(Format(ABS(Cast(Round(ISNULL(Sum(IncurredALAEAmountC43),0),2) as INT)),'000000000') as varchar(9))  IncurredALAEAmountC43, 
	ScheduledIndemnityC44,
	min(CompensabilityDenied) CompensabilityDenied,
	PolicyID,
	PolicyNumber, 
	TermNumber,
	ReportMonth,
	ReportYear,
	ExposureState
 
 From CTE_LossDetails 

 Group By 
	RecordTypeCodeC1,
	ReserveForFutureUseC2, 
	EmployementClassCodeC3, 
	ReserveForFutureUseC4, 
	ClaimCountC5,
	AccidentDateC6, 
	ClaimNumberC7, 
	ClaimStatusCodeC8, 
	WeeklyWageAmountC9,  
	InjuryCodeC10,  
	CatastropheNumberC11,  
	RecoveryDateC14,
	ReserveForFutureUseC15,
	CaseNoAssignedByStateC16, 
	UpdateTypeCodeC17 , 
	ReservedForFutureUserC18,
	LossCoveragActCodeC19,
	TypeOfLossCodeC20,
	TypeOfRecoveryCodeC21, 
	TypeOfClaimCodeC22,  
	TotalIncurredVocationalAmountC24, 
	JurisdictionState,
	ManagedCareOrgTypeCodeC26, 
	PartOfBody,
	TypeOfInjury,
	CauseOfInjury,
	OccupationalDescriptionC30, 
	VocationalRehabilitationIndicatorC31, 
	LumpSumIndicatorC32, 
	FraudulentClaimCodeC33, 
	ReservedForFutureUseC34,  
	TotalGrossIncurredAmountC40,
	ReservedForFutureUseC41,  
	ScheduledIndemnityC44,
	PolicyID,
	PolicyNumber, 
	TermNumber,
	ReportMonth,
	ReportYear,
	ExposureState

 	Having (SUM(IncurredMedicalAmountC13) !=0 OR SUM(IncurredIndemnityAmountC12) != 0 OR  SUM(ClaimantAttorneyFeesIncurredAmountC37) != 0 OR SUM(EmployerAttorneyFeesIncurredAmountC38) != 0 OR SUM(IncurredALAEAmountC43) != 0 
	 OR SUM(SubrogationRecovery) !=0 OR SUM(DeductibleReimbursementAmountC39) !=0 ) or Min(CompensabilityDenied) != 0</property>
