                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">With CTE_MostRecentVersion as 
(
	Select Policyid 
	from
	(
		Select 	 
		 p.PolicyID,	 
		 Row_number() over(partition by p.PolicyNumber, p.TermNumber order by p.JobCloseDate Desc) RowNumber 
	 
		From FactWCPremiumTransaction f 
		inner join DimPolicy p  on f.PolicyID = p.PolicyID
		
		Where p.EffectiveDate &lt; DATEADD(mm, DATEDIFF(mm, 0, dbo.udf_GetDate()), 0)
		and p.JobCloseDate &lt;  DATEADD(mm, DATEDIFF(mm, 0, dbo.udf_GetDate()), 0)
  		and p.TransactionType = 'Audit'
		and p.PolicyStatus = 'Completed'
		) t
	Where t.RowNumber = 1  
  
 ),

 CTE_Audit as 
 (
	
	Select  PolicyId, PolicyNumber, TermNumber, ClassCode, EffectiveDateID,  
	Sum(PremiumAmount) PremiumAmount,
	Sum(exposureAmount) ExposureAmount,
	ManualChargedRate,
	Row_number() over(partition by PolicyNumber, TermNumber, ClassCode, EffectiveDateID order by JobCloseDate Desc) RowNumber
	From 
		(
		Select  
			p.PolicyId, 
			p.PolicyNumber, 
			p.TermNumber, 
			c.ClassCode, 
			p.JobCloseDate,
			f.EffectiveDateID,
			ABS(c.ActualAmount) PremiumAmount,  
			ABS(IIF(IsClassCode = 1, IIF(c.ClassCode = '0930', 0, IIF(c.BasisType='Per Capita', c.Basis*100, c.Basis)) ,0)) ExposureAmount,
			ABS(IIF(IsClassCode = 1, ISNULL(IIF(c.ClassCode = '0930', 0, c.ActualAdjRate),0) ,0))*1000 ManualChargedRate 

		from dimpolicy p  
			inner join FactWCPremiumTransaction f on f.PolicyID = p.PolicyID
			inner join DimWCCoverage c on c.CoverageID = f.CoverageID 	 
			 
		where  Charges = 1 	 
			and p.TransactionType = 'Audit'
			and p.PolicyStatus = 'Completed'
			and p.EffectiveDate &lt; DATEADD(mm, DATEDIFF(mm, 0, dbo.udf_GetDate()), 0)
			and p.JobCloseDate &lt;  DATEADD(mm, DATEDIFF(mm, 0, dbo.udf_GetDate()), 0)
		 
	 )T
	 Group by PolicyId, PolicyNUmber, TermNumber, ClassCode, ManualChargedRate, JobCloseDate, EffectiveDateID

 ),

 CTE_AnniversaryDate as 
 (
	 Select   p.PolicyNumber, p.TermNumber, Convert(varchar(8), c.AnniversaryDate, 112) AnniversaryDate,  
	 Count(Distinct ExperienceRating)  DistinctExpMod
	 from FactWCPremiumTransaction f 
		inner join DimWCCoverage c on f.CoverageID = c.CoverageID
		inner Join DimPolicy p on f.PolicyId = p.PolicyId
		
		Where c.AnniversaryDate IS NOT NULL
		 
	Group By p.PolicyNUmber, p.TermNumber, c.AnniversaryDate, p.effectivedate
),

 
 CTE_ExpMod as 
(
	Select Distinct p.PolicyId,f.EffectiveDateID, ExperienceRating
	From DimWCCoverage c 
		Inner Join FactWCPremiumTransaction f on f.CoverageID = c.CoverageID
		Inner Join DimPolicy p on f.PolicyID = p.PolicyID
		
	Where ClassCode in ('9887', '9898', '9889','0000') 
		And c.EffectiveDate &lt;&gt; c.ExpirationDate  
)


,CTE_SplitPeriod as 
(
	Select  PolicyNumber, TermNumber, classcode, EffectiveDateId,
	Dense_Rank() Over(Partition by PolicyNumber, TermNumber  Order by EffectiveDateId) -1 RNK
	from (
		Select Distinct   p.PolicyNumber, p.TermNumber, EffectiveDateId, c.ClassCode
		From DimWCCoverage c 
			Inner Join FactWCPremiumTransaction f on c.CoverageId = f.CoverageID
			Inner Join DimPolicy p on f.PolicyId = p.PolicyID
			Inner join CTE_MostRecentVersion l on p.PolicyID = l.Policyid
		Where 1 =1 		 
		--and (ActualAmount != 0 or Basis !=0)  	
 
	)p
 
),


 CTE_SplitCount as 
 (
		Select Distinct p.PolicyNumber, p.TermNumber,  Count(Distinct SplitDate) SplitCount
		From DimWCCoverage c 
		Inner Join FactWCPremiumTransaction f on c.CoverageId = f.CoverageID
		Inner Join DimPolicy p on f.PolicyId = p.PolicyID
		--Inner Join VW_WCStatEffectivePolicies ep on p.PolicyNumber = ep.PolicyNumber and p.TermNumber = ep.TermNumber
		Group By p.PolicyNumber, p.TermNumber
	),

 
CTE_ExpModEffDate
as 
(
	Select 
	 ClassCode, ExperienceRating, T.PolicyNUmber, T.TermNumber, CoverageEffectiveDate, /*  Policyeffectivedate, a.AnniversaryDate,*/

	Case When SplitCount = 0 Then Right(PolicyEffectiveDate,6)
		 When T.PolicyEffectiveDate != a.AnniversaryDate and DistinctExpMod &lt;  2 Then Right(PolicyEffectiveDate,6)
		 When T.PolicyEffectiveDate != a.AnniversaryDate and DistinctExpMod &gt;= 2 Then Right(CoverageEffectiveDate,6)
		 When T.PolicyEffectiveDate = a.AnniversaryDate Then Right(CoverageEffectiveDate,6)
		 Else Right(PolicyEffectiveDate,6)
	END as ExperienceModificationEffectiveDate,

	Case  When SplitCount = 0 Then Right(PolicyEffectiveDate,6)
		 When T.PolicyEffectiveDate != a.AnniversaryDate  Then Right(CoverageEffectiveDate,6)
	  	 Else Right(PolicyEffectiveDate,6)
	END as RateEffectiveDate  
 
	From 
	(
		Select   
				p.PolicyId, p.PolicyNUmber, p.TermNumber,
				c.CoverageId, 
				c.IsClassCode,
				c.GoverningLaw,
				c.CoalMine,
				c.coverageName,
				ep.State,
				c.ActualAdjRate,
				c.BasisType,
				c.Basis,
				ClassCode,
				ExperienceRating,
				f.EffectiveDateID CoverageEffectiveDate,
				Convert(varchar(8), c.AnniversaryDate, 112) AnniversaryDate,
				Convert(varchar(8), c.SplitDate, 112) SplitDate,
				Convert(varchar(8), p.EffectiveDate, 112) PolicyEffectiveDate
		From DimWCCoverage c 
			Inner Join FactWCPremiumTransaction f on c.CoverageId = f.CoverageID
			Inner Join DimPolicy p on f.PolicyId = p.PolicyID
			Inner Join VW_WCStatEffectivePolicies ep on p.PolicyNumber = ep.PolicyNumber and p.TermNumber = ep.TermNumber
	)T Left join CTE_AnniversaryDate a on T.PolicyNumber = a.PolicyNumber and T.TermNumber = a.TermNumber
	  Left Join CTE_SplitCount s on t.PolicyNumber = s.PolicyNumber and t.TermNumber = s.TermNumber
),

CTE_Summary as (
  
  Select 
  '4' as RecoordTypeCodeC1,
  ' ' ReservedForFutureUseC2,
  Left(ISNULL(wc.ClassCode,' 0000'),4) ClassCodeC3, 
  Replicate(' ',4) ReservedForFutureUseC4, 
  IIF(wc.IsClassCode = 1 or wc.ClassCode in ('9848', '9812', '0930','0121', '0122','9807','9664'),Cast(Format(IIF(isnull(em.ExperienceRating,0) = 0, 0, em.ExperienceRating)*1000,'0000')as varchar(4)),'0000')  ExperienceModFactorC5,  
  dbo.udf_MakeFixedWidthStringIN(ExperienceModificationEffectiveDate,'0',6) ExperienceModificationEffectiveDateC6, /* CoverageEffectiveDate, Policyeffectivedate, emd.anniversarydate,*/
   
  dbo.udf_MakeFixedWidthStringIN(RateEffectiveDate,'0',6) RateEffectiveDateC7,
  Cast(Format(Cast(Round(ISNULL(ExposureAmount,0),0) as INT),'0000000000')as varchar(10)) ExposureAmountC8,
  Cast(Format(Cast(Round(ISNULL(PremiumAmount,0),0) as INT),'000000000')as varchar(9))    PremiumAmountC9,
  Cast(Format(ISNULL(ManualChargedRate,0),'0000000')as varchar(7))   ManualChargedRateC10, 

  IIF(SplitCount = 0, '0', ISNULL(s.RNK,0)) SplitPeriodCodeC11,  
 

  Replicate(' ',17) ReservedForFutureUseC12, 
  '00' RatingTierIdCodeC13,
  Replicate(' ',8) ReservedForFutureUseC14, 
  'R' UpdateTypeCodeC15,
  ' ' ReservedForFutureUseC16, 
  ISNULL(Case wc.IsClassCode when 1  then 
     case when wc.GoverningLaw ='Longshore And Harbor Workers C' then '02'
          when ep.State = 'IN' and isnull(cast(wc.CoalMine as varchar(1)),'0')!= '0' then '03'
	      when ep.State = 'IN' and isnull(wc.CoverageName,'')  = 'WC7MaritimeCov' then '09'
	      when ep.State = 'MI' then '01'
	 else '01' end
  else '00' end, '00')  as ExposureActCodeC17, 
  Replicate(' ',126) ReservedForFutureUseC18, 
  p.Policyid,  
  p.PolicyNumber, 
  p.TermNumber, 
  ep.State ExposureState,
  ep.ReportMonth,
  ep.ReportYear,  
  Row_number() over(partition by p.PolicyNumber, p.TermNumber, wc.ClassCode, f.EffectiveDateID  order by p.JobCloseDate Desc) RowNumber 

from dimpolicy p  
	inner join VW_WCStatEffectivePolicies ep on p.PolicyNumber = ep.PolicyNumber and p.TermNumber = ep.TermNumber	
	left join FactWCPremiumTransaction f on f.PolicyID = p.PolicyID
	left join DimWCCoverage wc on wc.CoverageID = f.CoverageID 	
	left Join CTE_Audit a on a.PolicyNumber = p.PolicyNumber and a.TermNumber = p.TermNumber and wc.ClassCode = a.ClassCode and f.EffectiveDateID = a.EffectiveDateID  and a.RowNumber = 1  
	Left Join CTE_ExpMod em on p.PolicyID = em.PolicyID and f.EffectiveDateID = em.EffectiveDateID
	Left Join CTE_ExpModEffDate emd on p.PolicyNumber  = emd.PolicyNumber and p.TermNumber = emd.TermNumber and  f.EffectiveDateID = emd.CoverageEffectiveDate and wc.ClassCode = emd.ClassCode 
	Left join CTE_SplitPeriod s on p.PolicyNumber = s.PolicyNumber and p.TermNumber = s.TermNumber and  wc.ClassCode = s.ClassCode and f.EffectiveDateID = s.EffectiveDateID
	Left Join CTE_SplitCount c on p.PolicyNumber = c.PolicyNumber and p.TermNumber = c.TermNumber
	inner join CTE_MostRecentVersion m on p.PolicyID = m.PolicyID
 
 
where 1= 1 
	And NOT (wc.EffectiveDate = wc.ExpirationDate and f.WrittenPremium = 0 )
	and wc.ClassCode not in ('0000', '9898')
	and (ExposureAmount != 0 or PremiumAmount !=0) 
 


)

select *
 from CTE_Summary
 where 1= 1 
 and rownumber =1</property>
