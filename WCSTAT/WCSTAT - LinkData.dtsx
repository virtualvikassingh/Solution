              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">With CTE_LinkData as 
(
	Select 	 
	 '19968' CarrierCode,
	  dbo.udf_MakeFixedWidthStringIN(p.PolicyNumber,'',18) PolicyNumberIdentifier,
	 Replicate(' ',3) SeperatedSegmentNumber,
	 Replicate(' ',4) ReservedForFutureUse,
	 Case e.State 
		When 'MI' Then '21'
		When 'IN' Then '13' 
	 END ExposureStateCode,
	 Convert(varchar(6),p.effectiveDate,12) PolicyEffectiveDate,
	 
	 '0' CorrectionSeqNumber,
	 p.PolicyID,
	 p.TermNumber,
	 e.ReportMonth,
	 e.ReportYear,
	 Row_number() over(partition by p.PolicyNumber, p.TermNumber order by p.JobCloseDate Desc) RowNumber 
	 
 From FactWCPremiumTransaction f 
	inner join DimPolicy p  on f.PolicyID = p.PolicyID
	inner join VW_WCStatEffectivePolicies e on e.PolicyNumber = p.PolicyNumber and e.TermNumber = p.TermNumber
 
Where p.EffectiveDate &lt; DATEADD(mm, DATEDIFF(mm, 0, dbo.udf_GetDate()), 0)
	and p.JobCloseDate &lt;  DATEADD(mm, DATEDIFF(mm, 0, dbo.udf_GetDate()), 0)
  	and p.TransactionType = 'Audit'
	and p.PolicyStatus  = 'Completed'
 )


 Select * from CTE_LinkData
 Where RowNumber = 1</property>
