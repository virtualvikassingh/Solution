              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Select ExposureStateCode, DateAdded,   99 SortOrder, '99' SortOrder1, '99' SortOrder2,
	(
		Replicate('9',40) 
		+ '9'	--RecordTypeCode
		+ Cast(Format(Sum(DetailRecordCount),'00000000')   as varchar(8))	--DetailRecordCount
		+ Cast(Format(Sum(UnitRecordCount),'0000000')   as varchar(7))		--UnitRecordCount
		+ Cast(ReportYear as varchar(4))					--PrimaryEffectiveYear
		+ Right('00' + Cast(ReportMonth as varchar(2)),2)	--PrimaryEffectiveMonth
		+ Cast(Format(Sum(ICRTotal),'00000000')   as varchar(8))  --ICRTotal
		+ Replicate(' ',180) --ReservedForFutureuse
	) ReportData
	
from 
	(
		Select  
		IIF(ExposureState = 'MI','21','13') ExposureStateCode, 
		1 DetailRecordCount, 
		IIF(Column1 = 1, 1,0) UnitRecordCount, 
		0 as ICRTotal,
		Cast(l.DateAdded as Date) DateAdded,
		l.ReportingMonth,
		l.ReportingYear,
		Month(DATEADD(Month,  -18, dbo.udf_GetDate())) ReportMonth,
		Year( DATEADD(Month,  -18, dbo.udf_GetDate())) ReportYear

	 From WCStatLinkData l
		inner join WCStatReportData r on l.PolicyID = r.PolicyID and l.ReportingMonth = r.ReportingMonth and l.ReportingYear = r.ReportingYear  and l.RecordType = IIF(r.RecordType = 'CorrectionHeader', 'Correction', r.RecordType)
	 Where Column1 NOT IN ( 5,6)
		and r.ReportingMonth = Month(dbo.udf_GetDate()) 
		and r.Reportingyear = Year(dbo.udf_GetDate())
	 

	 UNION All

	 Select 
		IIF(ExposureState = 'MI','21','13') ExposureStateCode, 
		1  DetailRecordCount, 
		IIF(Column1 = 1, 1,0)  UnitRecordCount, 
		0 as ICRTotal,
		Cast(l.DateAdded as Date) DateAdded,
		l.ReportingMonth,
		l.ReportingYear,
		Month(DATEADD(Month,  -18 , dbo.udf_GetDate())) ReportMonth,
		Year( DATEADD(Month,  -18 , dbo.udf_GetDate())) ReportYear

	 From WCStatLinkData l
		inner join WCStatReportData r on l.PolicyNumberIdentifier = r.PolicyNumber and l.TermNumber = r.TermNumber and l.ReportingMonth = r.ReportingMonth and l.ReportingYear = r.ReportingYear and l.RecordType = IIF(r.RecordType = 'CorrectionHeader', 'Correction', r.RecordType)
	 Where Column1 In( 5,6)
		and r.ReportingMonth = Month(dbo.udf_GetDate()) 
		and r.Reportingyear = Year(dbo.udf_GetDate())
	 )T

 

Group By ExposureStateCode, DateAdded, ReportingYear, ReportingMonth, ReportYear, ReportMonth</property>
